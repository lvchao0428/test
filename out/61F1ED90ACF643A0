<script type="text/javascript">                 	
var postAct = document.getElementById("post_act");
  var postActTop = getOffsetTop(postAct);
  var postActHeight = postAct.offsetHeight;
 // postActTop = 400;
  //alert(postActTop);
  document.onscroll = function() {
      var docTop = document.body.scrollTop | document.documentElement.scrollTop;
      console.log(postActTop +" ` " +docTop)
      if (postActTop  < (docTop + 10)) {
          postAct.setAttribute("style", "position:fixed;width:265px; top: 10px; z-index:300; box-shadow: 0 0 5px 2px rgba(0,0,0,0.3)");
      } else {
          postAct.setAttribute("style", "");
      }
  }
  
  function getOffsetTop(elm) {    
    var mOffsetTop = elm.offsetTop;    
    var mOffsetParent = elm.offsetParent;    
    while(mOffsetParent) {    
        mOffsetTop += mOffsetParent.offsetTop;    
        mOffsetParent = mOffsetParent.offsetParent;    
    }    
    return mOffsetTop;    
}


