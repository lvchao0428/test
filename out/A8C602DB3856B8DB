		function getBrz(obj, type){
			if (!obj.data("brzdata")) {
			    jQuery.getJSON("http://webbiz.xici.net/biz.asp?from=doc&sid=BEA5B644E6D2D548&tags=职业培训 公考 行测 行政能力测验 申论 2013省考 云南省公务员 云南公务员 公务员真题 公务员考试 @628370@&clubid=2672&jsoncallback=?", function (data) {
			        obj.html(data.Addocsidebar).data("brzdata", data).show();
