        $('.do').click(function () {
            var t = $('#ft,#forum,.ft').offset().top;
            $('html,body').animate({scrollTop:t}, 500);
        });
        $('.go').click(function(){
            $('html,body').animate({scrollTop: 0},500);
        });
        $(window).bind('scroll resize', function () {
            var w = $(window);
            if (w.scrollTop() >= w.height()) {
                $('a.go').css('display', 'block');
                $('a.do').hide();
            }
            else {
                $('a.do').css('display', 'block');
                $('a.go').hide();
            }
