<script>
function md_show(thisid,desc,name){
 	jQuery("#md_show_menu").removeClass("hidecss");
id = jQuery(thisid).attr("id");
src = jQuery('#'+id+'_src').attr('src');
src += '.png';
jQuery('#show_medal_name').html(name);
jQuery('#show_medal_des').html(desc);
jQuery("#show_medal_src").attr("src",src);
jQuery("#md_show_menu").addClass("showcss");
var xico=jQuery(thisid).offset().left;  
var yico=jQuery(thisid).offset().top-120;
jQuery("#md_show_menu").css({left:xico,top:yico});
jQuery(thisid).mouseout(function(){
jQuery("#md_show_menu").removeClass("showcss");
jQuery("#md_show_menu").addClass("hidecss");
});
jQuery("#md_show_menu").mouseleave(function(){jQuery("#md_show_menu").removeClass("showcss");jQuery("#md_show_menu").addClass("hidecss");});
jQuery("#md_show_menu").mouseover(function(){
jQuery("#md_show_menu").removeClass("hidecss");
  	jQuery("#md_show_menu").addClass("showcss");
 });
}

function mdclick(thisid){
  jQuery("#md_show_menu").css("display","block");
  var xico=jQuery(thisid).offset().left;  
  var yico=jQuery(thisid).offset().top-120;
  jQuery("#md_show_menu").css({left:xico,top:yico});
}
document.body.onmousedown=function(){
 jQuery("#md_show_menu").css("display","");

 
 }	
</script>
