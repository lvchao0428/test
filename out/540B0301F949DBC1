function succeedhandle_followmod(url, msg, values) {
var fObj = $('followmod_'+values['fuid']);
if(values['type'] == 'add') {
fObj.innerHTML = '不收听';
fObj.href = 'home.php?mod=spacecp&ac=follow&op=del&fuid='+values['fuid'];
} else if(values['type'] == 'del') {
fObj.innerHTML = '收听TA';
fObj.href = 'home.php?mod=spacecp&ac=follow&op=add&hash=693857e5&fuid='+values['fuid'];
}
}
</script>

<script>

function b(){
h = jQuery(window).height();
t = jQuery(document).scrollTop();
if(t > h){
jQuery('#gotop').css("display","block");
jQuery('#bottom_u').css("display","none");
}else{
jQuery('#gotop').css("display","none");
jQuery('#bottom_u').css("display","block");
}
}

jQuery(document).ready(function(e) {
b();
jQuery('#gotop').click(function(){
jQuery(document).scrollTop(0);	
})
jQuery('#code').hover(function(){
jQuery(this).attr('id','code_hover');
jQuery('#code_img').css("display","block");
},function(){
jQuery(this).attr('id','code');
jQuery('#code_img').css("display","none");
})

});


jQuery(window).scroll(function(e){
b();		
})
 
//按钮滚动事件
jQuery("#bottom_u").click(function(){jQuery(document).scrollTop(jQuery("#botton_u").offset().top) });
