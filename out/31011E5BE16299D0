<ignore_js_op>

<img id="aimg_7482354" aid="7482354" src="static/image/common/none.gif" zoomfile="http://f3.rednet.cn/data/attachment/forum/201510/09/220455jh0v8h3b2b5183db.jpg" file="http://f3.rednet.cn/data/attachment/forum/201510/09/220455jh0v8h3b2b5183db.jpg" class="zoom" onclick="zoom(this, this.src, 0, 0, 1)" width="800" id="aimg_7482354" inpost="1" onmouseover="showMenu({'ctrlid':this.id,'pos':'12'})" />

<div class="tip tip_4 aimg_tip" id="aimg_7482354_menu" style="position: absolute; display: none" disautofocus="true">
