	function () { 
		$(this).parents('.posted-box-add').hide(); 
		$(this).parents('.posted-box-add').prev('.posted-box-add').fadeIn(); 
	}
);
$(".shieldshow").click( 
	function () { 
		$(this).parents('.reply-box').hide(); 
		$(this).parents('.reply-box').prev('.reply-box').fadeIn(); 
	}
);
