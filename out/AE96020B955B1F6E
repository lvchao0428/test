	jQuery("#tag_li_zt li").mouseover(function () {
		var n = jQuery(this).index();
		jQuery(".zt_numlist").hide().eq(n).show();
		jQuery("#tag_li_zt li").removeClass("focus");
		jQuery(this).addClass("focus");
	});
	</script>
	
