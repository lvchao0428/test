//sunny 打赏 B
function rewardBtn(aid, lay){
	var valid_integral = $("#valid_integral").attr("value");

	document.getElementById("followup").value = aid;
	document.getElementById("lay").value = lay;
	document.getElementById("aid").value = aid;

	$('.reward-fixed-box').fadeIn(300);
	$('.reward-fixed-main').css({'margin-left':'-'+($('.reward-fixed-main').outerWidth()/2)+'px','margin-top':'-'+($('.reward-fixed-main').height()/2)+'px'});

	if (valid_integral == 0 || valid_integral < 10) {
		document.getElementById("butsubmit").attributes["onclick"].value = '';
		document.getElementById("butsubmit").attributes["class"].value = "reward-btn-big reward-btn-big2 forbidden-btn";
		$("#notenough_memo").html('您的积分不够，<a href="http://user.kdnet.net/index.asp#chongzhi" target="_blank" class="c-2477b3">充值</a> 获得。');
	}
}

//赏了
function butsubmit(){
	//document.getElementById("butsubmit").attributes["onclick"].value = '';
	document.getElementById("butsubmit").attributes["class"].value = "reward-btn-big reward-btn-big2 forbidden-btn";

	//判断
	var errmsg = "";

	////打赏积分
	var valid_integral = $("#valid_integral").attr("value");
	
	var this_integral = '';
	var arr_radio = $("input[name='RadioGroup1']");
	for (var i = 0 ; i < arr_radio.length; i++){
		if (arr_radio[i].checked) {
			this_integral = arr_radio[i].value;
		}
	}
	if (this_integral == '') this_integral = $("#input_integral").val();

	if (this_integral != '') {
		if (parseInt(valid_integral) < parseInt(this_integral)) {
			errmsg = "您的积分不够，请先充值再打赏。";
		}
	} else {
		errmsg = "请选择或填写打赏的积分数。";
	}
	
	if (errmsg == "") {
		////是否跟帖回复
		var reply_words = $("#reply-words").val();
		var rpy_check = $("input[name='rpy_checkbox']");
		if (rpy_check[0].checked &&　reply_words == '') {
			errmsg = "您选择跟帖回复，请说点什么吧。";
		}
	}

	if (errmsg != "") {
		document.getElementById("butsubmit").attributes["class"].value = "reward-btn-big reward-btn-big2";
		alert(errmsg);
		return;
	} else {
		document.getElementById("butsubmit").attributes["onclick"].value = '';
		if (rpy_check[0].checked || (rpy_check[0].checked == false && reply_words != '')) {	//跟帖回复
			document.t_form.attributes["action"].value = 'http://upfile1.kdnet.net/giving_do.asp?action=sre&method=fastreply&BoardID=1&intetral='+this_integral+'&valid_integral='+valid_integral; 
			//alert(document.t_form.attributes["action"].value);
			document.t_form.submit(); 
		} else {	//不跟贴回复，直接打赏
			document.t_form.attributes["action"].value = 'http://club.kdnet.net/giving.asp?rootid=11321541&posttable=8&intetral='+this_integral+'&valid_integral='+valid_integral; 
			//alert(document.t_form.attributes["action"].value);
			document.t_form.submit(); 
		}
	}
}

//清除积分弹窗
function closeR(){
	document.getElementById("input_integral").value = "";
	document.getElementById("reply-words").value = "";
	document.getElementById("butsubmit").attributes["onclick"].value = 'butsubmit();';
	document.getElementById("butsubmit").attributes["class"].value = "reward-btn-big reward-btn-big2";
	$("#notenough_memo").html('');
	
	$('.reward-fixed').fadeOut(300);
};

//判断积分是否够
function chk_integral(){
	document.getElementById("butsubmit").attributes["onclick"].value = '';
	document.getElementById("butsubmit").attributes["class"].value = "reward-btn-big reward-btn-big2 forbidden-btn";
	var valid_integral = $("#valid_integral").attr("value");
	
	var this_integral = '';
	var arr_radio = $("input[name='RadioGroup1']");
	for (var i = 0 ; i < arr_radio.length; i++){
		if (arr_radio[i].checked) {
			try{
				if(arr_radio[i].value=="q")
				{
					this_integral ="";
				}
				else
				{
					this_integral = parseInt(arr_radio[i].value);
				}
			}
			catch(e){}
		}
	}
	if (this_integral == '') this_integral = $("#input_integral").val();

	if (this_integral != '') {
		if (parseInt(valid_integral) < parseInt(this_integral)) {
			document.getElementById("butsubmit").attributes["onclick"].value = '';
			document.getElementById("butsubmit").attributes["class"].value = "reward-btn-big reward-btn-big2 forbidden-btn";
			
			$("#notenough_memo").html('您的积分不够，<a href="http://user.kdnet.net/index.asp#chongzhi" target="_blank" class="c-2477b3">充值</a> 获得。');
		}
		else {
			document.getElementById("butsubmit").attributes["onclick"].value = 'butsubmit();';
			document.getElementById("butsubmit").attributes["class"].value = "reward-btn-big reward-btn-big2";

			$("#notenough_memo").html('');
		}
	}
}

//正则正整数
function checkNum(obj){
	var re = /^[1-9]\d*$/;
	if (!re.test(obj.value)){
		alert("必须为正整数!");
		obj.value="";
		obj.focus();
		return false;
	}
} 

//全部打赏弹窗加入body
function showall (rootid, announcedid, t_integral,total_num) {
	var data = "a=show&rootid="+rootid+"&show_announceid=" + announcedid + "&t_integral="+t_integral;

	$.ajax({
		url: "http://club.kdnet.net/dispbbs_func.asp",
		type: "POST",
		data: data,
		cache: false,
		success: function(html){
			//alert(html);
			var s_html = html.split("|||||");
			$("#userList").html(s_html[0]);

			var strtmp = '共<b class="red">'+total_num+'</b>人打赏 <b class="red">'+s_html[1]+'</b> 分';
			$("#userRewNum").html(strtmp);

			//$('body').append(html);
			$('.reward-userfixed-box').fadeIn(300);
			$('.reward-userfixed-main').css({'margin-left':'-'+($('.reward-userfixed-main').outerWidth()/2)+'px','margin-top':'-'+($('.reward-userfixed-main').height()/2)+'px'});			
		}
	});
}
