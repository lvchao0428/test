	<script>
		function getBrz(obj, type){
			if (!obj.data("brzdata")) {
			    jQuery.getJSON("http://webbiz.xici.net/biz.asp?from=doc&sid=2F2A58DCEAB18A06&tags=同乡 公益 睢宁 反腐倡廉 社会人文 城市发展 评论 行政执法 情感 @1203001@&clubid=1208&jsoncallback=?", function (data) {
			        obj.html(data.Addocsidebar).data("brzdata", data).show();
			        if (type == "another") {
			            jQuery("#tag_li_zt").show();
			            obj.next().hide();
			        } else {
			            if(jQuery("#Adygdoc")){
			                jQuery("#Adygdoc").html(data.Adygdoc);
			            }
			            jQuery("#Adygdoc1").html(data.Adygdoc1);
			            jQuery("#middoc").html(data.middoc);
			        }
			        obj.find("li").each(function () {
			            jQuery(this).addClass(jQuery(this).index() % 2 == 0 ? 'fl' : 'fr');
			        });
			    });
			}
		}

		function light_box() {
	    	//登陆后才给看幻灯片，o(∩_∩)o
	    	if (G_UID > 0) {
	            jQuery('div[data-type="main_topic"], div[id^="floor"]').find('img').each(function (i){
	                var _src = jQuery(this).attr("src");
			var sourceUrl;
			if(_src.indexOf("files.xici.net") > 0){
				sourceUrl=_src.split("?")[0];
			}
			if(_src.indexOf("pan.xici.com") > 0){
				//sourceUrl=_src.split("/");
				//sourceUrl.pop();
				//sourceUrl=sourceUrl.join("/");
				//alert(_src);
				sourceUrl=_src;
			}
	                if (_src.indexOf("files.xici.net") > 0 || _src.indexOf("pan.xici.com") > 0) {
	                	jQuery(this).get(0).removeAttribute("width");
	                	jQuery(this).get(0).removeAttribute("height");
	                }
	                if ((_src.indexOf("files.xici.net") > 0 || _src.indexOf("pan.xici.com") > 0) && sourceUrl ) {
	                    _src = "/img.asp?src=" + sourceUrl;
	                    if ((jQuery(this).width() >= 300 || (jQuery(this).height() >= 300) && jQuery(this).width() >= 150)) {
	                        jQuery(this).attr("rel", "light_box").css("cursor", "pointer").attr('viewatuo', _src);
	                    }
	                }
	            });
	            jQuery("img[rel=light_box]").fancybox({
	                    'padding'			: 10,
	                    'paddingtop'		: 30,
	                    'preventDefault'	: false,
	                    'changeFade'		: 0,
	                    'changeSpeed'		: 0,
	                    'cyclic'			: true,
	                    'autoScale'			: true,
	                    'centerOnScroll'	: true
	            });
	        } else {
	        	jQuery('div[data-type="main_topic"], div[id^="floor"]').find('img').each(function (){
	        		var _src = jQuery(this).attr("src");
					if (_src.indexOf("files.xici.net") > 0) {
	                	jQuery(this).get(0).removeAttribute("width");
	                	jQuery(this).get(0).removeAttribute("height");
	                }
