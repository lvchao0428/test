}
function addtui(data){
if(data==1){
alert('推荐成功\n管理员审核中，感谢您的推荐');
}else if(data==-1){
alert('推荐失败\n该贴已经被推荐过拉');
}else{
alert('推荐失败\n请刷新页面后再次尝试');
}
}
