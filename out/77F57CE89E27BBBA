<script type="text/javascript">
function CheckLoginForm(loginForm)
{
loginForm.action = "http://reg.hexun.com/login.aspx?gourl=" + escape(this.location.href);
if(loginForm.username.value=='')
{
alert('请填写用户名！');
loginForm.username.focus();
return false;
}
else if(loginForm.password.value=='')
{
alert('请填写密码！');
loginForm.password.focus();
return false;
}

return true;
}
function adminshow()
{
var obj = document.getElementById('adminmanage');
if(obj.style.display != "block")
{
obj.style.display='block';
}
else
{
obj.style.display='none';
}
}
function adminclose()
{
var obj = document.getElementById('adminmanage');
obj.style.display='none';
}
function loginclose()
{
var obj = document.getElementById('loginDiv');
obj.style.display='none';
}
function SetCwinHeight(){
  var vote_iframe=document.getElementById("vote_iframe");
  if (document.getElementById){
   if (vote_iframe && !window.opera){
    if (vote_iframe.contentDocument && vote_iframe.contentDocument.body.offsetHeight){
     vote_iframe.height = vote_iframe.contentDocument.body.offsetHeight+20;
    }else if(vote_iframe.Document && vote_iframe.Document.body.scrollHeight){
     vote_iframe.height = vote_iframe.Document.body.scrollHeight+20;
    }
   }
  }
 }

</script>

