var icon = new Object();
icon.root		= CDN_URL + '/' + IMGDIR + '/tree_root.gif';
icon.folder		= CDN_URL + '/' + IMGDIR + '/tree_folder.gif';
icon.folderOpen		= CDN_URL + '/' + IMGDIR + '/tree_folderopen.gif';
