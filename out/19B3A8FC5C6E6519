function swapDisplay(element) {
	if (element.nextSibling) {
		if (element.nextSibling.style.display == "none") {
			element.nextSibling.style.display = "";
			element.className = "navtitle";
		} else {
			element.nextSibling.style.display = "none";
			element.className = "closenav";
		}
	}
}
function switchDisplay(nodeId) {
	if($('nodes_' + nodeId)) {
		if($('nodes_' + nodeId).style.display == 'none') {
			$('nodes_' + nodeId).style.display = '';
			$('icon1_' + nodeId).src = icon.minusMiddle;
			$('icon2_' + nodeId).src = icon.folderOpen;
		} else {
			$('nodes_' + nodeId).style.display = 'none';
			$('icon1_' + nodeId).src = icon.plusMiddle
			$('icon2_' + nodeId).src = icon.folder;
		}			
	}
}
