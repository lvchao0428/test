<script type="text/javascript">
function checkleftmenudiv(menu) {
	//banned user return  by shaoqi 2011/11/24
		var leftmenudiv = document.getElementById('leftmenudiv');
	var stylemenu = document.getElementById('stylemenu');
	var switchbar = document.getElementById('switchbar');
	var frameswitch = document.getElementById('frameswitch');
	var FloatDIV = document.getElementById('FloatDIV');
	if(menu == 'yes') {
		leftmenudiv.style.display = '';
		switchbar.style.display = '';
		FloatDIV.style.left = "143px";
		stylemenu.innerHTML = '<a href="javascript:;" onclick="checkleftmenudiv(\'no\');" class="frameoff">平板模式<\/a>';
		changemenu(menu);
	} else {
		leftmenudiv.style.display = 'none';
		switchbar.style.display = 'none';
		FloatDIV.style.left = '0px';
		stylemenu.innerHTML = '<a href="javascript:;" onclick="checkleftmenudiv(\'yes\');" class="frameon">分栏模式<\/a>';
		changemenu(menu);	
	}
}
function changemenu(num){
	setcookie('stylemenu', '', 0, '/', '.cntv.cn');
	setcookie('stylemenu', num, 1000000, '/', '.cntv.cn');
}
function framebutton(){
	var leftmenudivyes = document.getElementById('leftmenudiv');
	var frameswitch = document.getElementById('frameswitch');
	var FloatDIV = document.getElementById('FloatDIV');
	if(leftmenudivyes.style.display == 'none'){
		leftmenudivyes.style.display = '';
		FloatDIV.style.left = "143px";
	}else{
		leftmenudivyes.style.display = 'none';
		FloatDIV.style.left = '0px';
	}
}
</script>
