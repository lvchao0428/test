<script language="javascript">
//弹出层控制
function dingwxf(announceid,topicid,flagdc) {
	$.ajax({
		  url: "dingwxf.asp?announceid="+announceid+'&topicid='+topicid+'&flagdc='+flagdc,
		  type: "GET",
		  cache: false,
		  success: function(html){
			$('[id=ding_'+announceid+']').html(html);
		  }
		});
}
//sunny 20130925 begin
function showExposeMask (str_id, str_lay) {
	var _floor = $('#floor'+str_lay);
	var x = _floor.offset().top;
	var y = _floor.offset().left;

	var scrollTop = document.documentElement.scrollTop || window.pageYOffset || document.body.scrollTop;
	var exposeMask = document.getElementById('exposeMask');
	var uplBoxF = document.getElementById('uplBoxF');
	var bodyn = document.getElementsByTagName('body')[0];

	var _thisurl = window.location.href;
	if (_thisurl.indexOf('#') >= 0){
		var newrul = _thisurl.substr(0, _thisurl.indexOf('#') + 1) +str_id;
	}
	else {
		var newrul = _thisurl + '#'+str_id;
	}

	if (str_lay == 0) {
		$('#w-title').html('复制楼主的地址');
	}
	else if (str_lay == 1) {
		$('#w-title').html('复制沙发的地址');
	}
	else if (str_lay > 1) {
		$('#w-title').html('复制第 '+str_lay+' 楼的地址');
	}
	$('#w-content').val(newrul);

	$('#exposeMask').show(0).css({'filter':'alpha(opacity=50)'});
	$('#uplBoxFC').show(0);
	$('#uplBoxF').show(0);
	$('body').css({'overflow-x':'hidden','overflow-y':'hidden'});
	bodyn.scroll = 'no';
	exposeMask.style.width = bodyn.offsetWidth+'px';
	exposeMask.style.height = bodyn.scrollHeight+'px';

	uplBoxFC.style.top = x+'px';
	uplBoxFC.style.left = (($(window).width() - 500)/2 - 500)+'px';

	window.scrollTo(0,scrollTop);
}

function hideExposeMask(){
	$('#exposeMask').fadeOut(300);
	$('#uplBoxF').fadeOut(300);
	$('body').css({'overflow-x':'auto','overflow-y':'auto'});
	document.getElementsByTagName('body')[0].scroll = 'auto';
}

function copyToClipboard() { 
	var _data = $('#w-content').val();

	if (window.clipboardData){             //处理费IE浏览器
		var flag = window.clipboardData.setData("text",_data);
		if(flag == true){
			alert("复制成功。");
		}else{
			alert("复制失败。");
		}
	 }else{
		alert("您使用的浏览器不支持此复制功能，请使用Ctrl+C或鼠标右键。");
	} 
} 
//sunny 20130925 end

