<script type="text/javascript">
function category_highlight(){
  var c_words=new Array();
c_words.push(['中国制造2025','3573','0']);
  attrs = jq("#firstpostcontent [id^='attach_']");
  var attrs_str=new Array();
  attrs.each(function(index) {
    attrs_str.push(jq(this));
    jq(this).replaceWith("<div id='attrs_str_replace_" + index + "'></div>");
  });

  var str=jq('#firstpostcontent').html();
  for(w in c_words){
    //c_words[w][0] -> word; c_words[w][1] -> cid; c_words[w][2]->isstock;
    var reg=new RegExp(c_words[w][0] +'([^<>]*)(?=<[^<>]+>)','ig');
    var replace;
    if(c_words[w][2]==1){//股票类
      if(c_words[w][0].match(/^\d{6}$/i)){ //纯代码,显示k线
	replace= '<a class="art_links" href="javascript:void(0)" onmouseover="category_related(jq(this),' + c_words[w][1] + ',1);" title="相关股票">'  + c_words[w][0] +  '</a>$1';
      }else{
	replace= '<a class="art_links" href="javascript:void(0)" onmouseover="category_related(jq(this),' + c_words[w][1] + ',0);" title="相关股票">'  + c_words[w][0] +  '</a>$1';
      }
    }else{//概念类
      replace= '<a class="art_links" href="javascript:void(0)" onmouseover="category_related(jq(this),' + c_words[w][1] + ',0);" title="相关概念">'  + c_words[w][0] +  '</a>$1';
    }
    str=str.replace(reg, replace);
  }
  jq('#firstpostcontent').html(str);
  
  jq(attrs_str).each(function(index){
    jq('#firstpostcontent #attrs_str_replace_' + index).replaceWith(jq(this));
  });


}
category_highlight();

function category_related(box,cid,showkline){
  category_related_hide();
  if(!jq("#kline_box_loader").length){
    /*
    os=box.offset();//元素位置
    sc=jq(window).scrollTop();//滚动条位置
    wh=document.documentElement.clientHeight;//窗口高度
    if( (wh + sc -os.top >= 300) || (os.top-sc < 300)){
      //向下显示
      newtop=os.top + box.height() + 5;
    }else{
      //向上显示
      newtop=os.top - 335;
    }
    newleft=os.left + 50;
    */
    os=box.offset();//元素位置
    sc=jq(window).scrollTop();//滚动条位置
    wh=document.documentElement.clientHeight;//窗口高度
    var boxclass='left';

    newtop=os.top + box.height();
    if(jq(window).width() - os.left < 470){
      newleft=os.left -470;
      boxclass='right';
    }else{
      newleft=os.left + box.width()/2 -30;
    }
    newleft=newleft>30 ? newleft : 30;

    jq("body").prepend("<div id='kline_box_loader' class='kline_boxes kline_boxes_" + boxclass + "' style='display:none'><img src='./images/ajax-loader.gif'/></div>");
    jq("body").prepend("<div id='kline_box' class='kline_boxes kline_boxes_" + boxclass + "' style='display:none'><img src='./images/ajax-loader.gif'/></div>");

    jq(".kline_boxes").css({ top: newtop + 'px', left: newleft + 'px' }); 

    jq("#kline_box_loader").fadeIn('slow');
    jq("#kline_box").load("category_related.php", {cid:cid, tid:6922987, showkline:showkline} , function(data){
      //jq("#kline_box").html(data).mouseleave(function(){category_related_hide();});
      jq("#kline_box").html(data).mouseleave(category_related_hide);
    });

    jq("#kline_box_loader").hide();
    jq("#kline_box").fadeIn('slow');

//    setTimeout(function(){
//      hidekline();
//    },6000);
  }
}

function category_related_hide(){
  jq("#kline_box_loader").remove();
  jq("#kline_box").remove();
}

//-->
</script>
