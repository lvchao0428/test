	slidePubLi.hover(
	function(){
	var way = '0px';
	for(i=0;i<slidePubLi.length;i++){
	if(slidePubLi.eq(i).attr('class') == $(this).attr('class')){
	slidePubLi.eq(i).animate({left:way},700).children().children('.m2').animate({opacity:1});
	way = '810px';
	}else{
	slidePubLi.eq(i).stop().animate({left:way},700);
	}
	}
	},
	function(){
	$(this).stop().children().children('.m2').stop();
	for(i=0;i<slidePubLi.length;i++){ slidePubLi.eq(i).stop().children().children('.m2').stop(); }
	for(i=0;i<slidePubLi.length;i++){
	slidePubLi.eq(i).animate({left:pubX[i]},700).children().children('.m2').animate({opacity:0});
	}
	}
