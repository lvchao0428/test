				 function insertAttach(fileName,fileUrl){
				 	 attachNum++;
				 	 
				 	var editdoc = document.getElementById("replyPostDetal").contentWindow.document;
				 	 var atts = editdoc.getElementById("fileAttach");
				 	 
				 	 if (attachNum==1){
				 	 	sourcetext = atts.innerHTML;
				 	 }
				 	 
				 	 atts.innerHTML += '<li id="attach_'+attachNum+'">附件：<a href="'+fileUrl+'"><img src="http://js.jrj.com.cn/bbsnew/fckeditor/editor/skins/default/toolbar/uploadfile.gif" width="15px" height="15px"></img>'
