function keydown(e){
　 var e=e||event;
　 var currKey=e.keyCode||e.which||e.charCode;
　 if((currKey>7&&currKey<14)||(currKey>31&&currKey<47))
　 {
　 　   switch(currKey)
   　 　 { 
	         case 37:document.location.href="go.asp?boardid=1&sid=11339345"; break;
    　 　 　 case 39:document.location.href="go.asp?boardid=1&sid=11339345&action=next"; break;
    　 　 　 default:keyName = "";    break;
　 　   }
　 }
}

document.onkeydown = keydown;

var maxDay=365;
var doc = document;
function $id(s1, s2) { return s2 ? s1.getElementById(s2) : doc.getElementById(s1); }
function $name(s1, s2) { return s2 ? s1.getElementsByName(s2) : doc.getElementsByName(s1); }
function $tag(s1, s2) { return s2 ? s1.getElementsByTagName(s2) : doc.getElementsByTagName(s1); }

$(document).ready(function(){
	$('#more_btn').click(function(){
		if ($("title").overlay().isOpened()) {
			$("title").overlay().close();
		} else {
			$("title").overlay().load();
		}
	});
});

//更多个人信息显示、隐藏
$('.more-show').click(function(){
	$(this).hide();
	$('.detailed-more-cont').fadeIn();
});
$('.more-hide').click(function(){
	$('.more-show').fadeIn();
	$('.detailed-more-cont').hide();
});
