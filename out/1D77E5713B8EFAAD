		function getBrz(obj, type){
			if (!obj.data("brzdata")) {
			    jQuery.getJSON("http://webbiz.xici.net/biz.asp?from=doc&sid=8938E1BF95899BFD&tags=社团 谈金论股 读书与思考 难忘岁月 都市生活 书画影音 养生保健 诗词歌赋 读帖有感 @1264967@&clubid=1265&jsoncallback=?", function (data) {
			        obj.html(data.Addocsidebar).data("brzdata", data).show();
			        if (type == "another") {
			            jQuery("#tag_li_zt").show();
			            obj.next().hide();
			        } else {
			            if(jQuery("#Adygdoc")){
			                jQuery("#Adygdoc").html(data.Adygdoc);
			            }
			            jQuery("#Adygdoc1").html(data.Adygdoc1);
			            jQuery("#middoc").html(data.middoc);
			        }
