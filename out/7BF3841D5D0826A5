function initialFootprint(data){
    var oc = $id('ulVisitors');
    
    if(data && data.length){
        for(var i=0;i<data.length;i++){
            var u = data[i];
            var li = crt('LI',{},oc);
            crt('IMG',{src:u.logo},crt('A',{href:u.url},li));
            crt('A',{href:u.url,innerHTML:u.name},li);
        }
    }
}
function onGotUser(u){
    if(!post.isLock && typeof theForm != 'undefined')setPoliceCode(theForm);
    
    if(u.id > 0){
        var oS = $id('spnLoginStatus');
        if(oS){
            oS.innerHTML = '已登录用户可在此更换帐号：';
        }
        if($id('aReg'))$id('aReg').style.display = 'none';
    }
    
    if(typeof u.needVerifyCode != 'undefined' && u.needVerifyCode === true){
        //显示验证码
        $id('plVc').style.display = 'block';
        if(!$id('ctl00_c_vc'))
        $id('plVc').innerHTML = '<style>.verifyCodeInput{width:60px;height:16px;line-height:16px;margin-right:4px;}</style>'
        + '<p style="margin:10px 0 5px 0;line-height:20px;">'
        + '请输入验证码：<span id="ctl00_c_vc"><input onfocus="onVcInputFocus(this,\'vc__img\',\'/vc.asmx?\');" maxlength="4" type="text" name="vc_code" id="vc_code" class="verifyCodeInput"/><img src="/vc.asmx" align="absmiddle" id="vc__img" class="verifyCodeImg"/><a class="verifyCodeAnchor" href="javascript:void(0);" target="_self" onclick="replaceCode(\'vc_code\',\'vc__img\',\'/vc.asmx?\')"/>看不清?</a></span>'
        +'</p>';
    }
    
    if(u.isAdmin){
        
        var as = $tags('SPAN',$id('content'));
        for(var i=0;i<as.length;i++){
            var a = as[i];
            if(a.className.indexOf('admA')>-1)a.style.display = 'inline';
        }
        var oc = $id('divAbc');
        var d1 = oc.firstChild.innerHTML;
        var d2 = oc.lastChild.innerHTML;
        new Ajax({url:'BBS.WebUI.Ajax.IPParser,BBS.WebUI.ajax.asmx?bid=153&k=8947423',postBody:'i=' + d1+'&l='+d2,onComplete:function(trans){
            setIL(eval(trans.responseText));
        }});
        
        var odivs = $tags('div');
        for(var i=0;i<odivs.length;i++){
            if(odivs[i].className.indexOf('ipedit')>-1){
                odivs[i].style.display =  'block';
            }
        }
    }
}
function setIL(d){
    if(!d)return;
    var id = d[0];
    var ld = d[1];
    var odivs = $tags('div');
    var rI = 0;
    for(var i=0;i<odivs.length;i++){
        if(odivs[i].className.indexOf('ipedit')>-1){
            var od = odivs[i];
            var spn = $tag('SPAN',od);
            if(spn){
                var oi = $tag('SPAN',spn,0);
                var ol = $tag('SPAN',spn,1);
                if(oi && ol){
                    oi.innerHTML = id[rI];
                    ol.innerHTML = ld[rI];
                }
            }
            rI ++;
        }
    }
}


