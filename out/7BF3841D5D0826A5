var btndiv = document.getElementById('share').getElementsByTagName("div")
for(var i=0;i<btnList.length;i++){
   (function(i){
btnList[i].onmouseover = function(){
btndiv[i].className='on'
}
btnList[i].onmouseout = function(){
btndiv[i].className=''
}
})(i)
}
