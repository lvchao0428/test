			if (_src.indexOf('http://files.xici.net') > -1) {
				_src = _src.substr(_src.length-8);
				if ('.mp3.swf' == _src) {
					_this.width(480);
					_this.height(400);
	                _this.css({
	                            position:"relative",
	                            bottom:"355px"

	                        }
	                );
				}

			}
			if(_src.indexOf("http://imgs.xici.net/_img/flash/flashplayer")>-1){
				_this.width(620);
				_this.height(80);
			}
		});
		

	    jQuery(".scan").mouseover(
