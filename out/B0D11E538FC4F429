function SetjsCookie(name,value)
{
    var exp  = new Date();
    exp.setTime(exp.getTime() + 100*60*1000);
    document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString();
}

SetjsCookie("isjssuport", "yes");
