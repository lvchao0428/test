function showstyledaohang(a,b,c){1==a?document.getElementById(b).className="open":c==100?document.getElementById(b).className="active":document.getElementById(b).className=""}var dbsto;function loaddaohang(){var a=document.getElementById("ifrguide");a&&(a.src="header/daohang.htm");showdh(1)}
