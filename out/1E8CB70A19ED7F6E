                highlightindex++;
            }
            if(highlightindex == autoNodes.length){
                highlightindex = 0;
            }


            autoNodes.eq(highlightindex).addClass("active");
            autoNodes.eq(highlightindex).siblings().removeClass("active");
            //autoNodes.eq(highlightindex).siblings().removeclass("active");
            var li_val=$(".checktype").children("li").eq(highlightindex).text()
            if(li_val!=''){
                $("#k").val(li_val);
            }
            /*var container = $('.option'),
                    scrollTo = $(".checktype").children("li").eq(highlightindex);

            container.scrollTop(
                    scrollTo.offset().top - container.offset().top + container.scrollTop()
            );*/



        }
        else {
            highlightindex = -1;
        }


    })



