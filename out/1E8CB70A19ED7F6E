var doc = document;
var close_cz = doc.getElementById('close_cz');
if(close_cz){
    close_cz.onclick = function(){
        doc.getElementById('popup_02').style.display = 'none';
        //关闭状态保持30天
        var Days = 30;
        setcookie('close_cz',1,Days*24*60*60*1000,'/','.xcar.com.cn');
    }
}
var cz_s;
var car_show = function(info){
    if(info.status == 0)return;
    var str = '<div class="icon"></div><dl>';
    str += '<dt><img src="'+ doc.getElementById('avatar_'+info.f).getAttribute('src')+'" width="50" height="50" /></dt>';
    str +=	'<dd style="padding:0 0 15px 55px"><b style="font-weight:bold; margin-left:-55px">认证卡友</b><span class="v v' + info.level +'"></span></dd>';
    str +=	'<dd><b>等级：</b><span class="line line_v'+ info.level +'"></span></dd>';
    if(info.qianming){
        str += '<dd><b style="float:left">称号：</b>'+info.qianming+'</dd>';
    }
    if(info.rongyu){
        str += '<dd><b>荣誉：</b><i>'+info.rongyu+'</i></dd>';
    }
    if(info.car_num){
        str +=	'<dd style="padding-left:45px;"><b style="margin-left:-45px; float:left; _display:inline">爱车：</b>'+ info.car_num +'辆认证车';
        if(info.car_number){
            str += '&nbsp;(其中&nbsp;<font>V'+info.level+'</font>&nbsp;'+info.car_number+'辆)';
        }
        str += '</dd>';
    }
    if(info.car){
        str +=	'<dd style="padding-left:45px;">';
        str +=		'<b style="margin-left:-45px; float:left; _display:inline">爱车：</b>';
        var c = '';
        for(i in info.car){
            c = info.car[i].level ? ' pic_line' : '';
            str += '<div class="pic';
            str += info.car[i].level ? ' pic_line"><div class="v_icon"></div>' :'">';
            str += '<img src="'+ info.car[i].img +'" width="80" height="60" onerror="this.onerror=null;this.src=\'http://my.xcar.com.cn/images/8060.jpg\';" /><span>';
            if(info.car[i].fid == 0){
                str += info.car[i].name;
            }else{
                str += '<a target="_blank" href="http://www.xcar.com.cn/bbs/forumdisplay.php?fid='+ info.car[i].fid +'">'+ info.car[i].name +'</a>';
            }
            str += '</span></div>';
        }
        str +=	'</dd>'
    }
    str +=	'</dl>';
    str +=	'<div class="clear"></div>';
    str +=	'<p><a href="http://my.xcar.com.cn/chezhu/apply.php?zoneclick=101730" target="_blank">申请认证</a></p>';
    doc.getElementById('cz_info_'+info.f).innerHTML = str;
    doc.getElementById('cz_info_'+info.f).style.display = 'block';
}
