<script language="javascript">
    /*getScript('http://d0.xcar.com.cn/d0/push_bbs_ad1.php?pid=112&fid='+897);
    getScript('http://d0.xcar.com.cn/d0/push_bbs_ad1.php?pid=99&fid='+897);*/

    var s = window.navigator.userAgent.toLowerCase().match(/msie ([\d.]+)/);
    isIE6 = (s != null && typeof s[0] == 'string' && s[0]=='msie 6.0');
    var preventDefault = preventDefault||function(event){
        if(event.preventDefault){	// !IE
            event.preventDefault();
        }else{	//IE
            event.returnValue = false;
        }
    };
    function element_fixed(o,C){
        if(typeof o != "object" && typeof o.element == 'undefined')return false;
        var doc  = document;
        var docE = doc.documentElement;
        var e    = o.element;
        var es   = e.style;
        var Int  = parseInt;
        if(is_exist(o.zIndex))es.zIndex = o.zIndex; else es.zIndex = 999;
        if(is_exist(o.width))es.width = o.width;
        if(is_exist(o.height))es.height = o.height;
        if(isIE6){
            es.position = "absolute";
            if(is_exist(o.left))var left = es.left = o.left;
            if(is_exist(o.top))var top = es.top = o.top;
            if(is_exist(o.right))var left = es.left = Int(docE.clientWidth)  - Int(e.offsetWidth);
            if(is_exist(o.bottom))var top = es.top  = Int(docE.clientHeight) - Int(e.offsetHeight) - Int(o.bottom);
            if(typeof window.onscroll == 'function')var CF = window.onscroll;
            window.onscroll = function(){
                if(typeof CF == 'function')CF();
                var scrollTop  = docE.scrollTop;
                var scrollLeft = docE.scrollLeft;
                if(is_exist(o.top) || is_exist(o.bottom))es.top  = ( Int(scrollTop)  + Int(top)  ) + "px";
                if(is_exist(o.left) || is_exist(o.right))es.left = ( Int(scrollLeft) + Int(left) ) + "px";
            }
        }else{
            if(is_exist(o.left))es.left = o.left;
            if(is_exist(o.top))es.top = o.top;
            if(is_exist(o.right))es.right = o.right;
            if(is_exist(o.bottom))es.bottom = o.bottom;
            es.position = 'fixed';
        }
        if(typeof C == 'function')C();
    }
    function is_exist(attr){
        return typeof attr != 'undefined'?true:false;
    }
    var bdshare_div = document.getElementById('bdshare_div');
    if(bdshare_div)element_fixed({element:bdshare_div,top:'-264'});
</script>
