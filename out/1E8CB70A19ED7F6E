    //对楼层点赞，显示效果，数据库更新
    $(".praise_tijiao").bind('click',function(){
        var praise_text=$(this).text();
        if(praise_text.indexOf('已') <0){
            var mythis = $(this);
            var pid = $(this).attr('pid');
            var zan_data={tid:22662327,fid:897,pid:pid,action:'zan'};
            $.ajax({
                type : 'POST',
                url : '/bbs/ajax.php',
                data : zan_data,
                success: function(data){
                    if(data==2){

                        var praise_text=mythis.text();

                        if(praise_text.indexOf('(') >=0){
                            var zan_num=/\((.*)\)/.exec(praise_text);
                            zan_num=parseInt(zan_num[1])+1;
                            praise_text='已赞'+'('+zan_num+')';
                        }else{

                            praise_text='已赞(1)';

                        }
                        /*var praise_text='已赞';*/
                        mythis.text(praise_text);

                    }
                }

            });
        }

    })

    var pid_str = '';//获取pid字符串
    $(".praise_tijiao").each(function(i){
        pid_str += $(this).attr('pid') + ',';
    })
    
    //判断是否已经赞过
    var praises = [];
    $('.make_info').bind('mouseover',function(){
        var pid = $(this).find('.praise_tijiao').attr('pid');
        if(praises[pid]){
            return false;
        }
        praises[pid] = true;
        var mythis = $(this);
        var my_zan_data={pid:pid,action:'my_zan'};
        $.ajax({
            type : 'POST',
            url  : '/bbs/ajax.php',
            data :  my_zan_data,
            success: function(data){
                var praise_text = mythis.find('.praise_tijiao').text();
                if(data == 1){
                    praise_text = '已' + praise_text;
                    mythis.find('.praise_tijiao').text(praise_text);
                }
            }
        });
    });

