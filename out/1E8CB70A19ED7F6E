        $(document).bind('keydown',function(event){
            var rb = document.activeElement.tagName.toLowerCase();
            var obj = event.target || event.srcElement;//获取事件源
            var t = obj.type || obj.getAttribute('type');//获取事件源类型
            var e = event || window.event;
            //var e = event;
            var kc = e.which || e.keyCode;
            if(!e.ctrlKey && kc == 13 && fast_state){
                $('#quick_reply').click();
            }
            if((kc==39 || kc==37) && t!=null){
                return true;
            }
            var size = 10;
            var currentpage = 1;
            var url = window.location.href;
            
            var total = 14+1;
            
            var maxpage = Math.ceil(total/size);
            maxpage = maxpage > currentpage ? maxpage : currentpage;
            switch(kc){
                case 27:
                    $(".grade_close,.close_btn").closest(".grade").hide();
                    $(".grade_close,.close_btn").closest(".grade").prev(".grader_bg").hide();
                    break;
                case 39:
                    if(currentpage<maxpage){
                        if(url.indexOf('&page') !='-1'){
                            url = url.replace(/&page+=+[^&]*/g,'&page='+(currentpage+1));
                        }else{
                            url +='&page='+(currentpage+1);
                        }
                        fanyebox('正在为您打开下一页...',url);
                    }else{
                        fanyebox('已经是最后一页啦！','');
                    }
                    break;
                case 37:
                    //prev page
                    //var href = $('.page2013').find("em[class='active']").prev('a').attr('href');
                    //if(href)window.location.href = href;
                    if(currentpage>1){
                        if(url.indexOf('&page') !='-1'){
                            url = url.replace(/&page+=+[^&]*/g,'&page='+(currentpage-1));
                        }else{
                            url +='&page='+(currentpage-1);
                        }
                        fanyebox('正在为您打开上一页...',url);
                    }else{
                        fanyebox('已经到第一页啦！','');
                    }
                    break;
            }
        });
