var is_tuijian = false;
$("#tuijian_ajax").click(function(){
if(is_tuijian || $(this).html().indexOf('已推荐') >= 0){
return false;
}
$(this).unbind('click');
$(this).xLoginBox({show:true,step:2,func:function(){window.location.href=window.location.href}});
return false;
is_tuijian = true;
var tuijian_num = $(this).html();
tuijian_num = tuijian_num.replace('推荐','');
tuijian_num = tuijian_num.replace('(','');
tuijian_num = tuijian_num.replace(')','');
if(!tuijian_num){
tuijian_num = 0;
}
$.ajax({
type : "POST",
url  : "viewthread.php",
dataType : 'json',
data : {'action':'digesttj','tid':'22662327','formhash':'a7a068f6','is_ajax':1},
success: function(data){
if(data.stat == 1){
tuijian_num = parseInt(tuijian_num)+1;
$("#tuijian_ajax").removeClass('collect_ing').addClass('collect_ed').html('已推荐('+tuijian_num+')');
}
$("#bbs_ft_win").html(data.reason).show();
floating_window_positioning('bbs_ft_win');
setTimeout(function(){
$('#bbs_ft_win').hide();
},3000);

is_tuijian = false;
}
});
