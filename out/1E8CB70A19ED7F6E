    $('.sq_Authenticate_btn01 > a').hover(function(){
        if(cz_s){
            clearTimeout(cz_s);
        }
        var f = $(this).parent('.sq_Authenticate_btn01').attr('id');
        var s = $('#cz_info_'+f.substr(3));
        if($(this).attr('class') == 'bussiness'){
            //认证商家
            $('#cz_info_'+f.substr(3)).show();
        }else {
            if ($(this).attr('class').substr(6) > 1) {
                if (!s)return;
                if (s.find('img').length) {
                    s.show();
                } else {
                    getScript(car_api + '?uid=' + $(this).attr('name') + '&f=' + f);
                }
            } else {
                var src = doc.getElementById('avatar_' + f.substr(3)).getAttribute('src');
                $('#cz_info_' + f.substr(3)).find('img').attr('src', src);
                $('#cz_info_' + f.substr(3)).show();
            }
        }
    },function(){
        var i = $(this).parent('.sq_Authenticate_btn01').attr('id').substr(3);
        cz_s = setTimeout(function(){
            $('#cz_info_'+i).hide();
        },300);
    });
