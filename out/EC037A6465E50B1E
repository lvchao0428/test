<script>
	var inter_variable;
	function refreshConfSet(){
		var time = parseFloat($('#autoRefreshConfSelect').val());
		var date = new Date();
		date.setTime(date.getTime()+365*24*3600*1000);
		document.cookie = 'Ref_Msg_Conf=' + time + ';expires=' + date.toGMTString();
		clearInterval(inter_variable);
		if(time != 0){
			inter_variable = setInterval(function(){
					window.location.href = window.location.href;
				},time*60*1000);
		}
	}
	function getDefaultRefreshTime(){
		var cook = document.cookie;
		var start = cook.indexOf('Ref_Msg_Conf');
		var time = parseFloat('0');
		if(start != -1){
			start += 'Ref_Msg_Conf'.length + 1;
			var end = cook.indexOf(';',start);
			if(end == -1){
				time = parseFloat(cook.substring(start));
			}else{
				time = parseFloat(cook.substring(start,end));
			}
		}
		clearInterval(inter_variable);		
		$('#autoRefreshConfSelect').val(time);
		if(time != 0){
			inter_variable = setInterval(function(){
					window.location.href = window.location.href;
				},time*60*1000);
		}
	}
	
	getDefaultRefreshTime();
</script>
