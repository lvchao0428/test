			    jQuery.getJSON("http://webbiz.xici.net/biz.asp?from=doc&sid=961C5E66D71E3EF4&tags=足球 校园足球 足球教学 足球联赛 体育 体育运动 体育教学 体质健康 足球改革 @1206011@&clubid=1307&jsoncallback=?", function (data) {
