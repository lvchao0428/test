function checkText2(){
var valtxt=jQuery("#hf_contents").val();
if(valtxt==''||valtxt=='我要说一句'||valtxt=='哎呀，你什么都还没说呢！')
{
  jQuery("#hf_contents").css("border","1px solid red");
  jQuery("#hf_contents").attr("placeholder","哎呀，你什么都还没说呢！");
  return false;
}
else {
jQuery("#hf-quick").submit();
}

}

