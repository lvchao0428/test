         key.push(i);
      }
      var img = new Image();
      var d = new Date().getTime();
      img.onload = function () {
         img = null;
      }
      img.onerror = function () {
         img = null;
      }
      var Src = "http://rc.so.cnzz.net/stat.gif?url=" + encodeURIComponent(window.location.href) + "&ts=" + d;
