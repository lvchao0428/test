	<script type="text/javascript">
	window.onload = function () {
			var SessionID = '0996AA36AF538748';
			if (SessionID != "" && window.localStorage){
				var key = localStorage.getItem("key");
				if (key!=SessionID){
					localStorage.setItem("key",SessionID);
					localStorage.setItem("count",1);
				}else{
					var value = Number(localStorage.getItem("count"));
					localStorage.setItem("count",value+1);
					if ((value % 6)==0){
						bdAccessLogon(location.href.substring(location.host.length + 8),'_self');
					}
				}
			}
		}
	</script>
