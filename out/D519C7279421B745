				 function delAttach(index){           
         			 delIndex=index;
         			 var editdoc = document.getElementById("replyPostDetal").contentWindow.document;
         			 var sAnnex = editdoc.getElementById("annxe");
         			 if(sAnnex) {
						var sValue = sAnnex.value;
						var iIndex = sValue.indexOf("|"+index);
						if (index==1) iIndex = 0;
						var iEndIndex = sValue.indexOf("|",iIndex+1);
						if(iEndIndex==-1) {
							iEndIndex = sValue.length;
							}
						var firstStr = sValue.substring(0,iIndex);
						var endStr = sValue.substring(iEndIndex+1);
						if (index!=1){
							sAnnex.value = firstStr +"|"+ endStr;
						}else{
							sAnnex.value = firstStr + endStr;
						}
						
						var pTar = document.getElementById("replyPostDetal");
					    if (pTar.contentDocument && pTar.contentDocument.body.offsetHeight){  
					      pTar.height = pTar.contentDocument.body.offsetHeight;   
					    }else if (pTar.Document && pTar.Document.body.scrollHeight){  
					      pTar.height = pTar.Document.body.scrollHeight;  
					    }
             		 }
         			 delCallBack();
				 }
				 function delCallBack(){
					 var editdoc = document.getElementById("replyPostDetal").contentWindow.document;
				   	var atts = editdoc.getElementById("fileAttach");
				 	 var att = editdoc.getElementById("attach_"+delIndex);
				 	 atts.removeChild(att);
				 }
				 function goPage(pagename){
				 	var gopage = document.getElementById("gopage"+pagename).value;
				 	if(gopage!=null && gopage!=""){
        				var r,re;
        				re = /\d*/i;
        				r = gopage.match(re);
        				if (r!=gopage){
        					alert("请输入数字页码");
        					return;
        				}
    				}else{
    					alert("请输入数字页码");
    					return;
    				}
    				//window.location.href= "/dmsg/1_98482923_"+gopage+".html";
				 	window.location.href= "/msg,98482923,"+gopage+".html";
				 }
				</script>
