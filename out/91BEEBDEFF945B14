    <script type="text/javascript">
        function ShowJunphProductList() {
            var str = "";
            $.ajax({
                type: "get ",
                url: "http://jph.itiexue.net/",
                dataType: "jsonp",
                success: function (data) {
                    if (data != null) {
                        if (data.Error == false) {
                            if (data.Result.length > 0) {
                                $.each(data.Result.slice(0,2), function (i, n) {
                                    var jumpUrl = "http://www.junph.com/html/product" + n.Id + ".html";
                                    if (n.JumpUrl != null && n.JumpUrl != "") {
                                        jumpUrl = n.JumpUrl;
                                    }
                                    jumpUrl = jumpUrl+"?utm_source=TXZN&utm_medium=404"+(i+1)+"&utm_campaign=CP"+(i+1);
                                    if (n.ImgUrls && n.ImgUrls.length > 0) {
                                        var urls = n.ImgUrls;
                                        var rand = Math.floor(Math.random() * urls.length);
                                        var ProductImgId = urls[rand].ProductImgId;
                                        var ImgUrl = urls[rand].ImgUrl;

                                        str+="<li><a target=\"_blank\" title=\""+ n.Name +"\" href=\"" + jumpUrl +"\"><img alt=\""+ n.Name +"\" src=\"" + ImgUrl + "\" width=\"103\" height=\"103\"></a><span><a target=\"_blank\" title=\""+ n.Name +"\" href=\"" + jumpUrl +"\">"+ n.Name +"</a></span></li>";

                                    } else {
                                        str+="<li><a target=\"_blank\" title=\""+ n.Name +"\" href=\"" + jumpUrl +"\"><img alt=\""+ n.Name +"\" src=\"" + ImgUrl + "\" width=\"103\" height=\"103\"></a><span><a target=\"_blank\" title=\""+ n.Name +"\" href=\"" + jumpUrl +"\">"+ n.Name +"</a></span></li>";

                                    }
                                });
                                $(".pp").html(str);
                            }
                        }
                    }
                }
            });           
        }
        ShowJunphProductList();
    </script>
