						function chksearchvalue(){
							var searchvalue = $$('headersearch');
							var valuetmp = searchvalue.value.replace('输入关键字','');
							if(valuetmp){
							}else{
								searchvalue.value=searchvalue.value.replace('输入关键字','');
							}
						}
						function chksearch(){
							
								var searchvalue = $$('headersearch').value;
								searchvalue = searchvalue.replace('输入关键字', '');
								if(searchvalue){
									return true;
								}else{
									alert('请输入搜索的关键词');
									return false
								}
							
						}

					
