function display700x250() {
	var div700x250 = document.getElementById("hetoo_tb_700x250");
	if ( div700x250 == null ) return;
	div700x250.style.cssText = "margin:10px auto;padding:0;width:960px;height:250px;overflow:hidden;background-color:#fff;position:relative;";
	var headTag = document.getElementsByTagName("head")[0];

	var div1 = document.createElement("div");
	div1.style.cssText = "margin:0 2px 0 0;padding:0;width:300px;height:250px;overflow:hidden;background-color:#fff;float:left;";
	var div1_a = document.createElement("a");
	div1_a.style.cssText = "display:none!important";
	div1_a.id = "tanx-a-mm_50128954_4406826_21202102";
	div1.appendChild(div1_a);
	var div1_script = document.createElement("script");
	div1_script.async = true;
	div1_script.type = "text/javascript";
	div1_script.charset = "gbk";
	div1_script.src = "http://p.tanx.com/ex?i=mm_50128954_4406826_21202102";
	div1_script.id = "tanx-s-mm_50128954_4406826_21202102";
	div700x250.appendChild(div1);
	headTag.appendChild(div1_script);

	var div2 = document.createElement("div");
	div2.style.cssText = "margin:0 2px 0 0;padding:0;width:296px;height:250px;overflow:hidden;background-color:#fff;float:left;";
	var div2_a = document.createElement("a");
	div2_a.style.cssText = "display:none!important";
	div2_a.id = "tanx-a-mm_50128954_4406826_21200182";
