function RePage(url,key){
    if(key == 1){
        document.location = url + "1.htm";
    }else{
        if ( key < 1 || key.toString == "" ){
            key = 1;
        }
        document.location = url + key.toString() + ".htm";
    }
    return false;
}
</script>
