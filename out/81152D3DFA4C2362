    function PointCity(baseurl,url,setcityid,setlat,setlng,setrange) {
        var Cfrm = document.citychangeform;
        Cfrm.action = baseurl;
        Cfrm.setcityid.value = setcityid;
        Cfrm.setlat.value = setlat;
        Cfrm.setlng.value = setlng;
