            function navTab(id) {
                var $_Tit = $("#" + id + ">li").not(".noLayer");
                var $_Sub = $("#" + id + ">li>ul");
                var timmer = null;
                var n = null;
                $_Tit.hover(function () {
                    n = $_Tit.index(this)
                    $(this).addClass("select").siblings().removeClass("select");
                    $(this).children("ul").show();
                }, function () {
                    $_Tit.eq(n).removeClass("select");
                    $_Sub.eq(n).hide();
                })
            }
            navTab("topNav");
