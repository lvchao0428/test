		function getBrz(obj, type){
			if (!obj.data("brzdata")) {
			    jQuery.getJSON("http://webbiz.xici.net/biz.asp?from=doc&sid=43D4974C08E0AC22&tags=职业培训 公考 行测 行政能力测验 申论 2013省考 云南省公务员 云南公务员 公务员真题 公务员考试 @628370@&clubid=2672&jsoncallback=?", function (data) {
			        obj.html(data.Addocsidebar).data("brzdata", data).show();
			        if (type == "another") {
			            jQuery("#tag_li_zt").show();
			            obj.next().hide();
			        } else {
			            if(jQuery("#Adygdoc")){
			                jQuery("#Adygdoc").html(data.Adygdoc);
			            }
			            jQuery("#Adygdoc1").html(data.Adygdoc1);
			            jQuery("#middoc").html(data.middoc);
			        }
