function kmmenu(id,op){
if(op){
clearTimeout(closetimer[id]);
$("menu"+id).className="hover";
$("menu"+id+"_1").style.display="block";
}else{
closetimer[id] = setTimeout(function(){kmhmenu(id,op);},500);
}
}
function kmhmenu(id,op){
$("menu"+id).className="";
$("menu"+id+"_1").style.display="none";
}
