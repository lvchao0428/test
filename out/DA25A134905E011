		                var _option = _this.parent().attr("node-type").split("{$$}");
		                _gaq.push(['_trackEvent', _option[1], _option[0], _option[2]]);

		            });
				}
				jQuery(".bfd_a").show();
			} else {
				jQuery(".doclist_show").eq(0).show();
			}
		}

