		function ViewTopic(datas){
			var strArr = [];
			if(datas && datas.length > 0) {
				strArr.push('<ul>');
				for (var i = 0; i < datas.length; i ++) {
					strArr.push('<li data-type="ga" node-type="百分点同类帖子推荐{$$}doc{$$}'+ bfd_key.clubmabe +'|'+ bfd_key.bdid + '|' + datas[i].name + '|' + datas[i].iid + '"> ・<a href="/' + datas[i].iid + '.htm" title="' + datas[i].name + '" target="_blank">' + datas[i].name + '</a></li>');
					if (i == 2) {
						strArr.push('</ul><ul>');
					}
				}
				strArr.push('</ul>');
				if(lClubId == "2512" && G_CONFIG.cityId == "142320100"){
					jQuery("#bf_doclist").remove();
					jQuery("#other_doc .doclist_hide").show();
					jQuery("#other_doc .doclist_show").remove();
				}else{
					jQuery("#other_doc .doclist_hide").remove();
					jQuery("#bf_doclist").html(strArr.join(""));
		            jQuery("#bf_doclist").find("a").live("click", function (){
		                var _this = jQuery(this);
		                var _option = _this.parent().attr("node-type").split("{$$}");
		                _gaq.push(['_trackEvent', _option[1], _option[0], _option[2]]);

		            });
				}
				jQuery(".bfd_a").show();
			} else {
				jQuery(".doclist_show").eq(0).show();
			}
		}

		function ViewBBS(json){
			var strArr = [];
			if (json && json.length > 0) {
				for (var i = 0; i < json.length; i ++) {
					strArr.push('<li title="'+ json[i].name +'" class="clearfix"><p class="boardpic"><a href="/' + json[i].iid + '" data-type="ga" node-type="百分点同类版推荐{$$}doc{$$}'+ bfd_key.clubmabe +'|'+ bfd_key.bdid + '|' + json[i].name + '|' + json[i].iid + '"><img src="' + json[i].img + '"></a></p><p class="boardmsg"><a href="' + json[i].iid + '" data-type="ga" node-type="百分点同类版推荐{$$}doc{$$}'+ bfd_key.clubmabe +'|'+ bfd_key.bdid + '|' + json[i].name + '|' + json[i].iid + '">' + json[i].name + '</a> </p></li>');
				}
				jQuery("#bf_boardlist .hot_board").html(strArr.join(""));
				jQuery("#bf_boardlist .hot_board").find("a").live("click", function (){
	                var _option = jQuery(this).attr("node-type").split("{$$}");
	                _gaq.push(['_trackEvent', _option[1], _option[0], _option[2]]);
	            });
				jQuery(".bf_board a").show();
				jQuery("#bf_boardlist").show();
			} else {
				jQuery(".boardlist_show").eq(0).show();
			}
		}
