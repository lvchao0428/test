				var evt = window.event?window.event:evt,target=evt.srcElement||evt.target;
				if(target.id == "idFace"){
					document.getElementById("idFaceList").style.display = "block";
					return;
				}else if (target.id != '') {
					while(target.nodeName.toLowerCase() != "div" && target.nodeName.toLowerCase() != "html"){
						target = target.parentNode;
					}
					if(target.id != "idFaceList"){
						document.getElementById("idFaceList").style.display = "none";
					}
				}
			});
		}
