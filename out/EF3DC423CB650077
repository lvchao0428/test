function lsSubmit_sylar(op) {
var op = !op ? 0 : op;
if(op) {
$('lsform_sylar').cookietime.value = 2592000;
}
if($('ls_username_sylar').value == '' || $('ls_password_sylar').value == '') {
showWindow('login', 'member.php?mod=logging&action=login' + (op ? '&cookietime=1' : ''));
} else {
ajaxpost('lsform_sylar', 'return_ls_sylar', 'return_ls_sylar');
}
return false;
}

function checklogtime(){
if(!getcookie('loginremind')||getcookie('loginremind')<new Date().getTime()){
document.getElementById("buttom_login").style.display='block';
}else{
window.setTimeout(function(){checklogtime();},1000)
}
}
function closelogin(){
var locktime = 20*1000;
document.getElementById("buttom_login").style.display='none';
pp=new Date().getTime()+locktime;
setcookie('loginremind',pp,100);
window.setInterval(function(){checklogtime();},1000);
}
</script>
