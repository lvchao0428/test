var _site = encodeURIComponent("红网论坛");
var _title = encodeURIComponent("衡阳市中心城区国有土地上房屋征收与补偿实施办法 2014年10月19日");
var _pics = "";
var _from = 'discuz';
var _u = 'http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?to=pengyou&url='+_url+'&site='+_site+'&title='+_title+'&pics='+_pics+'&from='+_from;
window.open(_u);
}
</script>
	


