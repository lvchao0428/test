    function lsSubmit() {
        var UserName = document.getElementById("ls_username").value;
        if (UserName.length <= 0||UserName=="请输入用户名") {
            alert("请输入用户名");
            return false;
        }
        var Pwd = hex_md5(document.getElementById("ls_password").value);
        if (document.getElementById("ls_password").value.length <= 0||document.getElementById("ls_password").value=="请输入密码") {
            alert("请输入密码");
            return false;
        }
   document.getElementById("ls_password").value=Pwd;
}
</script>
