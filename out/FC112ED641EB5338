  <script>
  (function (G,D,s,c,p) {
  c={//监测配置
  UA:"UA-xcar-000001", //客户项目编号,由系统生成
  NO_FLS:0,//是否使用Flash LocalStorage，默认为0开启，1表示不使用
  WITH_REF:1, 
  URL:'http://js.xcar.com.cn/cms/iwt/iwt-min.js'//iwt.js的URL位置，请客户自行托管JS文件，只需修改此值
  };
