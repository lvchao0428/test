				var attachNum=0;
				var delIndex=null;
				var sourcetext = "";
				 
				 function insertAttach(fileName,fileUrl){
				 	 attachNum++;
				 	 
				 	var editdoc = document.getElementById("replyPostDetal").contentWindow.document;
				 	 var atts = editdoc.getElementById("fileAttach");
				 	 
				 	 if (attachNum==1){
				 	 	sourcetext = atts.innerHTML;
				 	 }
				 	 
				 	 atts.innerHTML += '<li id="attach_'+attachNum+'">附件：<a href="'+fileUrl+'"><img src="http://js.jrj.com.cn/bbsnew/fckeditor/editor/skins/default/toolbar/uploadfile.gif" width="15px" height="15px"></img>'
				 	   +'&nbsp;'+fileName+'</a>&nbsp;&nbsp;<a href="javascript:void(0)" name="delete" onclick="parent.delAttach('+attachNum+')">删除附件</a>&nbsp;&nbsp;&nbsp;&nbsp;售价（铜板）：<input name="filecoin_'+attachNum+'" id="filecoin_'+attachNum+'" type="text" style="width:30px" /> 最高1000</li>';
				 	 atts.innerHTML = atts.innerHTML.replace(sourcetext,"") + sourcetext;
				 	   
				 	   var sAnnex = editdoc.getElementById("annxe");
				 	   if(!sAnnex){
					 	   var form1 = editdoc.getElementById("form1");
							var textHidden = editdoc.createElement("input");
							textHidden.type = "hidden";
							textHidden.name ="annxe";
							textHidden.id = "annxe";
							textHidden.value = "";
							form1.appendChild(textHidden);
							sAnnex = textHidden;
					   }
				 	  var value = sAnnex.value;
				 	  if(value!="") {
							value += "|";
					  }
				 	  value += attachNum+","+fileUrl+","+ fileName + "," + "http://js.jrj.com.cn/bbsnew/fckeditor/editor/skins/default/toolbar/uploadfile.gif";
				 	  sAnnex.value = value;
				 	  
						var pTar = document.getElementById("replyPostDetal");
					    if (pTar.contentDocument && pTar.contentDocument.body.offsetHeight){  
					      pTar.height = pTar.contentDocument.body.offsetHeight;   
					    }else if (pTar.Document && pTar.Document.body.scrollHeight){  
					      pTar.height = pTar.Document.body.scrollHeight;
					    }
				 }
				 function delAttach(index){           
         			 delIndex=index;
         			 var editdoc = document.getElementById("replyPostDetal").contentWindow.document;
         			 var sAnnex = editdoc.getElementById("annxe");
         			 if(sAnnex) {
						var sValue = sAnnex.value;
						var iIndex = sValue.indexOf("|"+index);
						if (index==1) iIndex = 0;
						var iEndIndex = sValue.indexOf("|",iIndex+1);
						if(iEndIndex==-1) {
							iEndIndex = sValue.length;
							}
						var firstStr = sValue.substring(0,iIndex);
						var endStr = sValue.substring(iEndIndex+1);
						if (index!=1){
							sAnnex.value = firstStr +"|"+ endStr;
						}else{
							sAnnex.value = firstStr + endStr;
						}
						
						var pTar = document.getElementById("replyPostDetal");
					    if (pTar.contentDocument && pTar.contentDocument.body.offsetHeight){  
					      pTar.height = pTar.contentDocument.body.offsetHeight;   
					    }else if (pTar.Document && pTar.Document.body.scrollHeight){  
					      pTar.height = pTar.Document.body.scrollHeight;  
					    }
             		 }
         			 delCallBack();
				 }
				 function delCallBack(){
					 var editdoc = document.getElementById("replyPostDetal").contentWindow.document;
				   	var atts = editdoc.getElementById("fileAttach");
				 	 var att = editdoc.getElementById("attach_"+delIndex);
				 	 atts.removeChild(att);
				 }
				 function goPage(pagename){
				 	var gopage = document.getElementById("gopage"+pagename).value;
				 	if(gopage!=null && gopage!=""){
        				var r,re;
        				re = /\d*/i;
        				r = gopage.match(re);
        				if (r!=gopage){
        					alert("请输入数字页码");
        					return;
        				}
    				}else{
    					alert("请输入数字页码");
    					return;
    				}
    				//window.location.href= "/dmsg/1_98387955_"+gopage+".html";
				 	window.location.href= "/msg,98387955,"+gopage+".html";
				 }
