<script type="text/javascript">
$(pageInit);
var editor;
function pageInit()
{
	var allPlugin={
		meitu:
		{  
			c:'meituxiuxiu',
		    t:'美图秀秀在线编辑插件',
			s:'ctrl+5',
			h:1,
			e:function(){
				var _this=this;
				var arrMenu=[{s:'美化图片',v:'external/xiuxiu.php?typeid=1',t:'美图秀秀-美化图片'},{s:'拼接图片',v:'external/xiuxiu.php?typeid=2',t:'美图秀秀-拼接图片'},{s:'大头贴',v:'external/xiuxiu.php?typeid=4',t:'美图秀秀-大头贴'},{s:'高级应用',v:'external/xiuxiu.php?typeid=3',t:'美图秀秀-高级应用'}];
				_this.saveBookmark();
				function clossxiuxiu()
				{
					var framesnum = window.frames.length;
					for(i=0;i<framesnum;i++)
					{
						try 
						{
							temp_url = String(window.frames[i].location);
							if (temp_url.indexOf('xiuxiu') > -1)
							{
								window.frames[i].location = window.frames[0].location;
								break;	
							}
						}
						catch(err)
						{
							continue;	
						}
						
					}
				}
				_this.showMenu(arrMenu,function(v){_this.showIframeModal('美图秀秀在线编辑',v,function(v){_this.loadBookmark();},800,530,function(v){clossxiuxiu();});});
			}
		}
	};
	
	editor=$('#WYSIWYG_HTML').xheditor({plugins:allPlugin, tools:'Blocktag,Fontface,FontSize,Bold,Italic,Underline,Strikethrough,FontColor,BackColor,|,Align,Outdent,Indent,|,Link,Img,meitu,Emot,',loadCSS:'<style>body{font-size:14px;line-height:1.5;}</style>'});//交互方式1
	editor=$('#WYSIWYG_HTML')[0].xheditor;//交互方式2
}

</script>
