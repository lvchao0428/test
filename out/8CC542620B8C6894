function setIL(d){
    if(!d)return;
    var id = d[0];
    var ld = d[1];
    var odivs = $tags('div');
    var rI = 0;
    for(var i=0;i<odivs.length;i++){
        if(odivs[i].className.indexOf('ipedit')>-1){
            var od = odivs[i];
            var spn = $tag('SPAN',od);
            if(spn){
                var oi = $tag('SPAN',spn,0);
                var ol = $tag('SPAN',spn,1);
                if(oi && ol){
                    oi.innerHTML = id[rI];
                    ol.innerHTML = ld[rI];
                }
            }
            rI ++;
        }
    }
}


function getPostCn(){
    var oposts = $id('ulCreatorPosts');
    var getPosts = oposts?1:0;
    new Ajax({showLoading:false,method:'get',
        url:'BBS.WebUI.Ajax.UserAjax,BBS.WebUI.ajax.asmx?a=getUserIsOnlineAndPostCn&uid=21790088&getPosts='+getPosts+'&t=' + new Date().getTime(),
        onComplete:function(trans){
            var r ;eval('r=' + trans.responseText);
            if(r){
                //$id('postCount').innerHTML = r.postCount;
                //$id('essentialCount').innerHTML = r.essentialCount;
                
                var oOnline=$id('imgIsOnline');
                if(oOnline){
                    oOnline.src = '/clientfiles/post/img/' + (r.joidOnline?'online.gif':'unline.gif');
                    oOnline.title = oOnline.alt = (r.joidOnline?'在线':'未在线');
                }
                if(getPosts == 1){
                    if(r.posts && r.posts.length > 0){
                        while(oposts.firstChild)remove(oposts.firstChild);
                        
                        for(var i=0;i<r.posts.length;i++){
                            var iP = r.posts[i];
                            crt('A',{href:iP.url,innerHTML:iP.title,title:iP.title},crt('LI',{},oposts));
                        }
                    }
                }
            }
        }});
}
addLoadEvent(getPostCn);

