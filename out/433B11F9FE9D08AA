function setImageSize() {
    var nMaxWidth;
    if (82 == 80){
	nMaxWidth = 930;
	}
	else{
	nMaxWidth = 700;
	}
	var arEls = document.getElementsByTagName("IMG");
	for( var i=0;i<arEls.length;i++ ){
		if( arEls[i].width >nMaxWidth ) {	//如果图片宽度大于nMaxWidth，则等比缩小显示
			var nHeight = (arEls[i].height * (nMaxWidth/arEls[i].width));
			arEls[i].height = nHeight;
			arEls[i].width  =	nMaxWidth;
			arEls[i].style.height = nHeight;
			arEls[i].style.width = nMaxWidth;
		}
	}
}
function doMailTo() {
	var	fmt	= document.forms["mailto"];
	if (fmt != null) {
		fmt.title.value	= document.title;
		fmt.url.value	= this.location;
		fmt.submit();
	}
}

function checkReply(form) {
    var mode = window.frames['Editor'].document.getElementById('ubb').value; 
    var nmessage = window.frames['Editor'].document.getElementById('posteditor_textarea');
	var str ="";

	if(mode == 0){
	   if(nmessage.value==""){
			str = "请输入回复的内容！";	   
		    alert(str);
			nmessage.focus();
	   		return false;
		} else if(nmessage.value.length > 5000){
			str = "回复内容的长度不能超过"+5000+"个字，请您拆分后再上帖。";	  		
			alert(str);
			nmessage.focus();
	   		return false;
		}
		   
	   }
	  
	 if(mode == 1){
		var content = window.frames['Editor'].editdoc.body.innerHTML;
	    if(content == "" || content == "<br>"){
		    str = "请输入回复的内容！";
		    alert(str);
		    window.frames['Editor'].editdoc.body.focus();
	   		return false;
		}
		else if(window.frames['Editor'].editdoc.body.innerHTML.length > 5000){
		  str = "回复内容的长度不能超过"+5000+"个字，请您拆分后再上帖。";			
		alert(str);
		window.frames['Editor'].editdoc.body.focus();
	   		return false;
		   }
	 }
	if (form.userid != null) {
		if(form.userid.value=="") {
		    str = "你好像忘了输入用户名啦！";		
			alert(str);
			form.userid.focus();
	   		return false;
		}
	}
	if (form.password != null) {
		if(form.password.value == "") {
		    str = "你好像忘了输入用户密码啦！";		
			alert(str);
			form.password.focus();
	   		return false;
		}
	}
	
		if (form.username != null) {
		if(form.username.value=="") {
		    str = "你好像忘了输入真实姓名啦！";		
			alert(str);
			form.username.focus();
	   		return false;
		}
	}
	if (form.usertel != null) {
		if(form.usertel.value == "") {
		    str = "你好像忘了输入联系电话啦！";		
			alert(str);
			form.usertel.focus();
	   		return false;
		}
	}
	if (form.useraddress != null) {
		if(form.useraddress.value == "") {
		    str = "你好像忘了输入通讯地址啦！";		
			alert(str);
			form.useraddress.focus();
	   		return false;
		}
	}
	if (form.userzip != null) {
		if(form.userzip.value == "") {
		    str = "你好像忘了输入邮编啦！";		
			alert(str);
			form.userzip.focus();
	   		return false;
		}
	}
	if (form.userjob != null) {
		if(form.userjob.value == "") {
		    str = "你好像忘了输入身份证号啦！";		
			alert(str);
			form.userjob.focus();
	   		return false;
		}
	}
	
	var result = true;
	//检测描述字数
	$.each($("textarea[name=descp]"),function(index,obj){
		result = result && checkDescpCount(obj);
	});
	
	if(!result){
		alert("描述字数超过200，请检查");
		return false;
	}

	return true;
}

//描述字数限制
var descpLimit = 200;

function checkDescpCount(obj){
	var len = obj.value.replace(/^\s+|\s+$/g, '').length; 
	var tipsObj = $(obj).parent().next().children(":first");
	if(len>descpLimit){
		tipsObj.html("当前"+len+"个字，<font color=red>描述不能超过"+descpLimit+"字。</font>");
		return false;
	}
	tipsObj.html("当前"+len+"个字");
	return true;
}

function hello() {
		mydate = new Date();
		myhour = mydate.getHours();
		if (myhour < 6)
			welcomestring = "凌晨好";
		else if (myhour < 9)
			welcomestring = "早上好";
		else if (myhour < 12)
			welcomestring = "上午好";
		else if (myhour < 14)
			welcomestring = "中午好";
		else if (myhour < 17)
			welcomestring = "下午好";
		else if (myhour < 19)
			welcomestring = "傍晚好";
		else if (myhour < 22)
			welcomestring = "晚上好";
		else
			welcomestring = "夜里好";
		return welcomestring;
	}

	function checkLogin(loginform) {
		if (loginform.userid.value == "") {
			alert("你好像忘了输入用户名啦！");
			loginform.userid.focus();
			return false;
		}
		if (loginform.password.value == "") {
			alert("你好像忘了输入用户密码啦！");
			loginform.password.focus();
			return false;
		}
		return true;
	}
	
	function get(id){
	return document.getElementById(id);
}

	//添加 引用
	function addQuote(id){
	var mode = window.frames['Editor'].document.getElementById('ubb').value;
//		var oauthor = get('author_'+id);
		//var au_tm = get('mess_'+id);
		var au_tm = $.trim($("#mess_"+id).text()).replace(/\s+/g, ' ');
		
//		var otime = get('time_'+id);
		var omessage = get('message_'+id);
		var str=omessage.outerHTML;
        var t1=str.indexOf("<DIV class=BBS_QUOTE><Q>");
              if(t1 > -1)
            {

                 var reg1=new RegExp("<DIV id=message_"+ id +"><DIV class=BBS_QUOTE><Q>","g");
                 var reg2=new RegExp("<DIV class=BBS_QUOTE><Q>","g");
                 var reg3=new RegExp("<I>","g"); 
                 var reg4=new RegExp("</I>","g");
                 var reg5=new RegExp("</Q></DIV><BR>","g");
                 var reg6=new RegExp("</Q>","g");
                 var reg7=new RegExp("<BR>","g");
                 var reg8=new RegExp("</DIV>","g");
                 var temp1=str.replace(reg1,"[qt]");
                 var temp2=temp1.replace(reg2,"[qt]");
                 var temp3=temp2.replace(reg3,"[I]");
                 var temp4=temp3.replace(reg4,"[/I]");
                 var temp5=temp4.replace(reg5,"[/qt]");
                 var temp6=temp5.replace(reg6,"[/qt]");
                 var temp7=temp6.replace(reg7," ");
                 var temp8=temp7.replace(reg8," ");
                // alert(temp8);
             }
		//var nmessage = get('content');
		var nmessage = window.frames['Editor'].document.getElementById('posteditor_textarea');
		var tempmess = "";

		tempmess+='[qt]';
		if(mode == 1){
		   tempmess+='引自:<i>'+au_tm+'</i>';
		}
		else{
		  tempmess+='引自:[i]'+au_tm+'[/i]';
		}
		//tempmess+='[i]'+oauthor.innerText+'[/i]';
		//tempmess+=' 于 '+otime.innerText+' 发表:';
         if(t1 > -1){
		tempmess+="\n"+temp8;
          }
         else{
                tempmess+="\n"+$("#message_"+id).text().replace(/\s+/g, ' ');
          } 
		tempmess+='[/qt]';
		try{
		    if(mode == 0){
			//nmessage.innerHTML = tempmess;
		    	window.frames['Editor'].newEditor(mode, tempmess);
				document.getElementById("Editor").contentDocument.getElementById("posteditor_textarea").focus();
			}
			else if(mode == 1){
			window.frames['Editor'].editdoc.body.innerHTML = tempmess;
			}
		}catch(e){}
		return false;
	}
	
	//添加回复
	function addRevert(id){
	    var mode = window.frames['Editor'].document.getElementById('ubb').value;
		//var oauthor = get('author_'+id).innerText;
		var oauthor=$("#author_"+id).text();
		var nmessage = window.frames['Editor'].document.getElementById('posteditor_textarea');
		var tempmess = "";
		if(mode == 1){
/* 			if(id == 1){
				tempmess+='<b>回复'+'首贴 '+oauthor+'的帖子</b>';
			}else{
		    	tempmess+='<b>回复'+id+'楼'+oauthor+'的帖子</b>';
		    } */
		    tempmess+='<b>回复'+oauthor+'的帖子</b>';
		}
	    else{
/* 	    	if(id == 1){
	    		tempmess+='[b]回复'+'首贴 '+oauthor+'的帖子[/b]';
	    	}else{
		    	tempmess+='[b]回复'+id+'楼'+oauthor+'的帖子[/b]';
		    } */
		    tempmess+='[b]回复'+oauthor+'的帖子[/b]'
		}
		try{
		    if(mode == 0){
				//nmessage.innerHTML = tempmess;
				window.frames['Editor'].newEditor(mode, tempmess);
				document.getElementById("Editor").contentDocument.getElementById("posteditor_textarea").focus();
			}
			else if(mode == 1){
			window.frames['Editor'].editdoc.body.innerHTML = tempmess;
			}
		}catch(e){}
		return false;
	}
function ubbaction(tag, val){
	var tag = tag.toUpperCase();
	if(typeof(val) == "undefined"){
 	val = "";
	}
	if(val){
 	val = "=" + val;
	}

	var r = document.selection.createRange().text;
	rr = "[" + tag + val + "]" + r + "[/" + tag +"]";
	if(r){
 	document.selection.createRange().text = rr;
	}else{
 	document.all.content.value += rr;
	}
	}
 function countTitleChars(){
		document.getElementById("title_num_leave").innerHTML = 50-document.bbsedit.title.value.length;
	}
	
	function Cswf()
{		
	document.bbsedit.content.focus();
		 //var enterSIZE   = prompt("输入Flash尺寸（宽，高）！", "WIDTH=300 HEIGHT=250");
		var enterURL   = prompt("请输入Flash网址", "http://");
		var ToAdd="";
		if (enterURL)
		{		 
			ToAdd = "[FLASH=400,300]"+enterURL;	
			InsertText(objActive,ReplaceText(objActive,ToAdd,'[/FLASH]'),true); 				
	}
}
function Curl()
{
	document.bbsedit.content.focus();
	var FoundErrors = '';
	var enterURL   = prompt("输入链接网址！", "http://");
	
	if (!enterURL)
	{
		FoundErrors += "\n" + "链接网址不能为空";
	}
	
	if (FoundErrors)
	{
		alert("错误！"+FoundErrors);
		return;
	}
	str = "[URL="+enterURL+"]";
	InsertText(objActive,ReplaceText(objActive,str,'[/URL]'),true); 
}
function Cswf() {
	document.bbsedit.content.focus();
	//var enterSIZE   = prompt("输入Flash尺寸（宽，高）！", "WIDTH=300 HEIGHT=250");
	var enterURL   = prompt("请输入Flash网址", "http://");
	var ToAdd="";
	if (enterURL) {
		ToAdd = "[FLASH=400,300]"+enterURL;
		InsertText(objActive,ReplaceText(objActive,ToAdd,'[/FLASH]'),true); 
	}
}
function Cinsertimage()
{
	document.bbsedit.content.focus();
	var enterURL   = prompt("请输入图片网址", "http://");
	var ToAdd="";
	if (enterURL)
	{
		ToAdd = " [IMG]"+enterURL;
		InsertText(objActive,ReplaceText(objActive,ToAdd,'[/IMG]'),true); 
	}
}
var objActive;
function GetActiveText(objHTML) {
	objActive=objHTML;
	if(document.selection){
		var obj=document.getElementById(objHTML);
		obj.currPos = document.selection.createRange().duplicate();
	}
}
function InsertText(objHTML,strText,bolReplace) {
	if (strText=="") {
		return "";
	}
	var obj=document.getElementById(objHTML);
	if (!obj) {
		return "";
	}
	if(document.selection){
		if (obj.currPos){
			if(bolReplace && (obj.value=="")){
				obj.currPos.text=strText;
			}
			else{
				obj.currPos.text+=strText;
			}
		}
		else{
			obj.value+=strText;
		}
	}
	else{
		if(bolReplace){
			obj.value=obj.value.slice(0,obj.selectionStart) + strText + obj.value.slice(obj.selectionEnd,obj.value.length);
		}
		else{
			obj.value=obj.value.slice(0,obj.selectionStart) + strText + obj.value.slice(obj.selectionStart,obj.value.length);
		}
	}
}
function ReplaceText(objHTML,strPrevious,strNext) {
	var obj=document.getElementById(objHTML);
	if (!obj) {
		return "";
	}
	var strText;
	if(document.selection && document.selection.type == "Text"){
		if (obj.currPos){
			var range = document.selection.createRange();
			range.text = strPrevious + range.text + strNext;
			return "";
		}
		else{
			strText=strPrevious + strNext;
			return strText;
		}
	}
	else{
		if(obj.selectionStart || obj.selectionEnd){
			strText=strPrevious + obj.value.slice(obj.selectionStart,obj.selectionEnd) + strNext;
			return strText;
		}
		else{
			strText=strPrevious + strNext;
			return strText;
		}
	}
}

function callback(id){
		MI.CallBack.init(this, function(data) {
		if (data.result == 200) {
			UI.getScript("http://t.home.news.cn/followmore.action?typeId=3&checkdel="+id+"&rd="+Math.random());
		} else if (data.result == 0) {
			//alert(data.msg);
			alert("关注成功");
		}
		}, function(data) {
		if (data.result == 201) {
			//隐藏登录框
			MI.dialog.hide();
			//alert(data.msg);
			alert("登陆失败");
		} else if (data.result == 1) {
			if(data.msg=="noRegister"){
				alert('您还没有开通新华微博，现在就去开通吗？');
				var win=window.open('','_blank');
				win.location.href="http://t.home.news.cn/index.jsp";
			}else if(data.msg=="userNotExist"){
				alert('您关注用户还未开通新华微博，通知TA一下吧？');
				var win=window.open('','_blank');
				win.location.href="http://my.home.news.cn/qm/control/quickMsgFormView.do?stype=1&content="+encodeURIComponent("我已经开通新华微博了，你也来吧！")+"http://t.home.news.cn/index.jsp &userName="+id;
			}else{
				alert("关注失败");
			}
		}
		});
}
function follow(id){
	callback(id);
	if(id!=''){
		UI.getScript("http://t.home.news.cn/followmore.action?typeId=3&checkdel="+id+"&rd="+Math.random());
	}
}

function dataLength(fData){   
    var intLength=0   
    for (var i=0;i<fData.length;i++) {   
        if ((fData.charCodeAt(i) < 0) || (fData.charCodeAt(i) > 255))   
            intLength=intLength+2   
        else  
            intLength=intLength+1       
    }   
    return intLength   
}
