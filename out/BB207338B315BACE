<script>
function ybbsup(data){
	try{
		if (/\.swf$/i.test(data.show) == false){
			jQuery("#ad_up").html('<a href="'+data.href+'" target="_blank"><img src="'+data.show+'" ></a>');
		}
		else{
			var objFlash = new sinaFlash(data.show, "", "585", "90", "7", "", false,"high");
			objFlash.addParam("wmode", "opaque");
			objFlash.write("ad_up");
		}
	}catch (e){
	}
}
function ybbsmid(data){
	try{
		if (/\.swf$/i.test(data.show) == false){
			jQuery("#ad_mid").html('<a href="'+data.href+'" target="_blank"><img src="'+data.show+'" width="300" height="31"></a>');
		}
		else{
			var objFlash = new sinaFlash(data.show, "", "300", "31", "7", "", false,"high");
			objFlash.addParam("wmode", "opaque");
			objFlash.write("ad_mid");
		}
	}catch (e){
	}
}
function ybbsdown(data){
	try{
		if (/\.swf$/i.test(data.show) == false){
			jQuery("#ad_down").html('<a href="'+data.href+'" target="_blank"><img src="'+data.show+'" width="950" height="90"></a>');
		}
		else{
			var objFlash = new sinaFlash(data.show, "", "950", "90", "7", "", false,"high");
			objFlash.addParam("wmode", "opaque");
			objFlash.write("ad_down");
		}
	}catch (e){
	}
}

