	jQuery("#change_nickname_dialog").dialog({ autoOpen: false,modal: true });
	jQuery('#change_nickname_dialog').bind('dialogopen', function(event, ui) {
		jQuery("select").hide();
	});
	jQuery('#change_nickname_dialog').bind('dialogclose', function(event, ui) {
		jQuery("select").show();
	});
	$$('set_nickname_form_submit').onclick = function() {if( validate($$('postform'))) {$$('postform').submit();}};//将昵称确认按钮事件绑定到回帖上
		<!-- 		jQuery("#select_login_user_checkbox").attr("checked", true);
		 -->
    //昵称修改end
});
</script>
