			if(jQuery(this).attr("data-change") == "0") {
				jQuery("." + o).hide().eq(1).show();
				jQuery(this).attr("data-change", "1");
				if (o == "doclist_show") {
					jQuery(".bfd_a .bfd_img_logo").css("visibility", "visible");
				}
			} else {
				jQuery("." + o).hide().eq(0).show();
				jQuery(this).attr("data-change", "0");
				if (o == "doclist_show") {
					jQuery(".bfd_a .bfd_img_logo").css("visibility", "hidden");
				}
			}
		});
