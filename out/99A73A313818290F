		function getBrz(obj, type){
			if (!obj.data("brzdata")) {
			    jQuery.getJSON("http://webbiz.xici.net/biz.asp?from=doc&sid=1554ECE87A39423C&tags=互联网 安全生产 职业卫生 CDC 职业健康 @1332824@&clubid=2423&jsoncallback=?", function (data) {
			        obj.html(data.Addocsidebar).data("brzdata", data).show();
			        if (type == "another") {
			            jQuery("#tag_li_zt").show();
			            obj.next().hide();
			        } else {
			            if(jQuery("#Adygdoc")){
			                jQuery("#Adygdoc").html(data.Adygdoc);
			            }
			            jQuery("#Adygdoc1").html(data.Adygdoc1);
			            jQuery("#middoc").html(data.middoc);
			        }
