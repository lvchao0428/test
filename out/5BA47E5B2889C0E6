						//创建marker
						baiduMap.addMarker = function (lat,lng){
							//标注点数组
							var markerArr = [{title:"",content:"",isOpen:0,icon:{w:21,h:21,l:0,t:0,x:6,lb:5}}];
							for(var i=0;i<markerArr.length;i++){
								var json = markerArr[i];
								var point = new BMap.Point(lat,lng);
								var iconImg = baiduMap.createIcon(json.icon);
								var marker = new BMap.Marker(point,{icon:iconImg});
								var iw = baiduMap.createInfoWindow(i);
								var label = new BMap.Label(json.title,{"offset":new BMap.Size(json.icon.lb-json.icon.x+10,-20)});
								marker.setLabel(label);
								map.addOverlay(marker);
								label.setStyle({
									borderColor:"#808080",
									color:"#333",
									cursor:"pointer"
								});
							}
						}
						//创建InfoWindow
						baiduMap.createInfoWindow = function (i){
							var markerArr = [{title:"",content:"",isOpen:0,icon:{w:21,h:21,l:0,t:0,x:6,lb:5}}];
							var json = markerArr[i];
							var iw = new BMap.InfoWindow("<b class='iw_poi_title' title='" + json.title + "'>" + json.title + "</b><div class='iw_poi_content'>"+json.content+"</div>");
							return iw;
						};
						//创建和初始化地图函数：
						if(!isNaN(data.Result.geo.lng) && !isNaN(data.Result.geo.lat)){
							baiduMap.createMap(data.Result.geo.lng,data.Result.geo.lat,15);//创建地图
							baiduMap.setMapEvent();//设置地图事件
							baiduMap.addMarker(data.Result.geo.lng,data.Result.geo.lat);//向地图中添加marker
							//baiduMap.mapSign();
						} else {
							jQuery('.store_box').html(huajiaHtml.error);
						}
					} else {
						jQuery('.store_box').html(huajiaHtml.error);
					}
				}
			});
