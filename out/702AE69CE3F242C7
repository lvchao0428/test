<script>IE6 = '';</script><!--[if lte IE 6]><script>IE6 = 'sucks';</script><![endif]-->
<script>
    $(function(){
        if($("#lpub_ad")[0])
        {
            function findPos_top(obj){var top = obj.offsetTop || 0;while (obj = obj.offsetParent){top += obj.offsetTop;}return top;}
            function findPos_left(obj){var l=obj.offsetLeft;while(obj.offsetParent != null){obj = obj.offsetParent;l += obj.offsetLeft;}return l;}
            ifSideSticky = (IE6 == 'sucks') ? false : true;
            var pubPos=findPos_top($("#bbs_banner")[0]) - findPos_top($("#sidenav_bbs")[0]) + 496;
            $("#lpub_ad").css('top',pubPos+'px');
            $("#lpub_ad").show();
            if(!isMobile() && ifSideSticky  && document.documentElement.clientHeight > 670)
            {
                var contentwitdth = $("#content").width();
                var sideSticky = function() {
                    var now_top=  document.body.scrollTop || document.documentElement.scrollTop || 0;
                    if(now_top > pubPos + 100){
                        $("#lpub_ad").attr('style','position: fixed; _position: absolute; top:10px;left:'+(findPos_left($("#container_padd")[0])+13 - document.documentElement.scrollLeft)+'px');
                    }else{
                        $("#lpub_ad").removeAttr('style').css('left','-133px').css('top',pubPos+'px');	
                    }
                };
                $(window).bind('scroll', sideSticky).resize(function(){
                    $(window).unbind("scroll");
                    $("#lpub_ad").removeAttr('style').css('left','-133px').css('top',pubPos+'px');
                });
            }
        }
        if($("div .c2c_ad")[0])
        {
            if($(".level2header").attr('title'))
            {
                $("div .c2c_ad").css('top',"-66px").show();
            }else{
                $("div .c2c_ad").css('top','-114px').show();
            }
        }

    })

    /**
     * check is mobile device
     * @date 2015-02-26
     * @author zhangguichuan
     * @return bool
     */
    function isMobile() {
        var userAgent = navigator.userAgent.toLowerCase(),
                mobile = /Mobile/i.test( userAgent ) || 'ontouchstart' in document.documentElement;

        return mobile;
    }

</script>
