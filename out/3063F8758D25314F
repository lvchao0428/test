d8;border-top-color:#ece9d8;padding-bottom:0cm;padding-left:5.4pt;padding-right:5.4pt;padding-top:0cm;width:348.65pt;\">\r\n      <p style=\"line-height:14pt;margin:2.4pt 0cm 3.6pt;text-indent:19.85pt;\"><span\r\n      style=\"color:black;font-family:楷体_GB2312;\">鉴于天然气是关系民生的基础能源，其波动对<\/span><span\r\n      style=\"color:black;\"><font face=\"Arial\">GDP<\/font><\/span><span style=\"color:black;font-family:楷体_GB2312;\">等影响巨大，因此如何实现气价改革，特别是实现国内气价的市场化，相关政府部门十分慎重，绝不会轻易做出决定。<\/span><span\r\n      style=\"color:black;\"><\/span><\/p>\r\n      <\/td>\r\n    <\/tr>\r\n    <tr>\r\n      <td valign=\"top\" width=\"465\" style=\"background-color:transparent;border-bottom-color:#ece9d8;border-left-color:#ece9d8;border-right-color:#ece9d8;border-top-color:#ece9d8;padding-bottom:0cm;padding-left:5.4pt;padding-right:5.4pt;padding-top:0cm;width:348.65pt;\">\r\n      <p style=\"line-height:14pt;margin:2.4pt 0cm 3.6pt;text-indent:19.85pt;\"><span\r\n      style=\"color:red;font-family:楷体_GB2312;\">点评<\/span><font face=\"Arial\"><span\r\n      style=\"color:red;\">:<\/span><span style=\"color:black;\"><span>&nbsp; <\/span><\/span><\/font><span\r\n      style=\"color:black;font-family:楷体_GB2312;\">中国天然气价格偏低，需要进行调整是肯定的。但从常理上讲，<\/span><span\r\n      style=\"color:black;\"><font face=\"Arial\">4<\/font><\/span><span style=\"color:black;font-family:楷体_GB2312;\">月也不可能调价。<\/span><span\r\n      style=\"color:black;\"><\/span><\/p>\r\n      <\/td>\r\n    <\/tr>\r\n  <\/tbody>\r\n<\/table>\r\n<\/p><\/div>\r\n","isAdminFlag":false,"isCanUpdate":false,"isDel":0,"isHide":0,"isShow":true,"keyThis":"","lDocModifiedCount":1,"LongDate":"13-03-26 09:17","online":false,"really_updated_at":"2013-03-26 09:17:31","ShortDate":"03-26 09:17","ShortDate_i":[],"Url":"/d184511362.u20683567.htm","UserID":20683567,"UserName":"桃儿_ha","UseVote":false,"v_isAdminDeleted":false,"v_isAppend":false,"v_isEditOwnDoc":false,"v_lDocModifiedPageNo":0,"v_lDocNo":15,"v_lPageCount":1.00000000000000000e+000,"v_lUserID":0,"v_lUserL