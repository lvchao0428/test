				prePageArr[i].title = "上一页(←键)";
			}
		}
		if (nextPageArr) {
			for (i = 0;i < nextPageArr.length; i++) {
				nextPageArr[i].title = "下一页(→键)";
			}
		}
		addListener(document.getElementsByName("DocLists")[0], "keydown", PreNextPageFunc);

		function getBdsharePic(str) {
			if(str.indexOf("files.xici.net")>-1 || str.indexOf("pan.xici.com") >-1){
				return str;
			}
		}

		var pic = [], bdText = "";
		if(document.getElementById("text_1_0")) {
			var nodes = document.getElementById("text_1_0").getElementsByTagName("img");
			if (nodes.length > 0) {
