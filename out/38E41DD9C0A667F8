        }
        
        function showLogin() {
        	var returnURL = encodeURIComponent(Topic.root + "/topic-" + Topic.topicId + ".html");
        	showMessage('<p>请登录后操作</p><div style="margin-top:10px;"><a href=' + BASECONFIG.UCROOT + '/passport/login.jsp?return=' + returnURL + '" style="color:#336797;" target="_blank">去登录/注册</a></div>');
        }
