    <script type="text/javascript">
	    var addingFlag = false;
	    function addFocus(focusId,focusName,name) {
	        if (addingFlag)
	            return;
	        addingFlag = true;
	        if("true"=="true"){
	            location.href = "http://my.pchouse.com.cn/login.jsp?return=http://bbs.pchouse.com.cn/topic.do?tid=1111445&pageNo=1";
	            return;
	        }
	        var jsonStr= eval( "({windowname:true,withCookie:true,act:'add',friendId:'" + focusId + "',name:'" + name +"',friendName:'" + focusName + "'})" );
	        $.post("http://bip.pchouse.com.cn/intf/focus.jsp",jsonStr,function(data){
	            data = eval( '(' + $.trim(data) + ')' );
	            var code = data.code;
	            if(typeof code != 'undefined'){
	                if(code == 0) {
	                    showInfo('关注成功!<div style="margin-top:10px;"><a href="http://my.pchouse.com.cn" style="color:#336797;" target="_blank">进入个人中心看看</a></div>');
	                    //   changeFanNum("add",focusId,1);
	                    changeFocusStatus(focusId);
	                    changeFocusNum("add",0,1);
	                } else if(code==-1){
	                    //showInfo(data.message);
	                    location.href = "http://my.pchouse.com.cn/login.jsp?return=http://bbs.pchouse.com.cn/topic.do?tid=1111445&pageNo=1";
	                    return;
	                } else {
	                    showInfo(data.message);
	                }
	            }
	            addingFlag = false;
	        });
	    }
	    //删除关注
	    function delFocus(focusId) {
	        var jsonStr= eval( "({windowname:true,withCookie:true,act:'delete',friendId:'" + focusId + "'})" );
	        $.post("http://bip.pchouse.com.cn/intf/focus.jsp?",jsonStr,function(data){
	            data = eval( '(' + $.trim(data) + ')' );
	            var code = data.code;
	            if(typeof code != 'undefined'){
	                if(code == 0) {
	                    //    showInfo("取消成功");
	                    //    changeFanNum("del",focusId,1);
	                    changeFocusStatus(focusId);
	                    changeFocusNum("del",0,1);
	                } else if(code==-1){
	                    //showInfo(data.message);
	                    location.href = "http://my.pchouse.com.cn/login.jsp?return=http://bbs.pchouse.com.cn/topic.do?tid=1111445&pageNo=1";
	                    return;
	                }
	            }
	        });
	    }
	    
	    /**
	     * 改变关注按钮状态
	     */
	    function changeFocusStatus(focusIds) {
	        $.getJSON(BASECONFIG.BIPROOT + "/intf/focus.jsp?callback=?",
	        {
	            "act":"getNum",
	            "accountIds":focusIds
	        },
	        function(data){
	            if(data.code == undefined) {
	                $.each(data,function(id,status) {
	                    changeFanNum("show",id,status.fsNum);
	                    changeFocusNum("show",id,status.fcNum);
	                    changeBtnStatus(id,status.status);
	                });
	            }
	        });
	    }
	    
	    /**
	     * 改变关注数目
	     * act 操作标志 add表示粉丝数+num，del粉丝数-num,show仅仅显示num
	     * id 用户id
	     * 类名follow+关注用户id
	     */
	    function changeFocusNum(act,id,num) {
	        if(act == "add") {
	            $(".follow" + id).text(parseInt($(".follow" + id).text()) + num);
	        } else if(act == "del") {
	            $(".follow" + id).text(parseInt($(".follow" + id).text()) - num);
	        } else if(act == "show") {
	            $(".follow" + id).text(num);
	        }
	    }
	    /**
	     * 改变粉丝数目
	     * act 操作标志 add表示粉丝数+num，del粉丝数-num,show仅仅显示num
	     * id 用户的id
	     * 类名fan+关注用户id
	     */
	    function changeFanNum(act,id,num) {
	        if(act == "add") {
	            $(".fan" + id).text(parseInt($(".fan" + id).text()) + num);
	        } else if(act == "del") {
	            $(".fan" + id).text(parseInt($(".fan" + id).text()) - num);
	        } else if(act == "show") {
	            $(".fan" + id).text(num);
	        }
	    }
	    
	    function changeBtnStatus(id,status) {
	        //0 没关系
	        //1 我关注的人
	        //2 我的粉丝
	        //3 互相关注
	        if(status == 2 || status == 3 ) {
	            $("#attendA" + id).hide();
	            $("#attendB" + id).show();
	        } else if(status == 0 || status == 1) {
	            $("#attendA" + id).css("display","inline-block");
	            $("#attendB" + id).hide();
	        }
	    }
    </script>
    


