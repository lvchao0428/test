    function btnSurePress(o,e){
        o.value=o.value.replace(/[^\d]/g,'');
        var pNum = o.value;
        if(pNum == null || pNum == '' || pNum == undefined || pNum == 0) {
            pNum = 1;
        }
        pNum = pNum> 1 ? 1 : pNum;
        if((window.event && e.keyCode == 13) || (e.which && e.which == 13)) {
            window.location =switchPage(pNum);
        }
    }
	function switchPage(pageNo){
		str = 'tid=1111445&pageNo=1';
		str = str.replace(/(^|&)pageNo=-?\d+/ig, '');
		str = (str != '' ? str + '&' : '') + 'pageNo=' + pageNo;
		return "http://bbs.pchouse.com.cn/topic.do?" + str;
	}

    function changeStatus(sendMsgStatus){
        var topicId = 1111445;
        $.ajax({
            url: "http://bbs.pchouse.com.cn/action/topic/change_sendmsg.ajax",
            type: "POST",
            data: {sendMsg : sendMsgStatus,tid:topicId},
            success : function(msg) {
                var data = eval("(" + msg + ")");
                if(data.status == 0){
                    if (sendMsgStatus == 'true') {
                        $("#cancelSendMsg").show();
                        $("#noticeSendMsg").hide();
                        $.cookie("sendMsgUncheceked",null,{path:'/'}); // 接收回复通知
                    } else {
                        $("#cancelSendMsg").hide();
                        $("#noticeSendMsg").show();
                        $.cookie("sendMsgUncheceked",true,{path:'/'}); // 取消回复通知
                    }
                }
            }
        });
    }

	// 操作 回复通知的状态
	(function () {
		//发帖是否给我消息，记录点击cookie
		if ($.cookie("sendMsgUncheceked")) { //  取消回复通知
			$("#cancelSendMsg").hide();
		    $("#noticeSendMsg").show();
		} else {
			 $("#cancelSendMsg").show(); 
		     $("#noticeSendMsg").hide();
		}
	})();



