	                showMsgObj.html(showSuccessMsg.replace('[:MSG]',"删除成功"));
	                tb_show_2('','#TB_inline?inlineId=showMsg&width=340',false);
	                tb_fadeOut(3000);
	                var add = "";
	                if(delResult.tid == 0){
	                    setTimeout("tb_redirect_forum()",2500);
	                }else{
	                    //setTimeout("tb_redirect()",2500);
	                    setTimeout("tb_redirect(window.location.href)",2500);
	                }
	            }else{
	                showMsgObj.html(showErrorMsg.replace('[:MSG]',delResult.desc));
	                tb_show_2('','#TB_inline?inlineId=showMsg&width=310',false);
	            }
	        });
	    }
	    )
	}
