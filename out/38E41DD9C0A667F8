        function postImgQuote() {
            var postMsg = $.trim($('#msgimgquote').val());
            var imgSrc = $('#msgimgquote').data('prev').prev().attr('src');
            if ($.trim(imgSrc) == '') {
            	showMessage("引用的图片不存在！");
                return false;
            }
            postMsg = '[imgquote]' + imgSrc + '[/imgquote]' + postMsg;
            postMsg = replaceUbbCode(postMsg);
            $('#msgHidden').val(postMsg);
            if ($("#imgQuoteForm").data('onPost') == 1) {
        		return;
        	}
            $('#imgQuoteForm').submit();
        }
        
