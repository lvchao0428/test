	var barTotalLen = 150;
	function displayForumUserInfo(data) {
		for(var n in data) {
			var obj = data[n];
			var userId = obj.userId;
			$(".user_forum_rank_" + userId).text(obj.level + "级");
			$(".user_forum_score_" + userId).text(obj.contribution + "/" + obj.next);
			var levelLen = (obj.next === 0 || obj.next === '') ?
                0 : obj.contribution*barTotalLen/obj.next;
			$(".user_bar_forum_" + userId).width(levelLen);
		}
	}

    function switchOpera(obj,pId){
	    if($(obj).hasClass("ico_sq")){
	        $(obj).removeClass("ico_sq").addClass("ico_zk");
	        $("#"+pId).find(".tb").eq(0).addClass("hidden");
	    }else{
	        $(obj).removeClass("ico_zk").addClass("ico_sq");
	        $("#"+pId).find(".tb").eq(0).removeClass("hidden");
	    }
	}

    function changeFavoriteStatus(topicId) {
        $.post('http://bbs.pchouse.com.cn/action/user/check_favorite.jsp',
            {tid:topicId},
            function(data){
                if($.trim(data)==0){
                    $("#add_favorite").hide();
                    $("#favorited").show();
                }else{
                    $("#add_favorite").show();
                }
            }
        );
    }

	//在线用户套橙色
	var DisplayOnline = (function () {
	    var my = {};
	    var unique = function(s){
	        if (!s && typeof s !== 'Array') return [];
	        var newArray=[];
	        var temp = {};
	        var item = {};
	        for (var i = 0 ; (item= s[i]) != null; i++) {
	            if (!temp[item]) {
	                newArray.push(item);
	                temp[item] = true;
	            }
	        }
	        return newArray;
	    }
	    my.displayOnline = function (ids) {
	        for (var i = 0; i < ids.length; i ++) {
	            $('.user-' + ids[i]).addClass("uOnline");
	        }
	    }
	    my.run = function () {
	        var onlineIds = [];
	        var re = /user-([\d]+).*/ig;
	        $('.needonline').each(function() {
	            onlineIds.push(this.className.replace(re,'$1'));
	        });
	        $.getScript("http://bbs.pchouse.com.cn/action/online_user_passport.do?cb=DisplayOnline.displayOnline&ids=" + unique(onlineIds).join(','));
	    }
	    return my ;
	}());
	DisplayOnline.run();

    function getArray(s){
        if (!s && typeof s !== 'Array') return [];
        var newArray=[];
        var temp = {};
        var item = {};
        for (var i = 0 ; (item= s[i]) != null; i++) {
            if (!temp[item]) {
                newArray.push(item);
                temp[item] = true;
            }
        }
        return newArray;
    }
	var userIdStr = getArray(userIds);
	$.getScript("http://bbs.pchouse.com.cn/util/user_score.jsp?callback=displayForumUserInfo&ids=" + userIdStr);
    var visitedfid = $.cookie("visitedfid");
    if (visitedfid == null || visitedfid == '') {
        visitedfid = '5';
    } else {
        var fids = visitedfid.split('D');
        for (var i = 0; i < fids.length; i++) {
            if(fids[i] == '5') {
				fids.splice(i, 1); break;
            }
        }
		fids.unshift('5');
		if (fids.length > 10) fids.pop()
		visitedfid = fids.join("D");
    }
    $.cookie('visitedfid', null);
    $.cookie('visitedfid', visitedfid, { expires: 100, domain: 'pchouse.com.cn'});
    
    function viewPost(pid) {
    	location.href="http://bbs.pchouse.com.cn/post-" + pid + ".html";
    }
    //自动加载图片
    function clickAutoLoadImg(on){
        if(on){
            $.cookie('autoloadimg','1',{path:'/',expires:30});
            var selected = $('#chkAutoLoadImg').attr('checked');
            if(!selected){
                $('#chkAutoLoadImg').attr('checked',true);
            }
        }else{
            $.cookie('autoloadimg','',{path:'/',expires:30});
        }
    }
    (function(){
        var autoloadimg = !!$.cookie('autoloadimg');
        $('#chkAutoLoadImg').attr('checked',autoloadimg);
      //重置引用部分大图的尺寸
        imgDementionLimit($('span.cite img,div.imgQuoteWrap img'));
    })();
    //献花
    function showflower(page_index,jq){
        $("#floweritems tr").hide();
        $("#floweritems tr").slice(page_index*this.items_per_page,page_index*this.items_per_page+this.items_per_page).show();
        return false;
    }
    var flower_perpage = 10;
    var flower_opt = {callback: showflower,items_per_page: flower_perpage};
    var flower_num = $("#floweritems tr").length;
    if(flower_num>flower_perpage){
        $.getScript("http://js.3conline.com/bbs/pclady/2013/js/jquery.pagination.js",function(){
            $('<div class="floserpager" id="floserpager"></div>').appendTo($(".flowerlist"));
            $("#floserpager").pagination(flower_num, flower_opt);
        });
    }else{
    	$("#floweritems tr").show();
    }
</script>


