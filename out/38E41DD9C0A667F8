    <script>
        setTimeout(function(){
            var $imgSpans = $(".post_msg_wrap .post_main-pic");
            for (var i = 0; i < $imgSpans.length; i++) {
            	var $imgSpan = $imgSpans.eq(i);
                var _wid = $imgSpan.find('img').css('width');
                $imgSpan.css('width', _wid);
                if (!$imgSpan.parents('.hidecont').length) {
                    $imgSpan.addClass('canReply');
                }
            }

            $imgSpans.hover(function() {
            	var $img = $(this).find('img');
            	if (!$(this).hasClass('canReply') || !$img.attr('complete') || $img.width() < 300 || $img.height() < 100) {
            		return false;
            	}
                $(this).find('.reply_img-btn').css('display','block');
            }, function() {
            	var $img = $(this).find('img');
            	if (!$(this).hasClass('canReply') || !$img.attr('complete') || $img.width() < 300) {
            		return false;
            	}
            	var $reply = $(this).find('.reply_img-btn');
            	$reply.css('display','none');
                if ($reply.attr('clicked') == 'true') {
                	$reply.css('display', 'block');
                }
            })
        }, 500);
        
        $('#msgimgquote').keypress(function (e) {
            if (e.ctrlKey && e.which == 13 || e.which == 10) {
            	postImgQuote();
            }
        });


        $(".post_msg_wrap .post_main-pic .reply_img-btn").click(function() {
        	if (BASECONFIG.USERID == 0) {
        		return showLogin() && false;
        	}
        	var $this = $(this);
        	var $imgQuoteMsg = $('#msgimgquote');
        	var $prevReply = $imgQuoteMsg.data('prev');
        	
        	if ($prevReply && $prevReply[0]) {
        		$prevReply.data('message', $imgQuoteMsg.val());
        	}
        	
        	$imgQuoteMsg.data('prev', $this);
        	$imgQuoteMsg.val($.trim($this.data('message') || $imgQuoteMsg[0].defaultValue));
        	
            // $(this).siblings('a').css('display','none');
            
            if ($this.attr('clicked') == 'true') {
            	$this.attr('clicked','false');
                $('#boxReplyAdd').css({
                    'display' : 'none'
                });
            } else{
                $('.post_msg_wrap .post_main-pic .reply_img-btn').css('display','none').attr('clicked','false');
                $(this).css('display','block').attr('clicked','true');
                var offset = $(this).offset();
                var offsetLeft = offset.left;
                var offsetTop = offset.top;
                var top = parseInt(offsetTop) + 42;
                var left = parseInt(offsetLeft) - 620;
                $('#boxReplyAdd').css({
                    'display' : 'block',
                    'left' : $('.post_main:eq(0)').offset().left,
                    'top' : top
                });
                var insLeft = offsetLeft - 505;
                $('#insAdd').css('left', insLeft);
            }
        });
        $("#imgQuoteForm").ajaxForm({
            beforeSubmit : function showFastRequest(formData,jqForm,options) {
                var $imgQuoteMsg = $('#msgimgquote');
                var postMsg = $.trim($imgQuoteMsg.val());
                if (postMsg === '' || postMsg == $('#msgimgquote')[0].defaultValue){
                    showMessage("评论内容不允许为空！");
                    return false;
                }
                $("#imgQuoteForm").data('onPost', 1);
    	    },
            success : function (responseText,statusText) {
                var fastResult = new Function("return ("+responseText+");")();
                if (fastResult.status == 0) {
                	var rapitSubmit = $("#rapitSubmit").attr('checked');
                    if (rapitSubmit) {
                        $.cookie("topic_action", "reply");
                        var redirectUrl = "http://bbs.pchouse.com.cn/topic-" + fastResult.tid + "--1.html#post_" + fastResult.pid;
                        redirectUrl = encodeURIComponent(redirectUrl);
                        window.location.href="http://bbs.pchouse.com.cn/redirect.do?url=" + redirectUrl;
                    } else {
                        $.cookie("topic_action", "reply");
                        var request = $.ajax({
                            url: "http://bbs.pchouse.com.cn/reply_floor.do",
                            type: "POST",
                            data: {tid:fastResult.tid,pid:fastResult.pid},
                            success : function(msg){
                            	var $msgImgQuote = $('#msgimgquote');
                                $msgImgQuote.val('');
                                $msgImgQuote.data('prev').click();
                                var $msg = $(msg);
                                imgDementionLimit($msg.find('div.imgQuoteWrap img'));
                                $(".psot_wrap_first").after($msg);
                                $body = (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') : $('body')) : $('html,body');
                                $body.animate({scrollTop: $("#post_"+fastResult.pid).offset().top}, 500);
                                bindEvent();
                                //调用sns接口(计算小家打工赚钱的发帖任务)
                                //postSNSURL('0','1111445','积极研发新产品圣菲特4D多彩集成墙饰缔造大阵容')
                            }
                        });
                    }
                } else {
                    showMessage(fastResult.desc);
                }
                $("#imgQuoteForm").data('onPost', 0);
            }
        });
        // 屏蔽点击图片评论按钮触发外层a标签
    	$('.post_first .replyBody a').each(function(index, elem) {
        	$(elem).bind('click', function(e) {
