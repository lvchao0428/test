    <script>
        jQuery('#comment_tab1').click(function () {         
            if (jQuery("#comment_con1").css("display") != "none")
            {
                jQuery("#comment_con1").css("display", "none");
            } 
            else 
            {
                jQuery("#comment_con1").css("display", "");
            }
            
        });
        jQuery('#comment_cancel1').click(function () {           
            if (jQuery("#comment_con1").css("display") != "none")
            {
                jQuery("#comment_con1").css("display", "none");
            } 
            else 
            {
                jQuery("#comment_con1").css("display", "");
            }
            
        });
        //提交评论
        function commentCommit1(askid, answerid, userid, username) {
            jQuery('#comment_tip1').css("display", "none");
            var text = jQuery.trim(jQuery('#comment_text1').val());

            if (text.length <= 0) {
                jQuery('#comment_tip1').text("请您输入文字内容");
                jQuery('#comment_tip1').css("display", "");
                return;
            }
            if (text.length > 199) {
                jQuery('#comment_tip1').text("输入内容不能超过199字");
                jQuery('#comment_tip1').css("display", "");
                return;
            }

            jQuery.ajax({
                url: '/Ask/Ajax/SubmitComment.aspx',
                type: 'POST',
                dataType: 'text',
                contentType: "application/x-www-form-urlencoded; charset=utf-8",
                data: { AskId: askid, AnswerId: answerid, Content: text },
                error: function(data) {
                    alert('提交评论失败！' + data.statusText);
                },
                success: function(data) {
                    var result = eval("(" + data + ")");
                    if (result.success == "true") {
                        jQuery('#comment_text1').val('');
                        var time = new Date();
                        var timeStr = time.getFullYear() + '-' + time.getMonth() + '-' + time.getDay() + ' ' + time.getHours() + ':' + time.getMinutes();
                        jQuery('#comment_list2').prepend('<div class="comment-entry">\
                            <div class="details">\
                                <a target="_blank" href="usercenter.aspx?id=' + userid + '">\
                                    ' + username + '</a> <span class="times">\
                                        ' + timeStr + '</span>\
                            </div>\
                            <div class="comment-content">\
                                <div class="comment-word">\
                                    ' + text + '</div>\
                            </div>\
                        </div>');
                    }
                    else {
                        alert(result.message);
                    }
                }
            });
            var Ask = window.Ask = {};
            jQuery.extend(Ask, {
                //匹配内容字数
                CheckWordNumberNew: function (min, max, input, word) {
                    var length = input.val().length;
                    if (length != 0) {

                        if (length <= max) {
                            word.html(max - length);

                            word.css("color", "#888888");
                            //word.parents("span").html("您还可以输入");
                            word.parents("span").next("span").html("您还可以输入");
                        }
                        else if (length > max) {
                            word.html(length - max);

                            word.css("color", "#ff0000");
                            //word.parents("span").html("已超出");
                            word.parents("span").next("span").html("已超出");
                        }

                    }
                    else {
                        word.html(max);
                        word.css("color", "#888888");
                        word.prev("span").html("您还可以输入");
                    }
                }
            })
        };
        //回复评论
        function commentReply(commentId) {
            var username = jQuery.trim(jQuery('#comment_detail_' + commentId).text());
            jQuery('#comment_text1').val('回复 ' + username + '：');
            jQuery('#comment_text1').focus();
        };
        

    </script>
    <script type="text/javascript">
        //顶一下       
        function funding2(intn) {
            try {
                cding = document.getElementById("ding" + intn);
                var currentUserId = '0';
                //if (currentUserId == '0') {
                //    alert("用户未登录");
                //}
                if (currentUserId >= 0) {
                    if (document.cookies.get("answer_dingju_" + intn) != undefined || document.cookies.get("answer_caiju_" + intn) != undefined) {

                        alert("您已经投过票了");
                        $("answer_ding_" + intn).style.display = "";
                    }
                    else {
                        _ub.city = '北京'; //所在城市（中文）
                        _ub.biz = 'a'; //固定值，问答
                        _ub.location = '0'; //方位 ，网通为0，电信为1，如果无法获取方位，记录0
                        _ub.collect(55,{'ad':intn,'ac':'12562061','ab':encodeURIComponent('装修中')});//例如_ub.collect(0,{'ac':'12345','ab':'房屋买卖'}) 

                        if (!cding.isup) {
                            cding.innerHTML = parseInt(cding.innerHTML) + 1;
                            cding.isup = true;
                            jQuery.ajax({
                                url: '/ask/AJAX/Comment.aspx/Ding',
                                type: 'post',
                                dataType: 'json',
                                contentType: "application/json",
                                data: Common.jsonToString({ answerId: intn }),
                                success: function (data) {

                                }
                            });
                            //jQuery.ajaxWebService("/ask/AJAX/Comment.aspx/Ding", { answerId: intn }, function (data) { });
                            BindDing(intn, parseInt(cding.innerHTML) + 1);
                        }
                    }
                }
                else {
                    jQuery('#frmLogin').jqmShow();
                }
            } catch (err) { }
        }
        //踩一下
        function funcai2(intn) {
            try {
                ccc = document.getElementById("cai" + intn);
                cding = document.getElementById("ding" + intn);
                var currentUserId = '0';
                //if (currentUserId == '0') {
                //    alert("用户未登录");
                //}
                if (currentUserId >= 0) {
                    if (document.cookies.get("answer_caiju_" + intn) != undefined || document.cookies.get("answer_dingju_" + intn) != undefined) {
                        alert("您已经投过票了");
                        $("answer_cai_" + intn).style.display = "";
                    }
                    else {
                        _ub.city = '北京'; //所在城市（中文）
                        _ub.biz = 'a'; //固定值，问答
                        _ub.location = '0'; //方位 ，网通为0，电信为1，如果无法获取方位，记录0
                        _ub.collect(56,{'ad':intn,'ac':'12562061','ab':encodeURIComponent('装修中')});//例如_ub.collect(0,{'ac':'12345','ab':'房屋买卖'}) 

                        if (!cding.isup) {
                            ccc.innerHTML = parseInt(ccc.innerHTML) + 1;
                            cding.isup = true;
                            jQuery.ajax({
                                url: '/ask/AJAX/Comment.aspx/Cai',
                                type: 'post',
                                dataType: 'json',
                                contentType: "application/json",
                                data: Common.jsonToString({ answerId: intn }),
                                success: function (data) {
                                }
                            });
                            //jQuery.ajaxWebService("/ask/AJAX/Comment.aspx/Cai", { answerId: intn }, function (data) { });
                            BindCai(intn, parseInt(ccc.innerHTML) + 1);
                        }
                    }
                }
                else {
                    jQuery('#frmLogin').jqmShow();
                }
            } catch (err) { }
        }

        function BindDing(intn, n) {
            document.cookies.set("answer_dingju_" + intn, "answer_dingju_" + intn, 600 * 1000, null, "fang.com");
            jQuery("#dig_" + intn).val(jQuery("#ding" + intn).val());
        };
        function BindCai(intn, n) {
            document.cookies.set("answer_caiju_" + intn, "answer_caiju_" + intn, 600 * 1000, null, "fang.com");
            jQuery("#cig_" + intn).val(jQuery("#cai" + intn).val());
        };

    </script>

    <script>
        (function($) {
            $('.tab-wp .inner-tab').mouseover(function(event) {
                var isCurrent = $(this).hasClass('cur'),
                    parent = $(this).parent(),
                    innerTabs = $('.inner-tab', parent),
                    contentItems = parent.attr('data-related-content') == undefined ? 
                        $('.tab-content', parent.parent().next()) : $('.tab-content', parent.next()),
                    index = $(this).attr('data-index'),                   
                    contentPrefix = parent.attr('data-cotnentid-prefix'); 
                if(index==1)
                {
                    $('#Themore').attr('href','http://www.fang.com/ask/search.aspx?result=&city=&cid=1&s=0') 
                    if('2'==1)
                    {
                        $('#Themore2').attr('href','http://zhishi.fang.com/xf/');
                    }
                    if('2'==2)
                    {
                        $('#Themore2').attr('href','http://home.fang.com/zhishi/');
                    }
                }
                if(index==2)
                {
                    $('#Themore').attr('href','http://www.fang.com/ask/search.aspx?result=&city=&cid=1&s=3');
                    
                    $('#Themore2').attr('href','http://home.fangchenggang.fang.com/bbs/board/zxlt/');                  

                }
                if(index==3)
                {
                    $('#Themore').attr('href','http://www.fang.com/ask/search.aspx?result=&city=&cid=1&s=1');
                }           
                            
                if (!isCurrent) {
                    innerTabs.removeClass('cur');
                    $(this).addClass('cur');
			
                    contentItems.hide();
                    $('#' + contentPrefix + index).show();
                }
            });
        })(jQuery);
    </script>
    <script type="text/javascript">
        function showRelatedAsk() {
            if (document.getElementById("event_relatedask_change_show").show == "off" || document.getElementById("event_relatedask_change_show").show == undefined) {
                $(".event_relatedask_init_hide").show();
                document.getElementById("event_relatedask_change_show").show = "on";
                jQuery("#event_relatedask_change_show").attr("class", "btnStyle_23")
            }
            else {
                $(".event_relatedask_init_hide").hide();
                document.getElementById("event_relatedask_change_show").show = "off";
                jQuery("#event_relatedask_change_show").attr("class", "btnStyle_22")
            }
        }
</script>
