        <script type="text/javascript">
            function startmarquee(lh, speed, delay, index) {
                var t;
                var p = false;
                var o = document.getElementById("marqueebox" + index);
                o.innerHTML += o.innerHTML;
                o.onmouseover = function () { p = true; };
                o.onmouseout = function () { p = false; };
                o.scrollTop = 0;
                function start() {
                    t = setInterval(scrolling, speed);
                    if (!p) o.scrollTop += 2;
                }
                function scrolling() {
                    if (o.scrollTop % lh != 0) {
                        o.scrollTop += 2;
                        if (o.scrollTop >= o.scrollHeight / 2) o.scrollTop = 0;
                    } else {
                        clearInterval(t);
                        setTimeout(start, delay);
                    }
                }
                setTimeout(start, delay);
            }
            startmarquee(22, 50, 3000, 0);
            /**startmarquee(一次滚动高度,速度,停留时间,图层标记);**/

        </script>
        <script type="text/javascript">
            function moreBoxto(numcss1020) {
                for (j = 0; j < 10; j++) {
                    if (document.getElementById('moreBox' + j)) {
                        document.getElementById('moreBox' + j).style.display = 'none';
                    }
                }
                var id=$("#userid_"+numcss1020).val();
                var picurl=$("#picurl_"+numcss1020).val();
                var level=$("#level_"+numcss1020).val();
                var img=$("#img_"+numcss1020);               
                jQuery.ajax({
                    url:'/ask/AJAX/AgentUser.aspx/GetUserPicurl',
                    type: 'post',
                    dataType: 'json',
                    contentType: "application/json",
                    data: Common.jsonToString({ id: id,picurl: picurl, level: level }),
                    success:function(result){
                        data = eval('(' + result.d + ')');
                        img.attr('src',data);
                    }
                });          
                document.getElementById('moreBox' + numcss1020).style.display = 'block';
            }
            function hidden_moreBoxto(numcss1020) {
                timeout = setTimeout(function () { document.getElementById("moreBox" + numcss1020).style.display = "none"; }, 100);
            }
            function show_moreBoxto(numcss1020) {
               
                window.clearTimeout(timeout);
                document.getElementById("moreBox" + numcss1020).style.display = "";
            }
        </script>
        
        
