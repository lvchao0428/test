<script type="text/javascript">
    jQuery(function () {
        $('#search_btn_head').click(function () {
            $('#search_form_foot').submit();

        });
        $('#ask_btn_foot').click(function () {
            var asktitle = $.trim($('#search_text_head').val());
            if (asktitle == "请在这输入您的问题")
            { asktitle = "";}
            window.open("http://www.fang.com/ask/Ask_StepTwo.aspx?asktitle=" + escape(asktitle));
        });

        $('#search_text_head').focus(function () {
            jQuery(this).parents('.esf_s_box').addClass('focus');

        });
        $('#search_text_head').blur(function () {
            if ($.trim($(this).val()) == '')
                jQuery(this).parents('.esf_s_box').removeClass('focus');
        });
        if ($.trim($('#search_text_head').val()) != '') {
            jQuery('#search_text_head').parents('.esf_s_box').addClass('focus');
        }
        if ('' != '') {
            jQuery('#search_text_head').parents('.esf_s_box').addClass('focus');
            $('#search_text_head').val('');
        }
    });
</script>










