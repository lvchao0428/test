        //补充问题提交
        function addAskCommit() {
            jQuery('#askdetail_addask_tip').css("display", "none");
            var addContent = jQuery.trim(jQuery("#frmadd #askAdded").val());
            if(addContent.length<=0) {
                jQuery('#askdetail_addask_tip').text("请输入文字内容");
                jQuery('#askdetail_addask_tip').css("display", "");
                return false;
            }
            if(addContent.length<6) {
                jQuery('#askdetail_addask_tip').text("输入内容不能少于6字");
                jQuery('#askdetail_addask_tip').css("display", "");
                return false;
            }
            if(addContent.length>499) {
                jQuery('#askdetail_addask_tip').text("输入内容不能超过499字");
                jQuery('#askdetail_addask_tip').css("display", "");
                return false;
            }
            jQuery("#frmadd").submit();
            return true;
        }
        //提高问题的悬赏分
        function increaseAskScore() {
            var addScore = jQuery("#formAddAskScore #select").val();
            var totalScore =-305;            
            if(addScore>totalScore) {
                alert("财富值过低，暂不能操作！");
                return false;
            }
            
            var addNum='0';
            if(parseInt(addNum)>=2) {
                alert("已经提高悬赏2次，无法提高悬赏");
                return false;
            }

            jQuery("#formAddAskScore").submit();
            return true;
        }
    </script>

    <script type="text/javascript">
        //与举报相关
        function ShowLogin() 
        {
            jQuery("#frmLogin").jqmShow();
        }

        var complainAskId = '';
        var complainAnswerId = '';
        var complainUserId = '';
        jQuery(document).ready(function()
        {

            //设置标签最大高度
            
            if (2==2) 
            {
                var a=30;
                var maxTabs=a%4==0?a/4:a/4+1;
                document.getElementById("askxq_B02_53").style.height=maxTabs*41+"px";
            }
            $("#btnGetIdentityCode5").attr("disabled", false);
            var AnswerCount =0;
            if(parseInt( AnswerCount)>0)
            {
                jQuery('#askdetail_answer_con').hide();
            }
            else
            {
                jQuery('#askdetail_answer_con').show();
                GetVCode('imgcode');
            }
            jQuery('#jubao_box').jqm({ modal: true });
            jQuery('#jubao_box').css('opacity', 0.99);

            jQuery('#complain_submit').click(function() {
                var content = jQuery('#complain_content').text();
                var type = '';
                jQuery('#complain_type input').each(function() {
                    if (jQuery(this).attr("checked"))
                        type += jQuery(this).val();
                });
                if (type == '') {
                    alert("请选择举报类型");
                    return false;
                }
                if (complainAnswerId == '')
                    complainAnswerId = 0;

                jQuery.ajaxWebService('/ask/AJAX/Complain.aspx/ComplainSubmit', { type: type, content: content, askId: complainAskId, answerId: complainAnswerId, currentUserId: '0' }, function(data) {
                    var result = eval('(' + data.d + ')');
                    alert(result.message);
                    if (result.success == "true") {
                        jQuery("#complain_close").click();
                        return true;
                    }
                    else {
                        return false;
                    }
                }
            );
                return true;
            });
        });

        function showComplain(askId, answerId, userId)
        {
            if (askId == '')
                return false;
            var userName = "房天下网友";
            jQuery.ajaxWebService("/ask/AJAX/Complain.aspx/GetUserName", { userId: userId }, function(data) {
                if (data.d != '')
                    userName = data.d;
                if (answerId != '' && answerId > 0) {
                    jQuery('#complain_content').text(userName + "的回答");
                } else {
                    jQuery('#complain_content').text(userName + "的提问");
                }
                complainAskId = askId;
                complainUserId = userId;
                complainAnswerId = answerId;
                jQuery('#jubao_box').jqmShow();
            });

            return true;
        }
    </script>
    <script type="text/javascript">

        function DrawImage(ImgD,FitWidth) 
        { 
            var image=new Image(); 
            image.src=ImgD.src; 
            if(image.width>0 && image.height>0) 
            { 
                if(image.width!=FitWidth ) 
                {                   
                    ImgD.width=FitWidth; 
                    ImgD.height=(image.height*FitWidth)/image.width; 
                }                
                else 
                { 
                    ImgD.width=image.width; 
                    ImgD.height=image.height; 
                } 
                                
          
            }
        }

    </script>
