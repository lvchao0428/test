                    //美国 日本 德国 马来西亚 悉尼 西班牙 葡萄牙 塞浦路斯 希腊 澳大利亚 保加利亚 加拿大 
                    jQuery("#cityi010").append("<a target='_self' href='http://www.fang.com/ask/home.aspx?city=world'>海外</a>");

                    //判断用户是否登录
                    var isLogin = false;
                    var loginInfo = {};
                    jQuery.ajax({
                        url: '/Ask/UserInfo.aspx',
                        type: 'GET',
                        dataType: 'text',
                        //contentType: "application/x-www-form-urlencoded; charset=gb2312",
                        data: { q: Math.round(Math.random() * 10000) },
                        error: function (data) {
                            alert('登录失败，请您稍后再试！' + data.statusText);
                        },
                        success: function (data) {
                            loginInfo = eval("(" + data + ")");
                            //console.log(data);
                            //alert(loginInfo.UserID);
                            if (parseInt(loginInfo.UserID) > 0) {
                                //登录成功
                                var userid = parseInt(loginInfo.UserID);
                                var username = loginInfo.UserName;
                                jQuery("#topHeadLogin").show();
                                jQuery("#topHeadNotLogin").hide();
                                jQuery("#sfHeadUsername").text(username);
                                jQuery("#HideUserId").text(userid);

                                //console.log('login' + '-' + jQuery("#topHeadLogin").length + '-' + jQuery("#topHeadNotLogin").length);
                                $('#userCenter').attr("href", "http://www.fang.com/ask/usercenter.aspx?id=" + loginInfo.UserID);
                                $('#Myask').attr("href", "/ask/UserCenter.aspx?id=" + loginInfo.UserID);
                                $('#Myanswer').attr("href", "/ask/UserCenter.aspx?id=" + loginInfo.UserID + "&t=1");
                                $('#Myattention').attr("href", "/ask/UserCenter.aspx?id=" + loginInfo.UserID + "&t=2");
                                isLogin = true;
                            }
                            else {
                                jQuery("#topHeadLogin").hide();
                                jQuery("#topHeadNotLogin").show();
                                $('#userCenter').attr("href", "http://passport.fang.com/NewLogin.aspx?backurl=http://www.fang.com/ask/");
                                $('#Myask').attr("href", "http://passport.fang.com/NewLogin.aspx?backurl=http://www.fang.com/ask/");
                                $('#Myanswer').attr("href", "http://passport.fang.com/NewLogin.aspx?backurl=http://www.fang.com/ask/");
                                $('#Myattention').attr("href", "http://passport.fang.com/NewLogin.aspx?backurl=http://www.fang.com/ask/");
                                loginInfo = {};
                            }
                        }
                    });
                });
                jQuery(function () {
                    var messagecount = '0';
                    if (messagecount == "0")
                        jQuery("#msgBoxTj").hide();

                });
            </script>
