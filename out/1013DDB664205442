                success: function(result) {
                    data = eval('(' + result.d + ')');
                    if (data != null) {
                        $(".tenement").html(""); 
                        var str =  '<div class="side_fy_tit" style="border-bottom:1px dotted #bfbfbf;"><a href="' + data[0].moreurl + '" target="_blank">更多&gt;&gt;</a>您可能感兴趣的租房房源</div>';
                        $(str).insertBefore(".tenement");
                        var newHtml = ' <ul class="topcn">';
                        jQuery.each(data, function(i, rent) { 
                            newHtml+='<li><p style="display: none;"><a class="wid115" href="' + rent.url + '">' + rent.projname +'</a><span class="wid42">';
