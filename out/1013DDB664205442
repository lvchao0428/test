            jQuery("#niming_commit").click(function() {
                if(isManagerUser||UserLevel>=5)
                {
                    jQuery("#frmAnswerQuestion").submit();
                    return true;
                }
                if (jQuery.trim(escape(jQuery("#VCodeValue").val())) == '') {
                    alert('请输入验证码');
                    
                    return false;
                }
                jQuery.ajax({
                    url: '/Ask/Ajax/VcodeCheck.aspx',
                    type: 'Get',
                    dataType: 'text',
                    data: { VCode: escape(jQuery.trim(jQuery("#VCodeValue").val())) },
                    error: function(data) {
                        alert('验证失败，请您稍后再试！' + data.statusText);
                    },
                    success: function(data) {
                        if (data == "true") {
                            jQuery('#vcode').val(jQuery.trim(escape(jQuery("#VCodeValue").val())));
                            jQuery("#frmAnswerQuestion").submit();
                        }
                        else {
                            alert("验证码错误");
                            return false;
                        }
                        return true;
                    }
                });
                return true;
            });
        });

