    <script type="text/javascript">

        var isManagerUser = "False" == "True";
        var UserName="";
        var currentUserId = '0';
        var UserLevel='0';
        //验证码
        function GetVCode(imgname) {
            if(imgname == ''){
                imgname = 'imgcode';
            }
            if(document.images[imgname])
            {
                document.images[imgname].src = 'http://Captcha.fang.com/Display?r=' + Math.round(Math.random() * 10000);
            }
        }

        jQuery(function() {
            // GetVCode();

            var rootClassId = '2';
            if (rootClassId == '1') {
                jQuery("#event_menu").find("li").removeClass("cur");
                jQuery("#menu0").addClass("cur");

                jQuery("#event_city_change").find('.event_city').each(function(i) {
                    var spell = jQuery(this).attr("spell");
                    jQuery(this).attr("href", "/ask/esf/" + spell);
                    jQuery(this).click(function() {
                        //Common.SetCookie('ask_city', spell, 3600, null);
                        Common.SetCookie('city', spell, 3600, null);
                    });
                });
                jQuery("#event_current_city").show();
            } else if (rootClassId == '2') {
                jQuery("#event_menu").find("li").removeClass("cur");
                jQuery("#menu1").addClass("cur");
            } else {
                jQuery("#event_menu").find("li").removeClass("cur");
                jQuery("#menu2").addClass("cur");
            }

            jQuery(".editor_delbtnpic").live("click", function() {
                jQuery(this).hide();
                jQuery(".editor_btnpic").show();
                jQuery("#event_upload_img").val("");
            });

            jQuery('#niming_box').jqm({ modal: true });
            jQuery('#niming_box').css('opacity', 0.99);
            //匿名回答弹出层中的登录
            var updateTimeLabel = function (time) {
                var btn = $("#btnGetIdentityCode5");

                btn.val(time <= 0 ? "发送验证码" : ("" + (time) + "秒后可重发"));
                var hander = setInterval(function () {
                    if (time <= 0) {
                        clearInterval(hander);
                        hander = 0;
                        btn.val("发送验证码");
                        btn.attr("disabled", false);
                    }
                    else {
                        btn.attr("disabled", true);
                        btn.val("" + (time--) + "秒后可重发");
                    }
                }, 1000);
            };
            var sendVcode = function () {

                if ($("#btnGetIdentityCode5").attr("disabled") == true) {
                    return;
                }
                if( parseInt(currentUserId)>0)
                {
                    jQuery.ajax({
                        url: '/Ask/Ajax/MakeLinkAtPassPortAndMobile.aspx',
                        type: 'GET',
                        dataType: 'text',
                        data: { PhoneNum:jQuery("#txtPhoneNum2").val(), UserName: escape(UserName), q: Math.round(Math.random() * 10000) },
                        error: function (data) {
                            alert('登录失败，请您稍后再试！' + data.statusText);
                        },
                        success: function (data) {
                            if (data == "100") {
                                alert("验证码已发送成功 请耐心等候")
                                $("#btnGetIdentityCode5").attr("disabled", true);
                                updateTimeLabel(60);
                            }
                            else {
                                alert(data);
                                jQuery("#txtIdentityCode2").val("");
                            }
                        }
                    });
                }
                else
                {
                    jQuery.ajax({
                        url: '/Ask/Ajax/GetLoginValid.aspx',
                        type: 'GET',
                        dataType: 'text',
                        data: { PhoneNum: escape(jQuery("#txtPhoneNum2").val()), IdentityCode: jQuery("#txtIdentityCode2").val(), q: Math.round(Math.random() * 10000) },
                        error: function (data) {
                            alert('验证码发送失败 请重试！' + data.statusText);
                        },
                        success: function (data) {
                            if (data == "100") {
                                alert("验证码已发送成功 请耐心等候")
                                $("#btnGetIdentityCode5").attr("disabled", true);
                                updateTimeLabel(60);
                            }
                            else if (data == "008") {
                                alert("验证码发送太频繁  请稍后重试")
                            }
                            else if (data == "009") {
                                alert("短信发送失败  请稍后重试")
                            }   else if (data == "012") {
                                alert("一分钟内只能发送一次验证码 请稍后重试")
                            }
                            else {
                                alert("验证码发送失败 请重试");
                                jQuery("#txtIdentityCode2").val("");
                            }
                        }
                    });
              
                }
            };
     
        



            jQuery("#btnGetIdentityCode5").click(function () {
                sendVcode();
            });
        
             
