    <script> 
        function shangcommitt(Anid,AnUserid )
        {
            
            $("#dashang"+Anid).hide();
            $("#hui"+Anid).removeAttr("style");
            $("#shang"+Anid).css('display', 'none');
            var usertid = '0';
            var id='12562061';
            var Rewardthis = $("#Reward"+Anid);
            var zongfen=$("#dashangscore");
            if (usertid > 0 ) {
               
                jQuery.ajax({
                    url: '/ask/AJAX/WebAskReward.aspx/AskReward',
                    type: 'post',
                    dataType: 'json',
                    contentType: "application/json",
                    data: Common.jsonToString({ askID: escape('12562061'), currentUserID: escape('0'),answerUserID: escape(AnUserid),lAnswerId: escape(Anid) }),
                    success: function (result) {
                        data = eval('(' + result.d + ')');


                        if (data == 1)
                        {
                            $("#hui"+Anid).css('display', 'none');
                            $("#shang"+Anid).removeAttr("style");
                           
                            alert("不能对自己的回答赏TA")                          
                        }
                        else if(data == 2)
                        {
                            // $("#shang"+Anid).removeAttr("style");
                            // $("#shanged"+Anid).css('display', 'none');
                            
                            $("#shanged"+Anid).removeAttr("style");
                            $("#hui"+Anid).css('display', 'none');
                            alert("已经点过赏TA");
                        }
                        else if(data == 3)
                        {
                            
                            Rewardthis.html(parseInt(Rewardthis.text()) + 1);                           
                            zongfen.html(parseInt(zongfen.text()) - 1);
                            $("#hui"+Anid).css('display', 'none');
                            $("#shanged"+Anid).removeAttr("style");

                            alert("赏TA成功");                            
                        }
                        else {
                            $("#hui"+Anid).css('display', 'none');
                            $("#shang"+Anid).removeAttr("style");                                                   
                            alert("您的积分不足，赏TA失败");
                        }

                    },
                    error: function (result) {
                        $("#hui"+Anid).css('display', 'none');
                        $("#shang"+Anid).removeAttr("style");                       
                        alert("失败");
                    }

                });               
                

            }
            else {
                $("#dashang"+Anid).css('display', 'none');
                window.location = "http://passport.fang.com/NewLogin.aspx?backurl=http://www.fang.com/ask/ask_" + id + ".html";
            }

        }
       
        function Reward(Anid,AnUserid ){
            
            // if (document.cookies.get("answer_dashang" + '0') == undefined || document.cookies.get("answer_dashang" + '0') == undefined) 
            if(getCookie("answer_dashang"+'0')==undefined )
            {
                $("#dashang"+Anid).removeAttr("style");                                 
            }
            else
            {
                $("#hui"+Anid).removeAttr("style");
                $("#shang"+Anid).css('display', 'none');
                var usertid = '0';
                var id='12562061';
                var Rewardthis = $("#Reward"+Anid);
                var zongfen=$("#dashangscore");
                if (usertid > 0 ) {
               
                    jQuery.ajax({
                        url: '/ask/AJAX/WebAskReward.aspx/AskReward',
                        type: 'post',
                        dataType: 'json',
                        contentType: "application/json",
                        data: Common.jsonToString({ askID: escape('12562061'), currentUserID: escape('0'),answerUserID: escape(AnUserid),lAnswerId: escape(Anid) }),
                        success: function (result) {
                            data = eval('(' + result.d + ')');


                            if (data == 1)
                            {
                                $("#hui"+Anid).css('display', 'none');
                                $("#shang"+Anid).removeAttr("style");
                           
                                alert("不能对自己的回答赏TA")                          
                            }
                            else if(data == 2)
                            {
                                // $("#shang"+Anid).removeAttr("style");
                                // $("#shanged"+Anid).css('display', 'none');
                            
                                $("#shanged"+Anid).removeAttr("style");
                                $("#hui"+Anid).css('display', 'none');
                                alert("已经点过赏TA");
                            }
                            else if(data == 3)
                            {
                            
                                Rewardthis.html(parseInt(Rewardthis.text()) + 1);                           
                                zongfen.html(parseInt(zongfen.text()) - 1);
                                $("#hui"+Anid).css('display', 'none');
                                $("#shanged"+Anid).removeAttr("style");

                                alert("赏TA成功");                            
                            }
                            else {
                                $("#hui"+Anid).css('display', 'none');
                                $("#shang"+Anid).removeAttr("style");                                                   
                                alert("您的积分不足，赏TA失败");
                            }

                        },
                        error: function (result) {
                            $("#hui"+Anid).css('display', 'none');
                            $("#shang"+Anid).removeAttr("style");                       
                            alert("失败");
                        }

                    });               
                

                }
                else {
                    $("#dashang"+Anid).css('display', 'none');
                    window.location = "http://passport.fang.com/NewLogin.aspx?backurl=http://www.fang.com/ask/ask_" + id + ".html";
                }

            }
            // Dashang('0'); 
            setCookie("answer_dashang"+'0', '0')
        }  
        function shangclose(Anid){
            $("#dashang"+Anid).css('display', 'none');
        }
        //function Dashang(intn) {
        //  document.cookies.set("answer_dashang" + intn, "answer_dashang" + intn, 600 * 1000, null, "fang.com");    
        //};
        function setCookie(name, value) {
            var Days = 360;
            var exp = new Date(); //new Date("December 31, 9998");
            exp.setTime(exp.getTime() + Days * 24 * 60 * 60 * 1000);
            document.cookie = name + "=" + escape(value) + ";expires=" + exp.toGMTString();
        }
        //获取cookie
        function getCookie(name) {
            var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
            if (arr = document.cookie.match(reg)) return unescape(arr[2]);
            else return null;
        }
