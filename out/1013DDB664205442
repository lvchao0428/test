    jQuery("#LoginButton").click(function () {
        var rdoLoginType = $('input[type="radio"][name="rdoLoginType"]:checked').attr("value");

        if (rdoLoginType == "NomalLogin") {
            var username = jQuery("#UserName").val();
            if (username == '手机号/邮箱/用户名' || username == '') {
                alert("请输入用户名");
                return;
            }
            if (jQuery("#Password").val() == '') {
                alert("请输入登录密码");
                return;
            }
            setMaxDigits(129);
            var key_to_encode = new RSAKeyPair("010001", "", "978C0A92D2173439707498F0944AA476B1B62595877DD6FA87F6E2AC6DCB3D0BF0B82857439C99B5091192BC134889DFF60C562EC54EFBA4FF2F9D55ADBCCEA4A2FBA80CB398ED501280A007C83AF30C3D1A142D6133C63012B90AB26AC60C898FB66EDC3192C3EC4FF66925A64003B72496099F4F09A9FB72A2CF9E4D770C41");
            var pwdRtn = encryptedString(key_to_encode, jQuery("#Password").val());
            //jQuery("#Password").val(pwdRtn);


            $.ajax({
                type: "get",
                url: "https://passport.fang.com/login.api",
                dataType: "jsonp",
                jsonp: "callback",
                data: { "Uid": escape(jQuery("#UserName").val()), "Pwd": escape(pwdRtn), "Service": escape('soufun') },
                success: function (json) {
                    if (json.Message == 'Success') {

                        if (!window.location.href.toLowerCase().Contain('ask_steptwo.aspx')) {
                            window.location.reload();
                        } else {
                            window.location.href = "Ask_StepTwo.aspx?asktitle=" + escape(jQuery("#inputasktitle").val());
                        }
                    }
                    else {
                        alert('登录失败，请您稍后再试！');
                    }


                }
            });
        }
        else {
            var phoneNum = jQuery("#txtPhoneNum").val();
            if (phoneNum == '请输入手机号码' || phoneNum == '') {
                alert("请输入手机号");
                return;
            }
            if (jQuery("#txtIdentityCode").val() == '' || jQuery("#txtIdentityCode").val() == "手机验证码") {
                alert("请输入手机验证码!");
                return;
            }
            setMaxDigits(129);
            var key_to_encode = new RSAKeyPair("010001", "", "978C0A92D2173439707498F0944AA476B1B62595877DD6FA87F6E2AC6DCB3D0BF0B82857439C99B5091192BC134889DFF60C562EC54EFBA4FF2F9D55ADBCCEA4A2FBA80CB398ED501280A007C83AF30C3D1A142D6133C63012B90AB26AC60C898FB66EDC3192C3EC4FF66925A64003B72496099F4F09A9FB72A2CF9E4D770C41");
            var IdentityCodeRtn = encryptedString(key_to_encode, jQuery("#txtIdentityCode").val());
            //jQuery("#txtIdentityCode").val(IdentityCodeRtn);
            jQuery.ajax({
                url: '/Ask/Ajax/MobileLogin.aspx',
                type: 'GET',
                dataType: 'text',
                data: { PhoneNum: escape(jQuery("#txtPhoneNum").val()), IdentityCode: IdentityCodeRtn, q: Math.round(Math.random() * 10000) },

                error: function (data) {
                    alert('登录失败，请您稍后再试！' + data.statusText);
                },
                success: function (data) {
                    if (data == "100") {
                        if (!window.location.href.toLowerCase().Contain('ask_steptwo.aspx')) {
                            window.location.reload();
                        } else {
                            window.location.href = "Ask_StepTwo.aspx?asktitle=" + escape(jQuery("#inputasktitle").val());
                        }
                    }
                        //else if (data == "105") {
                        //    //alert("您的手机号需要验证，请您通过通行证验证手机号，十分感谢！");
                        //    //window.open("http://passport.soufun.com");
                        //}
                    else if (data == "103") {
                        window.location.href = "http://passport.fang.com/adduser.aspx?service=ask&host=www.soufun.com&backurl=" + window.location;
                    }
                    else {
                        jQuery("#lbyzMes").show();

                    }
                }
            });
    }
    });
jQuery("#event_logout").live("click", function () {
    jQuery.ajax({
        url: '/Ask/Ajax/LogOut.aspx',
        type: 'GET',
        dataType: 'text',
        data: { q: Math.round(Math.random() * 10000) },
        error: function (data) {
            alert('登录失败，请您稍后再试！' + data.statusText);
        },
        success: function (data) {
            //window.location.reload();

            jQuery("#topHeadLogin").hide();
            jQuery("#topHeadNotLogin").show();
            window.location.reload();
        }
    });
});

jQuery('input[name="asktitle"]').autocomplete("/ask/ajax/SearchTip.aspx",
{
    delay: 10,
    minChars: 1,
    matchSubset: 10,
    matchContains: 1,
    cacheLength: 10,
    onItemSelect: function (li, input) {
        return input.parents('form').submit();
    },
    onFindValue: function () { },
    formatItem: function (row) { return row[0]; },
    autoFill: false
});

var updateTimeLabel = function (time) {
    var btn = $("#btnGetloginvalid");

    btn.val(time <= 0 ? "免费获取验证码" : ("" + (time) + "秒后点击重新发送"));
    var hander = setInterval(function () {
        if (time <= 0) {
            clearInterval(hander);
            hander = 0;
            btn.val("免费获取验证码");
            btn.attr("disabled", false);
        }
        else {
            btn.attr("disabled", true);
            btn.val("" + (time--) + "秒后点击重新发送");
        }
    }, 1000);
};
var sendVcode = function () {

    if ($("#btnGetloginvalid").attr("disabled") == true) {
        return;
    }
    jQuery.ajax({
        url: '/Ask/Ajax/GetLoginValid.aspx',
        type: 'GET',
        dataType: 'text',
        data: { PhoneNum: escape(jQuery("#txtPhoneNum").val()), IdentityCode: jQuery("#txtIdentityCode").val(), q: Math.round(Math.random() * 10000) },
        error: function (data) {
            alert('登录失败，请您稍后再试！' + data.statusText);
        },
        success: function (data) {
            if (data == "100") {
                $("#btnGetloginvalid").attr("disabled", true);
                updateTimeLabel(60);
                alert("验证码已发送成功 请耐心等候");
            }
            else if (data == "008") {
                alert("验证码发送太频繁  请稍后重试");
            }
            else if (data == "009") {
                alert("短信发送失败  请稍后重试")
            }
            else if (data == "012") {
                alert("一分钟内只能发送一次验证码 请稍后重试")
            }
            else {
                alert("短信发送失败 请重试");
                jQuery("#txtIdentityCode").val("");
            }
        }
    });
};
jQuery("#btnGetloginvalid").click(function () {
    sendVcode();
});
