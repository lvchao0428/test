                            newHtml+=rent.url +'"><img height="75" width="100" title="'+rent.projname+'" src="'+rent.titleimage+'"></a><span><a href="';
                            newHtml+=rent.url + '">'+rent.projname +'</a><br>'+rent.room+'室'+rent.hall+'厅'+'<span class="marl15">'+rent.buildarea+'㎡</span><br>月租：<span class="fred">';
                            newHtml+=rent.price+'</span></span></div></li>';
                        });
                        newHtml += '</ul>';
                        $(".tenement").append(newHtml);
                        slidecss();
                        $(".tenementShow").show();
                        newHtml = "";
                    }
                        
                }
            });
        } 

            
