<script type="text/javascript">
    jQuery(document).ready(function() {
        //加载搜索信息
        jQuery.ajax({
            url: '/ask/AJAX/Newhouse.aspx/LoadLoupan',
            type: 'post',
            cache:false,
            dataType: 'json',
            contentType: "application/json",
            data: Common.jsonToString({ cityName: escape('防城港') }),
            success: function(result) {
                data = eval('(' + result.d + ')');
                if (data.HotNewHouses && data.HotNewHouses.length > 0) {
                    jQuery('#ask-loupan-Hot').html('');
                    var moreUrl = '';
                    if (data.MoreUrlHot && data.MoreUrlHot != 'null')
                        moreUrl = data.MoreUrlHot;
                    var newHtml = '<div class="side_fy"><div class="side_fy_tit"><a target="_blank" href="'+moreUrl+'">更多&gt;&gt;</a>防城港热门楼盘排行榜</div><div style="display: block;" class="proptcomprsn_cont noneBox"><div class="h1"><div class="wid106">楼盘名称</div><div class="wid42">区县</div><div class="wid72">点击(次)</div></div><ul class="proptcomprsn_list">';

                    jQuery.each(data.HotNewHouses, function(i, item) {
                        newHtml += '<li><div class="wid115"><a href="' + item.Url + '" target="_blank" title="' + item.ProjName + '">' + item.ProjName + '</a></div><div title="' + item.District + '" class="wid42">' + item.District + '</div><div class="wid62 fred center">' + item.Hits + '</div></li>';
                    });

                    newHtml += '</ul></div></div>';
                    jQuery('#ask-loupan-Hot').append(newHtml);
                }
                if (data.DiscountLoupans && data.DiscountLoupans.length > 0) {
                    jQuery('#ask-loupan-discount').html('');
                    var newHtml = '<div class="side_fy"><div class="side_fy_tit"><a target="_blank" href="' + data.MoreUrlDiscount + '">更多&gt;&gt;</a>防城港打折优惠楼盘</div><div style="display: block;" class="proptcomprsn_cont noneBox"><div class="h1"><div class="wid106">楼盘名称</div><div class="wid42">区县</div><div class="wid72">价格(元/㎡)</div></div><ul class="proptcomprsn_list">';

                    jQuery.each(data.DiscountLoupans, function(i, item) {
                        var price = item.Price;
                        if (jQuery.trim(price) == "") {
                            price = "--";
                        }
                        newHtml += '<li><div class="wid115"><a href="' + item.HouseUrl + '" target="_blank" title="' + item.ProjName + '">' + item.ProjName + '</a></div><div title="' + item.District + '" class="wid42">' + item.District + '</div><div class="wid62 fred center">' + price + '</div></li>';
                    });

                    newHtml += '</ul></div></div>';
                    jQuery('#ask-loupan-discount').append(newHtml);
                }
            }
        });
    });
</script>


        
        
        
        
