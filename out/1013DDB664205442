            if ($.trim($(this).val()) == '')
                jQuery(this).parents('.esf_s_box').removeClass('focus');
        });

        if ($.trim($('#asktitle2').val()) != '') {
            jQuery('#asktitle2').parents('.esf_s_box').addClass('focus');
        }
