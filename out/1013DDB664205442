<script type="text/javascript">
    jQuery(document).ready(function() {
        //加载搜索信息
        jQuery.ajax({
        url: '/ask/AJAX/HomeInfo.aspx/LoadHomeProduct',
            type: 'post',
            dataType: 'json',
            contentType: "application/json",
            data: Common.jsonToString({ askId: '12562061' }),
            success: function(result) {
                data = eval('(' + result.d + ')');
                if (data.ProductModels && data.ProductModels.length > 1) {
                    jQuery('#ask-homemall').html('');
                    var newHtml = '<div class="side_fy"><div class="side_fy_tit"><a target="_blank" href="' + data.MoreUrl + '">更多&gt;&gt;</a>' + data.Title + '</div><div class="side_fy_ul"><ul class="clearfix">';

                    jQuery.each(data.ProductModels, function(i, item) {
                    newHtml += '<li><div class="pic"><a target="_blank" href="' + item.Url + '"><img width="102px" height="78px" alt="'
                    + item.ShowName + '" title="' + item.ShowName + '" src="' + item.ShowImageUrl
                    + '"></a></div><p class="s1" style="overflow: hidden;max-width:140px;padding-top:3px;" alt="' + item.ShowName + '" title="' + item.ShowName
                    + '"><a target="_blank" href="' + item.Url + '">' + item.ShowName.substr(0, 16) + '</a></p><p class="s2">' + item.ShowPrice + '</p></li>';
                    });

                    newHtml += '</ul></div></div>';
                    jQuery('#ask-homemall').append(newHtml);
                }
            }
        });
    });
</script>
        
