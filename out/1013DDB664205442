                        $("#attention1").removeAttr("style");
                    }
                    else {
                        $("#attentionend1").removeAttr("style");
                    }
                }
                //关注
                $("#attention1").click(function () {
               
                    var usertid = '0';
                    var id='12562061';

                    if (usertid > 0) {

                        jQuery.ajax({
                            url: '/ask/AJAX/NewAskAttention.aspx/AddAttention',
                            type: 'post',
                            dataType: 'json',
                            contentType: "application/json",
                            data: Common.jsonToString({ id: escape('12562061'), userid: escape('0') }),
                            success: function (result) {
                                data = eval('(' + result.d + ')');


                                if (data == 2) {

                                    $("#attention1").css('display', 'none');
                                    $("#attentionend1").removeAttr("style");
                                    alert("关注成功");

                                }
                                else if(data == 1)
                                {
                                    alert("不能关注自己的问题");
                                }
                                else {
                                    alert("关注失败");
                                }

                            },
                            error: function (result) {
                                alert("失败");
                            }

                        });

                    }
                    else {
                    
                        window.location = "http://passport.fang.com/NewLogin.aspx?backurl=http://www.fang.com/ask/ask_" + id + ".html";
                    }
                });
                //取消关注
                $("#attentionend1").click(function () {
               
                    var usertid = '0';
                    var id='12562061';
                    if (usertid > 0) {

                        jQuery.ajax({
                            url: '/ask/AJAX/NewAskAttention.aspx/ConsoleAttention',
                            type: 'post',
                            dataType: 'json',
                            contentType: "application/json",
                            data: Common.jsonToString({ id: escape('12562061'), userid: escape('0') }),
                            success: function (result) {
                                data = eval('(' + result.d + ')');


                                if (data == true) {

                                    $("#attentionend1").css('display', 'none');
                                    $("#attention1").removeAttr("style");
                                    alert("取消关注成功");

                                }
                                else {
                                    alert("取消关注失败");
                                }

                            },
                            error: function (result) {
                                alert("失败");
                            }

                        });

                    }
                    else {
                        window.location = "http://passport.fang.com/NewLogin.aspx?backurl=http://www.fang.com/ask/ask_" + id + ".html";
                    }
                });

