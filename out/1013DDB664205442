    <script type="text/javascript">
        //提交回答
        var commitSucess = false;
        function answerCommit(askId)
        {
            //$('#event_answer_submit').unbind("click");
            //20150409
            $('#event_answer_submit').hide();
            $('#nosubmit').removeAttr("style");
            if(commitSucess)
            {
                return true;
            }
            jQuery('#askdetail_answer_tip').css("display", "none");
            
            var content = jQuery.trim(jQuery('#co').val());
            var currentUserId = '0';

            if (content.length <= 0) {
                jQuery('#askdetail_answer_tip').text("请输入文字内容");
                jQuery('#askdetail_answer_tip').css("display", "");
                commitSucess = false;  
                //$('#event_answer_submit').bind('click', function () {

                //answerCommit('12562061');
                // });
                //20150409
                $('#event_answer_submit').show();
                $('#nosubmit').css({ 'display': 'none' });
                return false;
            }
            if (content.length < 6) {
                jQuery('#askdetail_answer_tip').text("输入内容不能少于6字");
                jQuery('#askdetail_answer_tip').css("display", "");
                commitSucess = false; 
                //$('#event_answer_submit').bind('click', function () {

                //answerCommit('12562061');
                // });
                //20150409
                $('#event_answer_submit').show();
                $('#nosubmit').css({ 'display': 'none' });
                return false;
            }
            if (content.length > 2000) {
                jQuery('#askdetail_answer_tip').text("输入内容不能超过2000字");
                jQuery('#askdetail_answer_tip').css("display", "");
                commitSucess = false;
                // $('#event_answer_submit').bind('click', function () {

                // answerCommit('12562061');
                // });
                //20150409
                $('#event_answer_submit').show();
                $('#nosubmit').css({ 'display': 'none' });
                return false;
            }
            if (!isManagerUser &&UserLevel<5&& jQuery.trim(jQuery("#vcode").val()) == '') {
                alert('请输入验证码');
                
                // $('#event_answer_submit').bind('click', function () {

                //answerCommit('12562061');
                // });
                //20150409
                $('#event_answer_submit').show();
                $('#nosubmit').css({ 'display': 'none' });
                return false;
            }

            if(isManagerUser||UserLevel>=5)
            {
                if (currentUserId == 0) {
                    jQuery('#niming_box #niming_UserName').val('');
                    jQuery('#niming_box #niming_Password').val('');
                    //jQuery('#niming_box #VCodeValue').val('');
                    jQuery('#niming_box').jqmShow();
                    // $('#event_answer_submit').bind('click', function () {

                    // answerCommit('12562061');
                    // });
                    //20150409
                    $('#event_answer_submit').show();
                    $('#nosubmit').css({ 'display': 'none' });
                    return true;
                }
                else {
                    setTimeout(function(){jQuery('#frmAnswerQuestion').submit();},0);
                }

                commitSucess = true;
                return true;
            }
            else
            {
                jQuery.ajax({
                    url: '/Ask/Ajax/VcodeCheck.aspx',
                    type: 'Get',
                    dataType: 'text',
                    data: { VCode: escape(jQuery.trim(jQuery("#vcode").val())) },
                    error: function(data) {
                        alert('验证失败，请您稍后再试！' + data.statusText);
                        //$('#event_answer_submit').bind('click', function () {

                        //answerCommit('12562061');
                        //});
                        //20150409
                        $('#event_answer_submit').show();
                        $('#nosubmit').css({ 'display': 'none' });
                        return false;
                    },
                    success: function(data) {
                        if (data == "true") {
                            jQuery('#vcode').val(jQuery.trim(jQuery("#vcode").val()));

                            if (currentUserId == 0) {
                                jQuery('#niming_box #niming_UserName').val('');
                                jQuery('#niming_box #niming_Password').val('');
                                //jQuery('#niming_box #VCodeValue').val('');
                                jQuery('#niming_box').jqmShow();
                                //$('#event_answer_submit').bind('click', function () {

                                // answerCommit('12562061');
                                // });
                                //20150409
                                $('#event_answer_submit').show();
                                $('#nosubmit').css({ 'display': 'none' });
                                return true;
                            }
                            else {
                                setTimeout(function(){jQuery('#frmAnswerQuestion').submit();},0);
                            }

                            commitSucess = true;
                            return true;
                        }
                        else {
                            alert("验证码错误");
                            //$('#event_answer_submit').bind('click', function () {

                            // answerCommit('12562061');
                            //});
                            //20150409
                            $('#event_answer_submit').show();
                            $('#nosubmit').css({ 'display': 'none' });
                            return false;
                        }
                    }
                });
            }
        }
