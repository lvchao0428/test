			nextPLen = nextPage.length;
			isGoon = false;
			if (navigator.userAgent.toLowerCase().indexOf('msie')>0) {
				altKey = window.event.altKey;
				ctrlKey = window.event.ctrlKey;
				shiftKey = window.event.shiftKey;
			} else {
				altKey = e.altKey;
				ctrlKey = e.ctrlKey;
				shiftKey = e.shiftKey;
			}
			srcElement = e.target ? e.target : window.event.srcElement;
			if (srcElement && srcElement.tagName.toLowerCase() == 'input' && srcElement.getAttribute("type") == "text") return true;
