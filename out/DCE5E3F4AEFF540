(function(tracker) {
	"use strict"
	var hmtItems = $('div.hmt-track');
	if (hmtItems.length > 0) {
		hmtItems.each(function(k, v) {
			(function(t) {
				var $this = $(t);
				var data = $this.data('hmt').split('|');
				tracker.push(['_trackEvent', data[0], data[1], data[2]]);
			})(v);
		});
	}
})(_hmt);
