<script>
jQuery(function() {
var url = '';
var href = window.location.href;
var index1 = href.indexOf("?jumpurl=");
var idnex2 = href.indexOf("jumpurl=");
if(index1 > 0) {
href = href.substring(0, index1);
}  else if(idnex2 > 0) {
href = href.substring(0, index2);
}
if(url == '' || url == null) {
var jumpurl = document.getElementById('jumpurl');
jumpurl.value = href;
}
var loginurl = document.getElementById('loginurl');
loginurl.value =href;
jQuery('.b_txtbar li button[type="submit"]').click(function(){
var username = jQuery('input[name="username"]').val();
var password = jQuery('input[name="password"]').val();
if(username=='' || username==null) {
alert('请填写用户名');
return false;
}
if(password=='' || password==null) {
alert('请填写密码');
return false;
}
jQuery('#loginForm').submit();
});
});
</script>
