function keydown(e){
　 var e=e||event;
　 var currKey=e.keyCode||e.which||e.charCode;
　 if((currKey>7&&currKey<14)||(currKey>31&&currKey<47))
　 {
　 　   switch(currKey)
   　 　 { 
	         case 37:document.location.href="go.asp?boardid=77&sid=11320791"; break;
    　 　 　 case 39:document.location.href="go.asp?boardid=77&sid=11320791&action=next"; break;
    　 　 　 default:keyName = "";    break;
　 　   }
　 }
}

document.onkeydown = keydown;

