	function dp_delete(t) {
		var id = $(t).attr('data-id');
		var pid = $(t).attr('data-pid');
		var flag = $(t).attr('data-flag');
		var fid = $(t).attr('data-fid');
		var tid = $(t).attr('data-tid');
		if (!flag || flag == '0') {
			$(t).attr('data-flag', '1');
		} else {
			return;
		}
		if (!confirm('删除不可恢复，确认删除吗？')) {
			$(t).attr('data-flag', '0');
			return;
		}
		$.ajax({
			type:'POST',
			url: 'http://bbs.meizhou.com/plugin.php?id=superman_dianping:ajax',
			data: 'dpdelsubmit=1&tid='+tid+'&fid='+fid+'&dpid='+id+'&formhash=722729b7',
			dataType: 'JSON',
			beforeSend: function() {
				dp_loading('show');
			},
			complete: function() {
				dp_loading('hide');
				$(t).attr('data-flag', '0');
			},
			success: function(resp) {
				if (resp.errno == 0) {
					var count = parseInt($('#dp_count_'+pid).html()) - 1;
					$('#dp_count_'+pid).html(count);
					$(t).parent().parent().parent().remove();
					$('#dp_list_content_'+pid+' li:first').addClass('dp_item_no_border');
					showPrompt(null, null, '<span style="color:#fff">删除成功！</span>', 2000);
				} else if (resp.errno == -8) {
					showError('没有删除权限！');
					return;
				} else {
					showError(resp.errno+':'+resp.errmsg);
					return;
				}
			}
		});
	}
	var g_dp_list = [];
	$('.dp_list_content .pg a').bind('click', function(){
		$('.dp_list_content .pg a').each(function(){
			$(this).removeClass('dp_pg_link_hover');
		});
		$(this).addClass('dp_pg_link_hover');
		var t = this;
		var flag = $(t).attr('data-flag');
		if (!flag || flag == '0') {
			$(t).attr('data-flag', '1');
		} else {
			return;
		}
		var page = $(t).text();
		var pid = $(t).parent().parent().attr('data-pid');
		if (g_dp_list['"'+pid+'_'+page+'"']) {
			$('#dp_list_content_'+pid).html(g_dp_list['"'+pid+'_'+page+'"']);
			$(t).attr('data-flag', '0');
			return;
		}
		$.ajax({
			type:'POST',
			url: 'http://bbs.meizhou.com/plugin.php?id=superman_dianping:ajax',
			data: 'dpviewsubmit=1&pid='+pid+'&page='+page+'&formhash=722729b7',
			dataType: 'JSON',
			beforeSend: function() {
				dp_loading('show');
			},
			complete: function() {
				dp_loading('hide');
				$(t).attr('data-flag', '0');
			},
			success: function(resp) {
				if (resp.errno == 0 && resp.data) {
					var list = '';
					for (var i=0;i<resp.data.length;i++) {
						list += '<li class="'+((i==0)?'dp_item_no_border':'')+'">';
						list += '<a href="home.php?mod=space&uid='+resp.data[i].uid+'" target="_blank" class="z dp_item_avatar">'+resp.data[i].avatar+'</a>';
						list += '<div class="dp_item_content_wrap">';
						list += '<a href="home.php?mod=space&uid='+resp.data[i].uid+'" target="_blank">'+resp.data[i].username+'</a>:&nbsp;';
						list += '<span class="dp_item_content">'+resp.data[i].message+'</span>';
						list += '<div style="text-align:right;">';
						list += '<span>'+resp.data[i].dateline+'</span>&nbsp;';
						list += '<a href="javascript:void(0)" class="dp_item_reply" data-pid="'+pid+'" data-uid="'+resp.data[i].uid+'" data-author="'+resp.data[i].username+'">回复</a>';
						if ('0' == resp.data[i].uid || '0') {
							list += ' &nbsp| &nbsp;<a href="javascript:void(0)" class="dp_item_del" data-pid="'+pid+'" data-id="'+resp.data[i].id+'" style="color:#f00">删除</a>';
						}
						list += '</div>';
						list += '</div>';
						list += '</li>';
					}
					$('#dp_list_content_'+pid).html(list);
					g_dp_list['"'+pid+'_'+page+'"'] = list;
					
					$('.dp_item_reply').bind('click', function(){
						if ('0' == '0') {
							showWindow('login', 'member.php?mod=logging&action=login');
							return;
						}
						var pid = $(this).attr('data-pid');
						var author = $(this).attr('data-author');
					    sc_dianping_uid = $(this).attr('data-uid');
						$('#dp_editor_'+pid).fadeIn();
						$('#dpfastpost'+pid+'message').focus().val('回复 '+author+' :');
					});
					
					$('.dp_item_del').bind('click', function(){
						var t = this;
						dp_delete(t);
					});
				}
			}
		});
	});
	$('.dp_reply_btn').bind('click', function(){		var t = this;
		var seccode_post_data = '';
		var pid = $(this).attr('data-pid');
		var tid = $(this).attr('data-tid');
		var fid = $(this).attr('data-fid');
		var message = $('#dpfastpost'+pid+'message');
		if (message.val() == '') {
			message.focus();
			showError('回复内容为空，请填写后提交！');
			return;
		}		var dp_reply_interval = parseInt(getcookie('dp_reply_interval'));
		if (dp_reply_interval > 0) {
			var interval = ((new Date().getTime())/1000) - dp_reply_interval;
			if (interval < parseInt('10')) {
				var remain = Math.ceil(parseInt('10') - interval);
				showError('回复太快，请 '+remain+' 秒后再提交，喝杯水休息一下吧~~~');
				return;
			}
		}		msg = message.val();
		msg = encodeURI(encodeURI(msg));
		$.ajax({
			type:'POST',
			url: 'http://bbs.meizhou.com/plugin.php?id=superman_dianping:ajax',
			data: 'dppostsubmit=1&pid='+pid+'&fid='+fid+'&tid='+tid+'&dianping_uid='+sc_dianping_uid+'&formhash=722729b7&message='+msg+seccode_post_data,
			dataType: 'JSON',
			beforeSend: function() {
				$(t).attr('disabled', true).css({'background': '#ccc', 'border-color': '#aaa'});
				dp_loading('show');
			},
			success: function(resp) {
				dp_loading('hide');
				$(t).attr('disabled', false).css({'background': 'url("http://bbs.meizhou.com/static/image/common/pn.png") repeat-x scroll 0 0 #e5e5e5', 'border-color': '#06c', 'background-position': '0 -48px'});
				if (resp.errno == '0') {
				    var msg = '回复成功！';
				    if (resp.data[0].addcredit != '0' && resp.data[0].addcredit != '-1') {
                        msg += ' '+resp.data[0].addcredit;
				    }
					showPrompt(null, null, '<span style="color:#fff">'+msg+'</span>', 3000);
					var item = '<li>';
					item += '<a href="home.php?mod=space&uid='+resp.data[0].uid+'" target="_blank" class="z dp_item_avatar">'+resp.data[0].avatar+'</a>';
					item += '<div class="dp_item_content_wrap">';
					item += '<a href="home.php?mod=space&uid='+resp.data[0].uid+'" target="_blank">'+resp.data[0].username+'</a>:&nbsp;';
					item += '<span class="dp_item_content">'+resp.data[0].message+'</span>';
					item += '<div style="text-align:right;">';
					item += '<span>'+resp.data[0].dateline+'</span>&nbsp;';
					item += '<a href="javascript:void(0)" class="dp_item_reply" data-pid="'+pid+'" data-uid="'+resp.data[0].uid+'" data-author="'+resp.data[0].username+'">回复</a>';
					item += ' &nbsp| &nbsp;<a href="javascript:void(0)" class="dp_item_del" data-fid="" data-pid="'+pid+'" data-id="'+resp.data[0].id+'" style="color:#f00">删除</a>';
					item += '</div>';
					item += '</div>';
					item += '</li>';
					$('#dp_list_content_'+pid).append(item);
					$('#dp_list_content_'+pid+' li:first').addClass('dp_item_no_border');
					$('#dpfastpost'+pid+'message').val('');
					$('#dp_seccode_'+pid+' input[name=seccodeverify]').val('');
					$('#dp_seccode_img_'+pid).trigger('click');
					var count = parseInt($('#dp_count_'+pid).html()) + 1;
					$('#dp_count_'+pid).html(count);
					
					$('.dp_item_del').bind('click', function(){
						var t = this;
						dp_delete(t);
					});					setcookie('dp_reply_interval', (new Date().getTime())/1000, '10');					//$('html,body').animate({scrollTop:$("#dp_wrap_"+pid).offset().top}, 1000);
					
					$('.dp_item_reply').bind('click', function(){
						if ('0' == '0') {
							showWindow('login', 'member.php?mod=logging&action=login');
							return;
						}
						var pid = $(this).attr('data-pid');
						var author = $(this).attr('data-author');
					    sc_dianping_uid = $(this).attr('data-uid');
						$('#dp_editor_'+pid).fadeIn();
						$('#dpfastpost'+pid+'message').focus().val('回复 '+author+' :');
					});
					return;
				} else if (resp.errno == '-1') {
					showError('系统错误，请稍后重试！');		
					return;
				} else if (resp.errno == '-3') {
					showWindow('login', 'member.php?mod=logging&action=login');
					return;
				} else if (resp.errno == '-5' || resp.errno == '-6') {
					showError(resp.errmsg);
					return;
				} else if (resp.errno == '-7') {
					seccode.select().focus();
					showError('验证码错误，请检查后重新提交！');
					return;
				} else {
					showError(resp.errno+':'+resp.errmsg);
					return;
				}
			}
		});
		return;
	});

	function dp_loading(flag) {
		var left = ($(window).width() - $('#dp_loading').width())/2;
		var top = ($(window).height() - $('#dp_loading').height())/2;
		if (flag == 'show') {
			$("#dp_loading").css({'z-index': '9999', 'top': top, 'left': left, 'visibility':'visible'});
		} else {
			$("#dp_loading").css({'z-index': '9999', 'top': top, 'left': left, 'visibility':'hidden'});
		}
	}})(jQuery);
