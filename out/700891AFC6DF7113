var url=window.location.href,
isHas=url.indexOf("|") ,
us=url.split("|");

if(isHas!=-1){
   url=us[0]+"%7C"+".html";
}

if(NTES("#photoView")){
	bShare.addEntry({
	  	summary:"分享自 网易论坛",
	  	pic: {
	  		toString:function(){return NTES("#photo").attr("src")}
	  	},
		url:url
	});
} else {
	 if(NTES(NTES(".tie-content")[0]).NTES("img")[0]){
	 	var src= NTES(NTES(".tie-content")[0]).NTES("img")[0].src;
	 }else {
	 	var src = "";
	 }
	bShare.addEntry({summary:"分享自 网易论坛",pic:src,url:url});
};	
