<script type="text/javascript">
var totalAttach = $("#totalAttach").val(); //最多添加5个附件
//添加一个附件行
function addAttachLine(){
	if($("input[name='picfile']").length < totalAttach) {
		$("input[name='picfile']:last").parent().parent().after('<fieldset><legend>附件信息</legend><p class="fileP">图片：<input type="file" name="picfile" size="56" style="height:22;"></p><p class="fileP"><span style="height: 100%; vertical-align: middle;">描述：</span><textarea name="descp" placeholder="限200字" onkeyup="checkDescpCount(this)"></textarea></p><p class="fileP"><span class="descpTips"></span><span class="delBtn"><input type="button" value="删除" onclick="removePicInput(this)" /></span></p></fieldset>');
	}else{
		alert("最多只能添加"+totalAttach+"个附件");
	}
}
//删除一个附件行
function removePicInput(obj){
	$(obj).parent().parent().parent().remove();
}

</script>

