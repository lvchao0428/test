	<script>
		function getBrz(obj, type){
			if (!obj.data("brzdata")) {
			    jQuery.getJSON("http://webbiz.xici.net/biz.asp?from=doc&sid=6B7F018FF4F3A7FB&tags=都市生活 商业 摩天 经济发展 规划 购物 苏州 城市论坛 昆山 情感 @792591@&clubid=1276&jsoncallback=?", function (data) {
			        obj.html(data.Addocsidebar).data("brzdata", data).show();
			        if (type == "another") {
			            jQuery("#tag_li_zt").show();
			            obj.next().hide();
			        } else {
			            if(jQuery("#Adygdoc")){
			                jQuery("#Adygdoc").html(data.Adygdoc);
			            }
			            jQuery("#Adygdoc1").html(data.Adygdoc1);
			            jQuery("#middoc").html(data.middoc);
			        }
