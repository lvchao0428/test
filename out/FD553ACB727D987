				strArr.push('<ul>');
				for (var i = 0; i < datas.length; i ++) {
					strArr.push('<li data-type="ga" node-type="百分点同类帖子推荐{$$}doc{$$}'+ bfd_key.clubmabe +'|'+ bfd_key.bdid + '|' + datas[i].name + '|' + datas[i].iid + '"> ・<a href="/' + datas[i].iid + '.htm" title="' + datas[i].name + '" target="_blank">' + datas[i].name + '</a></li>');
					if (i == 2) {
						strArr.push('</ul><ul>');
					}
				}
				strArr.push('</ul>');
