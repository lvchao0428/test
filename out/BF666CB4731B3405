function checksInput(key)
{
    if(document.getElementById("s").value == "" &&key==1)
    {
        document.getElementById("s").value = '请输入关键词';
    }

    if (document.getElementById("s").value == "请输入关键词")
    {
        return false;
    }
    if (key==3)
    {
        window.open("/s/?k=" + escape(document.getElementById("s").value));
        return false;
    }
    return true;
}
</script>
