	}catch (e){
	}
}


function mygetscript(url) {
		var s, c,  d = document, src = (url.indexOf("?") == "-1" ? url + "?" : url);
		(s = d.createElement("script")).setAttribute("type", "text/javascript");
		s.setAttribute("charset", "UTF-8");
		d.getElementsByTagName("head")[0].appendChild(s).src = src;
		
		s.onload = s.onreadystatechange = function(){ s.readyState ? s.readyState.toLowerCase() == 'loaded'  : '' };
	
}


jQuery(document).ready(function(){

		
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbsup_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_2&ybbs3=milclub_1_2_4");
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbsmid_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_2&ybbs3=milclub_1_2_4");
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbsdown_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_2&ybbs3=milclub_1_2_4");
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbsfirstfloor_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_2&ybbs3=milclub_1_2_4");
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbssecondfloor_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_2&ybbs3=milclub_1_2_4");
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbsthirdfloor_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_2&ybbs3=milclub_1_2_4");
});
