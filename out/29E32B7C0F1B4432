    <script type="text/javascript">
	var postminchars = parseInt('0');
	var postmaxchars = parseInt('20000');
	var disablepostctrl = parseInt('0');
	function validate(theform) {

		if (jQuery("#_login_status").val() != "1") {
			if (!processLogin()){
				return false;
			}
		}

		if (!checkNickname()){
			return false;
		}
		if (theform.message.value == "" && theform.subject.value == "") {
			alert("请完成标题或内容栏。");
			theform.message.focus();
			return false;
		} else if (theform.subject.value.length > 80) {
			alert("您的标题超过 80 个字符的限制。");
			theform.subject.focus();
			return false;
		}
			theform.subject.value = theform.subject.value+'		';//防止机器灌水做的校验 add by xiaobo
		if (!disablepostctrl && ((postminchars != 0 && theform.message.value.length < postminchars) || (postmaxchars != 0 && theform.message.value.length > postmaxchars))) {
			alert("您的帖子长度不符合要求。\n\n当前长度: "+theform.message.value.length+" 字节\n系统限制: "+postminchars+" 发送到 "+postmaxchars+" 字节");
			return false;
		}
		if(!fetchCheckbox('parseurloff')) {
			theform.message.value = parseurl(theform.message.value, 'bbcode');
		}
		theform.replysubmit.disabled = true;
		return true;
	}

	function chk_reply(frm)
	{
		if(typeof(frm.ajax_login_name) != 'undefined')
		{
			if(frm.ajax_login_name.value == '')
			{
				alert('请输入用户名');
				frm.ajax_login_name.focus();
				return false;
			}
			if(frm.ajax_login_password.value == '')
			{
				alert('请输入密码');
				frm.ajax_login_password.focus();
				return false;
			}
		}
		/*if(frm.subject.value == '')
		{
			alert('请输入标题');
			frm.subject.focus();
			return false;
		}*/
		if(frm.message.value == '')
		{
			alert('请输入内容');
			frm.message.focus();
			return false;
		}
		return true;
	}
	function frmsubmit()
	{
		var frmsubmit = 	document.getElementById("postform");
		if(chk_reply(frmsubmit))
		{
			frmsubmit.submit();
		}

	}


	</script>
