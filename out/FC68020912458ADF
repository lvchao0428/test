			}
			if(_src.indexOf("pan.xici.com") > 0){
				//sourceUrl=_src.split("/");
				//sourceUrl.pop();
				//sourceUrl=sourceUrl.join("/");
				//alert(_src);
				sourceUrl=_src;
			}
	                if (_src.indexOf("files.xici.net") > 0 || _src.indexOf("pan.xici.com") > 0) {
	                	jQuery(this).get(0).removeAttribute("width");
	                	jQuery(this).get(0).removeAttribute("height");
	                }
	                if ((_src.indexOf("files.xici.net") > 0 || _src.indexOf("pan.xici.com") > 0) && sourceUrl ) {
	                    _src = "/img.asp?src=" + sourceUrl;
	                    if ((jQuery(this).width() >= 300 || (jQuery(this).height() >= 300) && jQuery(this).width() >= 150)) {
	                        jQuery(this).attr("rel", "light_box").css("cursor", "pointer").attr('viewatuo', _src);
	                    }
	                }
	            });
