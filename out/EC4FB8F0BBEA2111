var doc = document;
function $id(s1, s2) { return s2 ? s1.getElementById(s2) : doc.getElementById(s1); }
function $name(s1, s2) { return s2 ? s1.getElementsByName(s2) : doc.getElementsByName(s1); }
function $tag(s1, s2) { return s2 ? s1.getElementsByTagName(s2) : doc.getElementsByTagName(s1); }

$(document).ready(function(){
	$('#more_btn').click(function(){
		if ($("title").overlay().isOpened()) {
			$("title").overlay().close();
		} else {
			$("title").overlay().load();
		}
	});
});

