<script language="javascript"> 
    function so() {
        if (document.getElementById("ajaxinput").value == "" || document.getElementById("ajaxinput").value == "输入您持仓股票代码，匹配解套策略") {
            alert("请输入策略名称或者关键字！");
            return false;
        }
        window.open("http://c.hexun.com/MatchingStock.aspx?from=guba&SearchStockCode=" + escape(document.getElementById('ajaxinput').value));
        return true
    }
    document.onkeydown = function (e) {
        if (!e) e = window.event; //火狐中是 window.event 
        if ((e.keyCode || e.which) == 13) {
            so();
        }
    } 
</script> 
