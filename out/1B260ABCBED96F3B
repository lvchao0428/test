		}
		function addListener(element,e,fn) {
			if(element.addEventListener){
				element.addEventListener(e,fn,false);
			} else {
				element.attachEvent("on" + e,fn);
			}
		}
		function onkeydownStatHook(e, typeNum) {
			var x = 0;
			var y = 0;
			var StatTag = "";
			var StatBlock = "";
			var StatRefType = "";
			var surl = "";
			var CSKey, typeName;
			var n = document.cookie.indexOf("SessionID");
			try {
				if(n == -1)
					{return;}
			}catch(e){return;}
			var IEorFF = window.document;//:e.currentTarget;
			var Url = "http://www.xici.net";
			CSKey = document.cookie.substr(n + 10, 16);
			switch (typeNum) {
				case 1:
					typeName = "移至发帖框";
					break;
				case 2:
					typeName = "翻页";
					break;
				case 3:
					typeName = "发跟帖";
					break;
			}
			try {
				var s = window.document.createElement('script');//:currentTarget.createElement('script');
				s.type = "text/javascript";
				s.src = "http://data.xici.net/c.js?tag=" + escape(StatTag) + ";block=" + escape(typeName) + ";x1=" + x + ";y1=" + y + ";id=" + CSKey + ";srcurl=" + Url + ";desurl=" + surl + ";atimes=" + lATimes + ";ct=" + lCityId + ";ch=" + lClubId + ";fr=" + strFirstRefer + ";referurl=;refType=;uid=" + luserid + ";rnd=" + (new Date()).getTime();
					IEorFF.getElementsByTagName("HEAD").item(0).appendChild(s);
			}catch(e){}
		}
		function showFace(id,event,add_top,add_left) {
			var src = event.srcElement || event.target;
			x = src.clientTop;
			y = src.clientLeft;
			var top = x + add_top + "px";
			var left = y + add_left + "px";
			document.getElementById(id).style.cssText = "top:" + top + ";left:" + left + ";";
			document.getElementById(id).style.display = 'block';
		}
		function onClass(i) {
			document.getElementById('idFace').className= 'face fa' + i;
			document.getElementById('idFaceList').style.display = 'none';
			document.getElementById('doc_type').value= i;
		}
