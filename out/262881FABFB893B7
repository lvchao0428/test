<script>
function ybbsup(data){
	try{
		if (/\.swf$/i.test(data.show) == false){
			jQuery("#ad_up").html('<a href="'+data.href+'" target="_blank"><img src="'+data.show+'" ></a>');
		}
		else{
			var objFlash = new sinaFlash(data.show, "", "585", "90", "7", "", false,"high");
			objFlash.addParam("wmode", "opaque");
			objFlash.write("ad_up");
		}
	}catch (e){
	}
}
function ybbsmid(data){
	try{
		if (/\.swf$/i.test(data.show) == false){
			jQuery("#ad_mid").html('<a href="'+data.href+'" target="_blank"><img src="'+data.show+'" width="300" height="31"></a>');
		}
		else{
			var objFlash = new sinaFlash(data.show, "", "300", "31", "7", "", false,"high");
			objFlash.addParam("wmode", "opaque");
			objFlash.write("ad_mid");
		}
	}catch (e){
	}
}
function ybbsdown(data){
	try{
		if (/\.swf$/i.test(data.show) == false){
			jQuery("#ad_down").html('<a href="'+data.href+'" target="_blank"><img src="'+data.show+'" width="950" height="90"></a>');
		}
		else{
			var objFlash = new sinaFlash(data.show, "", "950", "90", "7", "", false,"high");
			objFlash.addParam("wmode", "opaque");
			objFlash.write("ad_down");
		}
	}catch (e){
	}
}

function ybbsfirstfloor(data) {//{"show":"文子内容","href":"广告链接"}
	try{	
		jQuery("#ad_thread1_0").html('<a href="'+data.href+'" target="_blank">'+data.show+'</a>');
	}catch (e){
	}
}

function ybbssecondfloor(data) {
	try{
		jQuery("#ad_thread1_1").html('<a href="'+data.href+'" target="_blank">'+data.show+'</a>');
	}catch (e){
	}
}

function ybbsthirdfloor(data) {
	try{
		jQuery("#ad_thread1_2").html('<a href="'+data.href+'" target="_blank">'+data.show+'</a>');
	}catch (e){
	}
}


function mygetscript(url) {
		var s, c,  d = document, src = (url.indexOf("?") == "-1" ? url + "?" : url);
		(s = d.createElement("script")).setAttribute("type", "text/javascript");
		s.setAttribute("charset", "UTF-8");
		d.getElementsByTagName("head")[0].appendChild(s).src = src;
		
		s.onload = s.onreadystatechange = function(){ s.readyState ? s.readyState.toLowerCase() == 'loaded'  : '' };
	
}


jQuery(document).ready(function(){

		
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbsup_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_7");
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbsmid_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_7");
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbsdown_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_7");
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbsfirstfloor_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_7");
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbssecondfloor_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_7");
	mygetscript("http://dcads.sina.com.cn/html.ng/pos=ybbsthirdfloor_ads_call_back&site=sina&ybbsgid=milclub&ybbs1=milclub_1&ybbs2=milclub_1_7");
});
</script>
