	                	jQuery(this).get(0).removeAttribute("height");
	                }
	            	if ((jQuery(this).width() >= 300 || (jQuery(this).height() >= 300) && jQuery(this).width() >= 150)) {
	                    jQuery(this).css("cursor", "pointer")
	                    jQuery(this).click(function (){ bdAccessLogon(location.href.substring(location.host.length + 8),'_self');});
					}
	            });
	        }
	    }
