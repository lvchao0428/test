            if (username.length) {
                Wind.use('draggable', 'ajaxForm', function() {
                    var password = $('#xmGuestPrompt_password');
                    $("#xmGuestPrompt_form").ajaxForm({
                        dataType: 'json',
                        beforeSubmit: function(arr, $form, options) {
                            Wind.Util.ajaxBtnDisable(sidebar_login_btn);
                            $('#J_login_tips').remove();
                        },
                        success: function(data, statusText, xhr, $form) {
                            if (data.state === 'success') {
								//window.location.href = decodeURI(data.referer);
								window.location.href = data.referer.replace('%3A',':').replace(/\%2F/g,'/').replace("%3F",'?').replace("%3D",'=');
                            }else{
                                Wind.Util.ajaxBtnEnable(sidebar_login_btn);
                                $('<li id="J_login_tips" style="display:none;"><div class="tips"><div class="tips_icon_error">' + data.message + '</div></div></li>').appendTo($('#xmGuestPrompt_ul')).fadeIn(200).delay(5000).fadeOut();
                            }
                        }

                    });

                });

            }
        });

