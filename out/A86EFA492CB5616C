h) index += 2;        switch (index) {        case 1:            _Rw = Mw;            _Rw = Rh * Mw / Rw;        case 2:            _Rh = Mh;            _Rw = Rw * Mh / Rh;        case 3:            _Rh = (Rh / Mh > Rw / Mw) ? Mh: Rh * Mw / Rw;            _Rw = (Rh / Mh > Rw / Mw) ? Rw * Mh / Rh: Mw        }        if (index != 0) {            need_resize = true        }        return [_Rw, _Rh]    }    var wh = getRightWH(_image.width, _image.height, _Mw, _Mh);    o.style.width = wh[0] + 'px';    o.style.height = wh[1] + 'px';    o.setAttribute("width",wh[0]);    o.setAttribute("height",wh[1]);    o.style.visibility = 'visible';    if (need_margin == true) {        o.style.marginTop = (Mh - parseInt(wh[1])) / 2 + 'px'    }    _image = null;    return need_resize}/*ç­¾åæ¡£åŠ è½½å¤±è´¥å¤„ç†*/function sign_change_img(img){img.src='http://tb1.bdstatic.com/tb/static-itieba/img/sign_err.png';img.width=100;img.height=25;}</script><div id="head" class=" search_bright clearfix" style=""><div class="search_top clearfix" ><a title="åˆ°è´´å§é¦–é¡µ" href="/" class="search_logo"  style=""></a><div class="search_nav j_search_nav" style=""><a param="word" href="http://news.baidu.com/ns?cl=2&amp;rn=20&amp;tn=news&amp;">æ–°é—»</a><a param="wd" href=https://www.baidu.com/s?cl=3&amp; >ç½‘é¡µ</a><b>è´´å§</b><a param="word" href="http://zhidao.baidu.com/q?ct=17&amp;pn=0&amp;tn=ikaslist&amp;rn=10&amp;">çŸ¥é“</a><a param="key" href="http://music.baidu.com/search?fr=tieba&">éŸ³ä¹</a><a param="word" href="http://image.baidu.com/i?tn=baiduimage&amp;ct=201326592&amp;lm=-1&amp;cl=2&amp;">å›¾ç‰‡</a><a param="word" href="http://v.baidu.com/v?ct=301989888&amp;rn=20&amp;pn=0&amp;db=0&amp;s=21&amp;">è§†é¢‘</a><a param="word" href="http://map.baidu.com/m?fr=map006&amp;">åœ°å›¾</a><a href="http://baike.baidu.com/" param="searchword/?pic=1&fr=tieba&word" >ç™¾ç§‘</a><a href="http://wenku.baidu.com/search?fr=tieba&lm=0&od=0&" param="word" target="_blank">æ–‡åº“</a></div></div><div class="search_main_wra