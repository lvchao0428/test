if("undefined"==typeof addHandler)var addHandler=function(e,t,h){var v=['addEventListener','attachEvent'];if(e[v[0]]){e[v[0]](t,h,false);}else if(e[v[1]]){e[v[1]]('on'+t,h);}else{e['on'+t] = h;}};
var nva_menu = document.getElementById('nva_menu');
var cmd_bq_menu = document.getElementById('cmd_bq_menu');
var cmd_dq_a = document.getElementById('cmd_dq_a');
function whichElement(e){
var targ;
if(!e){
var e = window.event;
}
if(e.target){
targ = e.target;
}else if (e.srcElement){
targ = e.srcElement;
}
if (targ.nodeType == 3){ // defeat Safari bug
   targ = targ.parentNode;
}
var tname = targ.tagName;
if(targ != nva_menu && !nva_menu.contains(targ)){
if(document.getElementById('nva_menu').className == 'mbbs fr hover'){
if(tname)document.getElementById('listmenu_menu').click()
}
}
if(cmd_dq_a && cmd_dq_a != targ && !cmd_bq_menu.contains(targ)){
        if(document.getElementById('xim_cgiclosed')){
            document.getElementById('xim_cgiclosed').click()
        }
    }
}
if(typeof (HTMLElement) != "undefined"){
    HTMLElement.prototype.contains = function (o) {
return this.compareDocumentPosition(o) == 20 ? true : false;
}
}
addHandler(document,'click',function(event){
whichElement(event);
})
