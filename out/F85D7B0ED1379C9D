    //对楼层点赞，显示效果，数据库更新
    $(".praise_tijiao").bind('click',function(){
        var praise_text=$(this).text();
        if(praise_text.indexOf('已') <0){
            var mythis = $(this);
            var pid = $(this).attr('pid');
            var zan_data={tid:21872849,fid:91,pid:pid,action:'zan'};
            $.ajax({
                type : 'POST',
                url : '/bbs/ajax.php',
                data : zan_data,
                success: function(data){
                    if(data==2){

                        var praise_text=mythis.text();

                        if(praise_text.indexOf('(') >=0){
                            var zan_num=/\((.*)\)/.exec(praise_text);
                            zan_num=parseInt(zan_num[1])+1;
                            praise_text='已赞'+'('+zan_num+')';
                        }else{

                            praise_text='已赞(1)';

                        }
                        /*var praise_text='已赞';*/
                        mythis.text(praise_text);

                    }
                }

            });
        }

    })

