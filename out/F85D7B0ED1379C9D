        var scroll_wall = setInterval(function(){
            $("#as2013_bulletin").find("li:first").animate({marginTop:"-24px"},1E3,function(){$(this).css({marginTop:"0px"}).appendTo("#as2013_bulletin")})
        },4E3)
        $('#as2013_bulletin').hover(function(){if(typeof(scroll_wall)!='undefined')clearInterval(scroll_wall)},function(){
            scroll_wall = setInterval(function(){
                $("#as2013_bulletin").find("li:first").animate({marginTop:"-24px"},1E3,function(){$(this).css({marginTop:"0px"}).appendTo("#as2013_bulletin")})
            },4E3)
        });

        //登录后点击收藏(老)
        $("#tpost_fav1,#tpost_fav2").click(function(){
            var tid = '21872849';
            var formhash='a7a068f6';

            $.ajax({
                type:'GET',
                url:'/bbs/my.php',
                data:{item:'favorites',tid:tid,formhash:formhash,is_ajax:1,zoneclick:100690},
                dataType:'json',
                success:function(data){
                    $("#favorite_msg").find('div').html(data.msg);
                    $("#favorite_msg").show();
                    setTimeout(function(){
                        $('#favorite_msg').hide();
                    },3000);
                }
            });
        });

        //登录后点击收藏（新）
        $("#tpost_fav3").click(function(){
            var tid = '21872849';
            var formhash='a7a068f6';

            $.ajax({
                type:'GET',
                url:'/bbs/my.php',
                data:{item:'favorites',tid:tid,style:2,formhash:formhash,is_ajax:1,zoneclick:100690},
                dataType:'json',
                success:function(data){
                    if(data==1){
                        //收藏成功
                        document.getElementById("tpost_fav3").style.display="none";
                        $('#tpost_fav4').show();
                    }
                }
            });
        });
