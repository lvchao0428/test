        //获取楼层积分相应的数据
        $('.xcarcoin_tb').click(function(){
            var tb_this = $(this);
            if($(this).parent().find('.xcarcoin_list').children().length<=0){
                var pid = $(this).parent().find('.rate_id').val();
                var rate = tb_this.attr('data-src');
                var rate_data={tid:21872849,pid:pid,action:'viewratings',rate:rate};
                $.ajax({
                    type : 'POST',
                    url : '/bbs/ajax.php',
                    dataType: 'json',
                    data :rate_data,
                    success: function(data){
                        tb_this.parent().find('.xcarcoin_list').empty();
                        tb_this.parent().find('.xcarcoin_top').empty();
                        if(data.number >= 3){
                            tb_this.parent().find('.xcarcoin_list').removeClass('xcarcoin_min');
                        }
                        if(data.fix){
                            //tb_this.text('爱卡币'+data.fix);
                        }
                        var str='<table border="0" cellspacing="0" cellpadding="0">';
                        var _onlineip = false;
                        $.each(data.log, function(i,item) {
                            if(item.reason_min==''){
                                item.reason_min="&nbsp;";
                            }
                            str +='<tr>';
                            if(typeof item.onlineip != 'undefined'){
                                _onlineip = true;
                                str += '<td width="14"><input style="visibility:hidden;" type="checkbox"></td>';
                            }
                            str += '<td width="38"><a target="_blank" href="http://my.xcar.com.cn/space.php?uid='+item.uid+'" class="xcarcoin_img"><img width="30" height="30" src="'+item.avatar+'" onerror="this.onerror=null;this.src=\'http://image1.xcarimg.com/attachments/my/default/120.jpg?v=1.1\'"></a></td>';
                            str += '<td width="102"><a target="_blank" href="http://my.xcar.com.cn/space.php?uid='+item.uid+'">'+item.username+'</a></td>';
                            str += '<td width="116">'+item.dateline+'</td>';
                            str += '<td width="72" title="'+item.reason+'">'+item.reason_min+'</td>';
                            str += '<td width="50">'+item.score+'</td>';
                            if(_onlineip){
                                str += '<td>'+item.onlineip+'</td>';
                            }
                            str += '</tr>';
                        });
                        str +='</table>';

                        tb_this.parent().find('.xcarcoin_list').append(str);
                        var str_2 = '';
                        if(_onlineip){
                            str_2 += '<tr>';
                            str_2 += '<td width="1"></td>';
                            str_2 += '<td width="130">已有<i>'+data.number+'</i>人评分</td>';
                            str_2 += '<td width="103">时间</td>';
                            str_2 += '<td width="56">理由</td>';
                            str_2 += '<td width="40">爱卡币</td>';
                            str_2 += '<td align="center">IP</td>';
                            str_2 += '</tr>';
                        }else{
                            $('.xcarcoin_dl').removeClass('xcarcoin_table');
                            str_2 += '<tr><td width="35%">已有<i>'+data.number+'</i>人评分</td><td width="29%">时间</td><td width="18%">理由</td><td width="18%">爱卡币</td></tr>';
                        }
                        tb_this.parent().find('.xcarcoin_top').append(str_2);
                        /* $('.xcarcoin_all').children('i').text(data.sum);*/
                        var aika='';
                        if(data.zhengfu==1){
                            aika='+';
                            tb_this.parent().find('.xcarcoin_dl').children('.xcarcoin_all').children('i').text(aika+data.sum);
                        }else{
                            tb_this.parent().find('.xcarcoin_dl').children('.xcarcoin_all').children('i').text(data.sum);
                        }
                    }
                })
            }
        });
        //从xcarcoin_dl移出 关闭弹框
        $('.xcarcoin_dl').mouseleave(function(){
            $(this).siblings('.xcarcoin_tb').trigger("click");
        })

        //爱卡币
        $('.xcarcoin_tb').click(function(){
            if($(this).parent().hasClass('xcarcoin_open')){
                $(this).parent().removeClass('xcarcoin_open');
            }else{
                $(this).parent().addClass('xcarcoin_open');
            }
        })

        $('.xcarcoin_list tr').live('mouseover',function(){
            $(this).css('background-color','#ffe4c4')
        }).live('mouseleave',function(){
            $(this).css('background','none')
        });
        var search;
        $(".selectInput").bind('keydown',function(event){
            //(event.keyCode)
            if(event.keyCode==13){
                $('.option').children().hide();
                $('.option').hide();
                $('.checktype').html('');
            }else if (event.keyCode!=38&&event.keyCode!=40){
                $('.option').children().show();
            if(search)clearTimeout(search);

             search = setTimeout(function(){
                var word = $.trim($('.selectInput').val());
                if(word == ''){
                    $('.checktype').html('');
                    return false;
                }
                $.getJSON("http://tools.xcar.com.cn/suggest/suggestutf8.php?k="+word+"&callback=?",
                        function(data){
                            if(data){
                                var str = '';
                                for(var s in data){
                                    str += '<li><a class="serach_word" href="javascript:;" onclick="clickP(\'124\',\'2\',\'\');">'+data[s]+'</a></li>';
                                }
                                if(str){
                                    $('.checktype').html(str).parent('div').show();
                                    $('.checktype > li').bind('click',function(){
                                        $('.selectInput').val($(this).find('a').html());
                                        $(this).parents("div[class='option']").hide();
                                    });
                                }
                            }
                        }
                );
            },300);
            }
        });
        $(".selectInput").bind('focus',function(){
            var word = $.trim($(this).val());
            if(word == '请输入关键字'){
                $(this).val('');
            }
            if($('.checktype').find('li').length > 0){
                $('.checktype').parent("div[class='option']").show();
            }
        });

        $('.selectInput').keyup(function(){
            if($(this).val()==''){
                $('.option').hide();
            }

        })

        $(".selectInput").bind('blur',function(){
            var word = $.trim($(this).val());
            if(word == ''){
                $(this).val('请输入关键字');

            }

        });
        $(".selectInput").blur();
        $('.serach_word').live('click',function(){
            var k_val=$("input[name='k']").val();
            if(k_val!=''){
                // location.href ='http://search.xcar.com.cn/search.php?k='+k_val+'&c=5&ie=gbk';
                $('.newcar_btn').parent('form').submit();
            }

        })

        $('.newcar_btn').bind('click',function(){
            var word = $.trim($('.selectInput').val());
            if(word=='请输入关键字'){
                word='';
                $('.selectInput').val('');
            }
            $(this).parent('form').submit();
        });
        var is_submit = false;
        
        
        $('#quick_reply').bind('click',function(){
            $('#ft_error').html('亲,请登录了再评论').show();
            setTimeout(function(){$('#ft_error').hide();},1500);
            return;
        });
        
        
        var is_praise = false;
        $('#ft_praise').bind('click',function(){
            if(is_praise) return false;
            
            $('#ft_error').html('亲,你还没登录哦').show();
            setTimeout(function(){$('#ft_error').hide();},1500);
            return;
            
            var referer = document.URL.replace(/(\#[\s\S]+)/,'');
            var	data_str = {tid:21872849,fid:91,action:'praise',mt:Math.random(),formhash:'a7a068f6',referer:referer};
            $('#last_praise').hide();
            is_praise = true;
            $.ajax({
                type : 'POST',
                url  : '/bbs/ajax.php',
                data : data_str,
                dataType: 'json',
                success: function(data){
                    var stat = parseInt(data.stat);
                    switch (stat){
                        case 0:
                            $('#ft_praise').css({"cursor":"default","text-decoration":"none"});
                            $('#ft_praise').attr('class','favorites_has t_praiseed').html('已赞（'+parseInt(data.num)+'）<i style=\'display:none;\' class=\'plus plus_one\'>+1</i>');
                            $(".ft_praise i").show();
                            setTimeout(function(){$(".ft_praise i").hide()},1000);
                            if(typeof data.data){
                                var str = '';
                                var d = data.data;
                                for(k in d){
                                    str += '<a href="http://my.xcar.com.cn/space.php?uid='+d[k].uid+'" target="_blank">'+d[k].username+'</a>';
                                }
                                $('#last_praise').find('dd').html(str);
                                setTimeout(function(){$('#last_praise').show()},1000);
                                setTimeout(function(){$('#last_praise').hide()},3000);
                            }
                            $('#ft_praise').unbind('click');
                            break;
                        case -1:
                            alert('网络异常,请稍后操作.');
                            break;
                        case 2:
                            alert('已赞过');
                            break;
                        case -2:
                            $('#ft_error').html('该主题不存在!').show();
                            setTimeout(function(){$('#ft_error').hide();},1500);
                            break;
                    }
                    is_praise = false;
                }
            });
            return false;
        });
        
        $('#ft_close').bind('click',function(){
            clickP('148','2','');
            $('#ft_bottom_coat').hide();
        });
        var fast_state = false;
        $('#fast_message').bind('focus',function(){
            if($.trim($(this).val()) == '亲，说两句呗! 按Enter键发送回复'){
                $(this).val('');
            }
            $(this).css('color','#444');
            
            $('.ft_phiz').hide();
            fast_state = true;
        });
        $('#fast_message').bind('blur',function(){
            if($.trim($(this).val()) == ''){
                $(this).val('亲，说两句呗! 按Enter键发送回复');
                $(this).css('color','#B5B5B5');
            }
            fast_state = false;
        });
        $('.verify_txt').bind('focus',function(){
            if($.trim($(this).val()) == '输入验证码'){
                $(this).val('');
            }
            fast_state = true;
        });
        $('.verify_txt').bind('blur',function(){
            if($.trim($(this).val()) == ''){
                $(this).val('输入验证码');
            }
            fast_state = false;
        });

        var praise;
        $('#ft_praise').bind('mouseover',function(){
            praise && clearTimeout(praise);
            $('#last_praise').find('a').length && $('#last_praise').show();
        });
        $('#ft_praise').bind('mouseout',function(){
            praise && clearTimeout(praise);
            praise = setTimeout(function(){
                $('#last_praise').hide();
            },300);
        });
        $('#last_praise').bind('mouseover',function(){
            praise && clearTimeout(praise);
            $(this).show();
        });
        $('#last_praise').bind('mouseout',function(){
            praise && clearTimeout(praise);
            praise = setTimeout(function(){
                $('#last_praise').hide();
            },300);
        });
        $('.fast_m').bind('click',function(){
            if(is_submit)return false;
            var id = $(this).find('img').attr('id').replace('fast_m_','');
            id = parseInt(id);
            if(id > 0){
                setTimeout(function(){
                    /*解决ft_phiz display table属性 bug原因为找到*/
                    $(".ft_search_on").removeClass("ft_search_on").addClass("ft_search");
                    $(".ft_phiz").hide();
                },300);
                var	data_str = {tid:21872849,fid:91,action:'reply',mt:Math.random(),land:'lord',fast_m:id,formhash:'a7a068f6',usesig:1,ssid:1450676972,replysubmit:'yes'};
                is_submit = true;
                $('#quick_reply').html('发布中..');
                $.ajax({
                    type : 'POST',
                    url  : '/bbs/post.php',
                    data : data_str,
                    success: function(data){
                        if(data){
                            data = $.trim(data);
                            if(0 !== data.indexOf('{"stat"')){
                                alert('请稍后回复');
                                return false;
                            }
                            data = eval('('+data+')');
                            var stat = parseInt(data.stat);
                            switch(stat){
                                case 1:
                                    var day_hot_news = $("td[id^='day_hot_news_']").eq(0).html();
                                    var new_floor_div = get_temp_floor_str(data.pid,data.user_avatar,data.new_message,data.page,data.floor_no,day_hot_news);

                                    $("#delpost").append(new_floor_div);

                                    clickP('99','1','');

                                    //成功提示条
                                    $('#ft_win_success').show();
                                    setTimeout(function(){
                                        $('#ft_win_success').hide();
                                        is_submit = false;
                                    },1500);

                                    //定位
                                    var scroll_offset = $("#new_quote_div_"+data.pid).offset();
                                    $("body,html").animate({
                                        scrollTop:(scroll_offset.top - 130) //让body的scrollTop等于pos的top，就实现了滚动
                                    },500);

                                    break;
                                case 3:
                                    $('#ft_error').html('请输入正确的验证码').show();
                                    setTimeout(function(){$('#ft_error').hide();},1500);
                                    break;
                                case 4:
                                    $('#ft_error').show();
                                    $('#ft_win').hide();
                                    setTimeout(function(){
                                        $('#ft_error').hide();
                                        is_submit = false;
                                    },3000);
                                    break;
                                case 10:
                                    $('#ft_error').html('账户尚未激活').show();
                                    $('#ft_win').hide();
                                    setTimeout(function(){
                                        $('#ft_error').hide();
                                        is_submit = false;
                                    },3000);
                                    break;
                                default:
                                    alert('请正确访问');
                            }
                        }else{
                            alert(data);
                        }
                        $('#quick_reply').html('快速回复');
                    }
                });
            }
        });

        /*微信分享*/
        var strs=[];
        var strs=location.href.split("&page");
        var share_href=encodeURIComponent(strs[0]);
        var share_iurl='http://hezuo.xcar.com.cn/index.php?partner=qrcode&url='+share_href;
        var share_url='http://hezuo.xcar.com.cn/index.php?partner=weixin_share&url='+share_href+'&title='+encodeURIComponent(document.title);
        var wxin="";
        $("#share_weixin").hover(
            function(){
                if(wxin)clearTimeout(wxin);
                //if($("#w_new").css("display")=="none"){
                $("#w_new").find("img").attr("src",share_iurl);
