<script>
var doc = document;
var curPage = new Array();
curPage['d4s'] = 2;
curPage['dzh'] = 2;
var gpage;
var czh = "14";
var c4s = "13";
function chClass(flag){
	var show = "div_"+flag;
	if(flag == 'dzh'){
		var blurid = "d4s";
		var hide = "div_d4s";
	}else{
		var blurid = "dzh";
		var hide = "div_dzh"; 
	}
	if(doc.getElementById(show) && doc.getElementById(hide)){
		doc.getElementById(flag).className='dealer_complex';
		doc.getElementById(blurid).className='';
		doc.getElementById(show).style.display='block';
		doc.getElementById(hide).style.display='none';
	}
}

function cleanStyle(flag){
	var i;
	p = (flag == 'd4s')? 'd4s':'dzh';
	var count = (flag == 'd4s')? c4s:czh;
		for(i = 1; i <= count;i++){
			doc.getElementById('ul_'+i+'_'+flag).style.display='none';
		}
	}
	
function nextPage(){
	if(doc.getElementById("d4s") && doc.getElementById("d4s").className == 'dealer_complex'){
		var flag = 'd4s';
		var count = c4s;
	}else{
		var flag = 'dzh';
		var count = czh;
	}

	if(curPage[flag] > count){
		curPage[flag] = 1;
	}
	cleanStyle(flag);
	doc.getElementById("page_"+flag).innerHTML=curPage[flag];
	doc.getElementById("ul_"+curPage[flag]+"_"+flag).style.display='block';
	curPage[flag]++;
}

function autobro(){
	var d4s = doc.getElementById('d4s');
	if(!d4s){
		doc.getElementById('dzh').className = 'dealer_complex';
	}
	gpage = setInterval("nextPage()",10000);
}
autobro();
(function($){
	$(".d_p_left").click(function(){
		var flag = $(this).attr("did");
		var page = $(this).siblings('.d_p_center').children('em').text();
		var maxp = c4s;
		if(flag == 'dzh')maxp = czh;
		page = parseInt(page);
		if(isNaN(page)){page=1;}else{page = page -1;}
		page = page>maxp ? maxp : page;
		page = page<1 ? 1 : page;
		if($(".ul_"+flag) != 'undefined'){
			if($("#ul_"+page+"_"+flag) == 'undefined')return false;
			$(".ul_"+flag).not().hide();
			$("#ul_"+page+"_"+flag).show();
			$(this).siblings('.d_p_center').children('em').text(page);
			curPage[flag] = page;
		}			
	});
	
	$(".d_p_right").click(function(){
		var flag = $(this).attr("did");
		var page = $(this).siblings('.d_p_center').children('em').text();
		var maxp = c4s;
		if(flag == 'dzh')maxp = czh;
		page = parseInt(page);
		if(isNaN(page)){page=1;}else{page = page+1;}
		page = page>maxp ? maxp : page;
		page = page<1 ? 1 : page;
		if($(".ul_"+flag) != 'undefined'){
			if($("#ul_"+page+"_"+flag) == 'undefined')return false;
			$(".ul_"+flag).not().hide();
			$("#ul_"+page+"_"+flag).show();
			$(this).siblings('.d_p_center').children('em').text(page);
			curPage[flag] = page;
		}			
	});
	
})(jQuery);
</script>
