            function favorite(){
                var event = window.event;
                $('#ajax_favorite').unbind('click');
                setTimeout(function(){
                    ajaxmenu(event,'ajax_favorite');
                    return false;
                },1500);
                setTimeout(function(){
                    window.location.reload();
                    return false;
                },3000);
                return false;
            }
