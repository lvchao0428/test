    var praises = [];
    $('.make_info').bind('mouseover',function(){
        var pid = $(this).find('.praise_tijiao').attr('pid');
        if(praises[pid]){
            return false;
        }
        praises[pid] = true;
        var mythis = $(this);
        var my_zan_data={pid:pid,action:'my_zan'};
        $.ajax({
            type : 'POST',
            url  : '/bbs/ajax.php',
            data :  my_zan_data,
            success: function(data){
                var praise_text = mythis.find('.praise_tijiao').text();
                if(data == 1){
                    praise_text = '已' + praise_text;
                    mythis.find('.praise_tijiao').text(praise_text);
                }
            }
        });
    });

