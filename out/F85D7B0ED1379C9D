    var postminchars = parseInt('1');
    var postmaxchars = parseInt('100000');

    //点击提交快速回复
    var is_submit_new = false;
    $('.sb_reply').live('click',function(){
        window.console && console.log('xx');
        if(is_submit_new) return;

        var pid = $(this).attr('pid');
        var post_author_id = $(this).attr('post_author_id');

        var message = $("#reply_message_"+pid).val();
        if(message){
            message = $.trim(message);
            message = message == default_message ? '' : message;
        }

        var warning = '';

        if(message == '' || message == default_message || ((postminchars != 0 && mb_strlen(message) < postminchars) || (postmaxchars != 0 && mb_strlen(message) > postmaxchars))) {
            alert("您的帖子长度不符合要求。\n\n当前长度: "+mb_strlen(message)+" 字节\n系统限制: "+postminchars+" 到 "+postmaxchars+" 字节");
            $("#reply_message_"+pid).focus();
            return false;
        }
        
         message = bbinsert ? parseurl(message,'bbcode') : message;//handong new add  
        
        var	data_str = {tid:21872849,fid:91,action:'reply',mt:Math.random(),land:'lord',message:encodeURIComponent(message),formhash:'a7a068f6',usesig:1,ssid:1450676972,repquote:pid,replysubmit:'yes',repquote_authorid:post_author_id};
        
        //回复贴的每日热点新闻
        var day_hot_news = $("#day_hot_news_"+pid).html();

        is_submit_new = true;

        $('#sb_reply_'+pid).html('发布中...');
        $.ajax({
            type : 'POST',
            url : '/bbs/post.php',
            data : data_str,
            success: function(data){
                $('#sb_reply_'+pid).html('马上回复');
                is_submit_new = false;
                if(data){
                    data = $.trim(data);
                    if(0 !== data.indexOf('{"stat"')){
                        $('#error_msg_'+pid).html('请稍后回复').show();
                        setTimeout(function(){
                            $('#error_msg_'+pid).hide();
                        },1500);
                        return false;
                    }
                    data = eval('('+data+')');
                    var stat = parseInt(data.stat);
                    switch(stat){
                        case 1:
                            $('#sb_reply_'+pid).unbind('click');

                            $("#reply_message_"+pid).val('');
                            $("#reply_tr_"+pid).find(".replay_post_con").hide();

                            var new_quote_div = get_temp_floor_str(data.pid,data.user_avatar,data.new_quote_message,data.page,data.floor_no,day_hot_news);

                            $("#table_"+pid).parent().after(new_quote_div);

                            //定位
                            var scroll_offset = $("#new_quote_div_"+data.pid).offset();
                            $("body,html").animate({
                                scrollTop:(scroll_offset.top - 50) //让body的scrollTop等于pos的top，就实现了滚动
                            },500);

                            //将最下端的回复验证码更新，否则提示验证码过期
                            
                            //回复完成弹出框
                            var successDiv = $("#ft_win_success");
                            successDiv.fadeIn();
                            setTimeout(function(){
                                successDiv.fadeOut();
                            },2000)
                            break;
                        case 3:
                            $('#error_msg_'+pid).html(data.msg).show();
                            setTimeout(function(){$('#error_msg_'+pid).hide();},1500);
                            break;
                        case 4:
                            $('#error_msg_'+pid).html('回复太频繁，休息一会儿').show();
                            
                            setTimeout(function(){
                                $('#error_msg_'+pid).hide();
                            },3000);
                            break;
                        default:
                            $('#error_msg_'+pid).html(data.msg).show();
                            setTimeout(function(){$('#error_msg_'+pid).hide();},1500);
                    }
                }else{
                    $('#error_msg_'+pid).html('请稍后回复!').show();
                    setTimeout(function(){$('#error_msg_'+pid).hide();},1500);
                    return false;
                }
            }
        });
        return false;
    });
