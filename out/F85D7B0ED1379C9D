//getScript('http://search.xcar.com.cn/related.php?tid='+21872849+'&out=json&rand='+Math.random());
if(typeof(pushads)=='undefined'){
    var pushads=function(data,pid){
        var doc = document;
        if(!doc.getElementById('pushAd_'+pid))return;
        if(data == '' || !pid){
            if(doc.getElementById('pushAd_'+pid))doc.getElementById('pushAd_'+pid).style.display='none';
            return;
        }
        switch(pid){
            case 99:
                doc.getElementById('pushAd_99').innerHTML += data;
                break;
            case 112:
                doc.getElementById('pushAd_112').innerHTML = data;
                var a = doc.getElementById('pushAd_112').getElementsByTagName('a')[0];
                a.onmouseover = function(){
                    this.style.textDecoration = 'underline';
                }
                a.onmouseout = function(){
                    this.style.textDecoration = 'none';
                }
                break;
            default:
                doc.getElementById('pushAd_'+pid).innerHTML = data;
                break;
        }
    }
}
/*getScript('http://d0.xcar.com.cn/d0/push_bbs_ad1.php?pid=101&fid='+91);*/
function insertEle(a){
    var doc = document;
    var oTest = doc.getElementById(a);
    if(!doc.getElementById(a).getElementsByTagName('iframe')[0]){
        var newNode = doc.createElement("iframe");
        newNode.setAttribute('scrolling',0);
        newNode.setAttribute('frameborder',0);
        var reforeNode = doc.getElementById(a).getElementsByTagName('ul')[0];
        oTest.insertBefore(newNode,reforeNode);
    }
    oTest.style.display="block";
}
function goToFloor(id){var floor=jQuery('#'+id).val();floor=jQuery.trim(floor);var r=floor.match(/([^\d]+)/i);if(r||floor>2+1||!floor){alert('此帖还没有回复到您输入的楼层\n请重新输入您想去的楼层!');return;}
    jQuery.getJSON('xunzhao/gotofloor.php?tid='+21872849+'&_floor='+floor+'&callback=?',function(json){jQuery.each(json,function(i,item){location.href=item;});});}
