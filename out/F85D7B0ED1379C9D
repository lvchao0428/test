    //翻页提示语 
    function fanyebox(txt,url){
        if(txt=='')return false;
        var successDiv = $("#fanye");
        if(successDiv){
            successDiv.html(txt);
            successDiv.fadeIn('fast',function(){
                if(url!=''){
                    window.location.href = url;
                }
                //successDiv.fadeOut('fast');
            });
            successDiv.fadeOut(2000);
        }
        return false;
    }
