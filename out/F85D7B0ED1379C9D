window.floating_window_positioning = function(id){
        if($('#'+id).is(':visible')){
            $('#'+id).css({
                position:'absolute',
                left: ($(window).width() - $('#'+id).outerWidth())/2,
                top: ($(window).height() - $('#'+id).outerHeight())/2 + $(document).scrollTop()
            });
        }
    }
