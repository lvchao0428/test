<script>
	;(function($){
		var timer=null;
		$('.lxh_bbs_hot_content a:not(.lxh_bbs_search_new)').each(function(){
			var that=this;
			$(this).hover(function(){
				timer&&clearTimeout(timer);
				timer=setTimeout(function(){
					var h_em_num = $(that).find("em").height();
					if(h_em_num==20){
						$(that).find('span.lxh_optity_bg').stop().animate({height:63},500);
					}else if(h_em_num==40){
						$(that).find('span.lxh_optity_bg').stop().animate({height:83},500);
					}
					
					$(that).find('span.lxh_essence_num').css('display','block');
				},300)
				
			},function(){
				timer&&clearTimeout(timer);
				$(this).find('span.lxh_optity_bg').animate({height:33},1000);				
				$(this).find('span.lxh_essence_num').css('display','none');
			});
		})	
    })(jQuery);

</script>

