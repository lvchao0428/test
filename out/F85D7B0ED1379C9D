.btn0624{display:inline-block; width:120px; height:30px; background:url(http://icon.xcar.com.cn/sq2013/Authenticate/btn20130624.jpg) left top no-repeat;}
-->

/* 认证商家 */
.sq_Authenticate_btn01 .popup dl em{ float:left; font-style:normal;}
.sq_Authenticate_btn01 .popup dl .bus_name{ float:left;color:#333;}
.sq_Authenticate_btn01 .popup dl .bus_img{ padding-left:10px;}
