	<script>
		function getBrz(obj, type){
			if (!obj.data("brzdata")) {
			    jQuery.getJSON("http://webbiz.xici.net/biz.asp?from=doc&sid=831A52CB0D90157B&tags=公益 人文 社会时评 思想 时事 历史 谈天说地 宗教 生活 社会 @15420@&clubid=1208&jsoncallback=?", function (data) {
			        obj.html(data.Addocsidebar).data("brzdata", data).show();
			        if (type == "another") {
			            jQuery("#tag_li_zt").show();
			            obj.next().hide();
			        } else {
			            if(jQuery("#Adygdoc")){
			                jQuery("#Adygdoc").html(data.Adygdoc);
			            }
			            jQuery("#Adygdoc1").html(data.Adygdoc1);
			            jQuery("#middoc").html(data.middoc);
			        }
			        obj.find("li").each(function () {
			            jQuery(this).addClass(jQuery(this).index() % 2 == 0 ? 'fl' : 'fr');
			        });
			    });
			}
		}

		function light_box() {
	    	//登陆后才给看幻灯片，o(∩_∩)o
	    	if (G_UID > 0) {
	            jQuery('div[data-type="main_topic"], div[id^="floor"]').find('img').each(function (i){
	                var _src = jQuery(this).attr("src");
			var sourceUrl;
			if(_src.indexOf("files.xici.net") > 0){
				sourceUrl=_src.split("?")[0];
			}
			if(_src.indexOf("pan.xici.com") > 0){
				//sourceUrl=_src.split("/");
				//sourceUrl.pop();
				//sourceUrl=sourceUrl.join("/");
				//alert(_src);
				sourceUrl=_src;
			}
	                if (_src.indexOf("files.xici.net") > 0 || _src.indexOf("pan.xici.com") > 0) {
	                	jQuery(this).get(0).removeAttribute("width");
	                	jQuery(this).get(0).removeAttribute("height");
	                }
	                if ((_src.indexOf("files.xici.net") > 0 || _src.indexOf("pan.xici.com") > 0) && sourceUrl ) {
	                    _src = "/img.asp?src=" + sourceUrl;
	                    if ((jQuery(this).width() >= 300 || (jQuery(this).height() >= 300) && jQuery(this).width() >= 150)) {
	                        jQuery(this).attr("rel", "light_box").css("cursor", "pointer").attr('viewatuo', _src);
	                    }
	                }
	            });
	            jQuery("img[rel=light_box]").fancybox({
	                    'padding'			: 10,
	                    'paddingtop'		: 30,
	                    'preventDefault'	: false,
	                    'changeFade'		: 0,
	                    'changeSpeed'		: 0,
	                    'cyclic'			: true,
	                    'autoScale'			: true,
	                    'centerOnScroll'	: true
	            });
	        } else {
	        	jQuery('div[data-type="main_topic"], div[id^="floor"]').find('img').each(function (){
	        		var _src = jQuery(this).attr("src");
					if (_src.indexOf("files.xici.net") > 0) {
	                	jQuery(this).get(0).removeAttribute("width");
	                	jQuery(this).get(0).removeAttribute("height");
	                }
	            	if ((jQuery(this).width() >= 300 || (jQuery(this).height() >= 300) && jQuery(this).width() >= 150)) {
	                    jQuery(this).css("cursor", "pointer")
	                    jQuery(this).click(function (){ bdAccessLogon(location.href.substring(location.host.length + 8),'_self');});
					}
	            });
	        }
	    }
		window.onload = function (){
	    	light_box();
	    };

		jQuery(function (){
	    	light_box();
	    	getBrz(jQuery(".zt_numlist").eq(1), 'default');
			jQuery("a[data-type='view_author']").click(function (){
				if(G_UID == 0) {
					bdAccessLogon(location.href.substring(location.host.length + 8),'_self');
					return false;
				}
			})
			jQuery.fn.sidebarFixed = function (options){
				var _default = {
					className:"sidebar-fixed"
				};
				var _opts = jQuery.extend({},_default,options);
				//取窗口滚动条高度
				var getScrollTop = function() {
					var scrollTop = 0;
					if (document.documentElement && document.documentElement.scrollTop) {
						scrollTop = document.documentElement.scrollTop;
					} else if (document.body) {
						scrollTop = document.body.scrollTop;
					}
					return scrollTop;
				}
				// 取窗口可视范围的高度
				var getClientHeight = function() {
					var clientHeight = 0;
					if (document.body.clientHeight && document.documentElement.clientHeight) {
						clientHeight = (document.body.clientHeight < document.documentElement.clientHeight) ? document.body.clientHeight : document.documentElement.clientHeight;
					} else {
						clientHeight = (document.body.clientHeight > document.documentElement.clientHeight) ? document.body.clientHeight : document.documentElement.clientHeight;
					}
					return clientHeight;
				}
				// 取文档内容实际高度
				var getScrollHeight = function() {
					return Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);
				}
				return jQuery(this).each(function() {
					var fixedDiv = jQuery(this);
					var H = 0;
					var Y = fixedDiv.get(0);
					while (Y) { H += Y.offsetTop;Y = Y.offsetParent};
					jQuery(window).scroll(function (){
						var s = document.body.scrollTop || document.documentElement.scrollTop;
						s > H ? fixedDiv.removeClass("sidebar-fixed-bottom").addClass(_opts.className) : fixedDiv.removeClass(_opts.className);
						if ( getScrollHeight()  == (getScrollTop() + getClientHeight()) ) {
							fixedDiv.removeClass(_opts.className).addClass("sidebar-fixed-bottom");
							getBrz(jQuery(".zt_numlist").eq(0), 'another');
						}
						if (getScrollTop() < 100) {
							fixedDiv.removeClass(_opts.className).removeClass("sidebar-fixed-bottom");
						}
					});
				});
			}
			jQuery("#sidebar-bd").sidebarFixed();
			jQuery.fn.tagsCloud = function () {
				var self = this;
				this.rand = function (num) {
					return parseInt(Math.random() * num + 1)
				};
				this.randomcolor = function (){
					var str = Math.ceil(Math.random() * 16777215).toString(16);
					str.length < 6 ? str = "0" + str : "";
					return str;
				};
				return jQuery(this).each(function() {
					jQuery(this).css({
						"color":"#" + self.randomcolor(),
						"fontSize": self.rand(3) + 11 + "px"
					})
				});
			};
			jQuery(".moretags a").tagsCloud();
		});

	//biz切换显示
	jQuery("#tag_li_zt li").mouseover(function () {
		var n = jQuery(this).index();
		jQuery(".zt_numlist").hide().eq(n).show();
		jQuery("#tag_li_zt li").removeClass("focus");
		jQuery(this).addClass("focus");
	});
	</script>
	
