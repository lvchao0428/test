<script type="text/javascript">
	function setUser() {
		new Ajax({ url: 'BBS.WebUI.Ajax.UserAjax,BBS.WebUI.ajax.asmx?a=getInfo' + (typeof bid != 'undefined' && bid ? '&bid=' + bid : '') + (typeof (judgeOnline) == 'undefined' ? '' : '&joid=' + judgeOnline) + (typeof (needVc) != 'undefined' && needVc ? "&judgeVc=1" : "") + '&t=' + (new Date().getTime()), method: 'get', onComplete: function (trans) {
			var u; eval('u=' + trans.responseText);
			var oinfo = crt('SPAN', { id: 'uInfo' }, $id('divHRight'));
			var h;
			u.homeUrl = '/u_' + u.id + '/_d.aspx';
			window.hexunUser = u;
			var url = escape(location.href);
			if (u.id) {
				h = '<span class="f39c"><a hr' + 'ef="' + u.homeUrl + '" target="_top" class="f39c">' + u.name + '</a>，您好！</span>'

				h += ' <span class="news"><a href="/message/" target="_blank">您有新纸条（<span  class="fc00" id="aPaperCn">' + u.paper + '</span>）</a></span>';

				h += '  [<a href="http://utility.tool.hexun.com/quit.aspx?gourl=' + url + '" onclick="logout()" target="_top">退出</a>]';

				if (u.police) {
					window.police = u.police;
				}
			}
			else {
				h = '游客您好，请<a target="_top" href="https://reg.hexun.com/login.aspx?gourl=' + url + '">登录</a>或<a target="_top" href="http://reg.hexun.com/Register.aspx?fromhost=HX_BBS">注册</a>';
			}
			oinfo.innerHTML = h;
			if (typeof onGotUser == 'function') {
				onGotUser(u);
			}
		}, showLoading: false
		});
	}
	addLoadEvent(function () { setTimeout(setUser, 20); });
