				_this.showMenu(arrMenu,function(v){_this.showIframeModal('美图秀秀在线编辑',v,function(v){_this.loadBookmark();},800,530,function(v){clossxiuxiu();});});
			}
		}
	};
	
	editor=$('#WYSIWYG_HTML').xheditor({plugins:allPlugin, tools:'Blocktag,Fontface,FontSize,Bold,Italic,Underline,Strikethrough,FontColor,BackColor,|,Align,Outdent,Indent,|,Link,Img,meitu,Emot,',loadCSS:'<style>body{font-size:14px;line-height:1.5;}</style>'});//交互方式1
	editor=$('#WYSIWYG_HTML')[0].xheditor;//交互方式2
}

</script>
