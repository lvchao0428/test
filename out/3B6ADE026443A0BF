function wechat_qrcode1(type) {
if(type && document.getElementById('wechat_float_qrcode1').style.display == 'none') {
return;
}
var qrleft = parseInt(document.getElementById('footer').clientWidth + parseInt(fetchOffset(document.getElementById('footer'))['left']));
document.getElementById('wechat_float_qrcode1').style.display = '';
if(qrleft + document.getElementById('wechat_float_qrcode1').clientWidth > document.documentElement.clientWidth) {
document.getElementById('wechat_float_qrcode1').style.cssFloat = 'right';
document.getElementById('wechat_float_qrcode1').style.left = 'auto';
document.getElementById('wechat_float_qrcode1').style.right = 0;
} else {
document.getElementById('wechat_float_qrcode1').style.cssFloat = 'left';
document.getElementById('wechat_float_qrcode1').style.left = (qrleft) + 'px';
document.getElementById('wechat_float_qrcode1').style.right = 'auto';
}
}
